  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	t.id,
	t.date,
	t.amount,
	defects.name AS def_type,
	n.name AS def_name,
	t.driver,
	t.route_list_id,
	t.doc_type,
	CASE t.source
		WHEN 'Warehouse' THEN 'Склад'
		WHEN 'Driver' THEN 'Водитель'
		WHEN 'Client' THEN 'Клиент'
		WHEN 'Production' THEN 'Производство'
		ELSE t.source
	END AS def_source,
	author.last_name AS author,
	t.cmt AS comment
FROM
	(SELECT
		regrading.id AS id,
		regrading.time_stamp AS date,
		regrading_items.amount AS amount,
		regrading_items.defect_type_id AS def_id,
		regrading_items.nomenclature_new_id AS nom_id,
		regrading_items.source AS source,
		NULL AS driver,
		'Документ пересортицы' AS doc_type,
		NULL AS driver_id,
		NULL AS route_list_id,
		regrading.author_id AS doc_author,
		regrading.comment AS cmt
	FROM
		store_regrading_of_goods_items regrading_items
	LEFT JOIN
		store_regrading_of_goods regrading ON regrading_items.store_regrading_of_goods_id = regrading.id

	UNION ALL

	SELECT
		unload.id AS id,
		unload.time_stamp AS date,
		o.amount AS amount,
		unload_items.defect_type_id AS def_id,
		o.nomenclature_id AS nom_id,
		unload_items.source AS source,
		d.last_name AS driver,
		'Документ разгрузки' AS doc_type,
		d.id AS driver_id,
		rl.id AS route_list_id,
		unload.author_id AS doc_author,
		unload.comment AS cmt
	FROM
		store_car_unload_document_items unload_items
	LEFT JOIN
		store_car_unload_documents unload ON unload_items.car_unload_document_id = unload.id
	LEFT JOIN
		warehouse_movement_operations o ON o.id = unload_items.warehouse_movement_operation_id
	LEFT JOIN
		route_lists rl ON unload.route_list_id = rl.id
	LEFT JOIN
		employees d ON d.id = rl.driver_id
	) AS t
LEFT JOIN
	culling_category defects ON defects.id = t.def_id
LEFT JOIN
	nomenclature n ON n.id = t.nom_id
LEFT JOIN
	employees author ON author.id = t.doc_author
WHERE
		t.date BETWEEN @start_date AND ADDDATE(@end_date, INTERVAL 1 DAY)
	AND
		(t.source = @source OR @source = 'All')
	AND
		(t.driver_id = @driver_id OR @driver_id = 0)
	AND
		t.def_id IS NOT NULL
ORDER BY
	t.date,
	def_source,
	def_type
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="source">
            <Value>=Parameters!source</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
