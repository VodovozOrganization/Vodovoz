  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    nomenclature.name AS nomenclature,
    nomenclature.category,
    measurement_units.digits,
    IFNULL((SELECT 
                    SUM(amount)
                FROM
                    warehouse_movement_operations
                WHERE
                    warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &lt; @startDate),
            0) AS before_added,
    IFNULL((SELECT 
                    SUM(amount)
                FROM
                    warehouse_movement_operations
                WHERE
                    warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &lt; @startDate),
            0) AS before_removed,
    IFNULL((SELECT 
                    SUM(amount)
                FROM
                    warehouse_movement_operations
                WHERE
                    warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS all_added,
    IFNULL((SELECT 
                    SUM(amount)
                FROM
                    warehouse_movement_operations
                WHERE
                    warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS all_removed,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        LEFT JOIN
                    store_movement_document_items ON store_movement_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
                        OR store_movement_document_items.delivery_movement_operation_id = warehouse_movement_operations.id
                        LEFT JOIN
                    store_movement_document ON store_movement_document.id = store_movement_document_items.movement_document_id
                WHERE
                    warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)
                        AND store_movement_document.transportation_wagon_id IS NOT NULL),
            0) AS wagons_added,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        LEFT JOIN
                    store_movement_document_items ON store_movement_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
                        OR store_movement_document_items.delivery_movement_operation_id = warehouse_movement_operations.id
                        LEFT JOIN
                    store_movement_document ON store_movement_document.id = store_movement_document_items.movement_document_id
                WHERE
                    warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)
                        AND store_movement_document.transportation_wagon_id IS NOT NULL),
            0) AS wagons_removed,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        INNER JOIN
                    store_incoming_invoice_items ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
                WHERE
                    warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS warehouse_added,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        INNER JOIN
                    store_writeoff_document_items ON store_writeoff_document_items.writeoff_warehouse_movement_operation_id = warehouse_movement_operations.id
                WHERE
                    warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS warehouse_removed,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        INNER JOIN
                    store_regrading_of_goods_items ON store_regrading_of_goods_items.warehouse_income_operation_id = warehouse_movement_operations.id
                WHERE
                    warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS regrading_added,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        INNER JOIN
                    store_regrading_of_goods_items ON store_regrading_of_goods_items.warehouse_writeoff_operation_id = warehouse_movement_operations.id
                WHERE
                    warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS regrading_removed,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        INNER JOIN
                    store_inventory_item ON store_inventory_item.warehouse_movement_operation_id = warehouse_movement_operations.id
                WHERE
                    warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS inventory_added,
    IFNULL((SELECT 
                    SUM(warehouse_movement_operations.amount)
                FROM
                    warehouse_movement_operations
                        INNER JOIN
                    store_inventory_item ON store_inventory_item.warehouse_movement_operation_id = warehouse_movement_operations.id
                WHERE
                    warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id
                        AND warehouse_movement_operations.nomenclature_id = nomenclature.id
                        AND warehouse_movement_operations.operation_time &gt;= @startDate
                        AND warehouse_movement_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)),
            0) AS inventory_removed
FROM
    (SELECT DISTINCT
        warehouse_movement_operations.nomenclature_id
    FROM
        warehouse_movement_operations
    WHERE
        warehouse_movement_operations.incoming_warehouse_id = @warehouse_id
            OR warehouse_movement_operations.writeoff_warehouse_id = @warehouse_id) moved_nom
        LEFT JOIN
    nomenclature ON nomenclature.id = moved_nom.nomenclature_id
        LEFT JOIN
    measurement_units ON measurement_units.id = nomenclature.unit_id
HAVING  before_added - before_removed &lt;&gt; 0 OR  all_added - all_removed &lt;&gt; 0
ORDER BY nomenclature.category , nomenclature.name</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!startDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!endDate</Value>
          </QueryParameter>
          <QueryParameter Name="warehouse_id">
            <Value>=Parameters!warehouse_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="nomenclature">
          <DataField>nomenclature</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="digits">
          <DataField>digits</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="before_added">
          <DataField>before_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="before_removed">
          <DataField>before_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="all_added">
          <DataField>all_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="all_removed">
          <DataField>all_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="wagons_added">
          <DataField>wagons_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="wagons_removed">
          <DataField>wagons_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="warehouse_added">
          <DataField>warehouse_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="warehouse_removed">
          <DataField>warehouse_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="regrading_added">
          <DataField>regrading_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="regrading_removed">
          <DataField>regrading_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="inventory_added">
          <DataField>inventory_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="inventory_removed">
          <DataField>inventory_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
