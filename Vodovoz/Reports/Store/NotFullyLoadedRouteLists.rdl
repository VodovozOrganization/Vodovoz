          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="count_loaded">
          <DataField>count_loaded</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="count_total">
          <DataField>count_total</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
