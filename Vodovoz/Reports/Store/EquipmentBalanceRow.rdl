  <DataSets>
    <DataSet Name="Nomenclatures">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	q.nomenclature_id,
	q.type, 
	q.nom_name,
    
    CASE q.nom_category
		WHEN 'rent' THEN 'Аренда кулеров'
		WHEN 'water' THEN 'Вода в многооборотной таре'
		WHEN 'deposit' THEN 'Залог'
		WHEN 'spare_parts' THEN 'Запчасти'
		WHEN 'equipment' THEN 'Оборудование'
		WHEN 'additional' THEN 'Товары'
		WHEN 'service' THEN 'Услуга'
		WHEN 'bottle' THEN 'Тара'
		WHEN 'material' THEN 'Сырьё'
		WHEN 'disposableBottleWater' THEN 'Вода в одноразовой таре'
		WHEN 'master' THEN 'Выезд мастера'
		ELSE ''
	END as category_name, 
    
	SUM(q.quantity) AS quantity,    
	GROUP_CONCAT(
		CONCAT_WS(': ',
			w.name,
			CONCAT(FORMAT(q.quantity, 0), ' ', mu.name)
		)
		SEPARATOR '\n'
	) AS place,    
	CASE q.subtype_of_equipment
		WHEN 'forSale' THEN 'Для продажи'
		WHEN 'notForSale' THEN 'Не для продажи'
		ELSE ''
	END AS subtype_of_equipment
FROM
	(
		SELECT 
			TableAmount.nom_id AS nomenclature_id,
            nom.category as nom_category,
            CASE nom.category
				WHEN 'equipment' THEN eqt.name
				WHEN 'additional' THEN nomenclature_groups.name
				ELSE ''
			END AS type,
            nom.manufacturer_id AS manufacturer,
			nom.name AS nom_name,
			TableAmount.wh AS incoming_wh,
			SUM(TableAmount.quantity) AS quantity,
			nom.unit_id AS unit_id,
			nom.subtype_of_equipment 
		FROM
			(
				SELECT
					nomenclature_id AS nom_id,
					writeoff_warehouse_id AS wh,
					-SUM(amount) AS quantity
				FROM warehouse_movement_operations
				WHERE writeoff_warehouse_id IS NOT NULL
				GROUP BY nomenclature_id, wh
				UNION ALL
				SELECT
					nomenclature_id AS nom_id,
					incoming_warehouse_id AS wh,
					SUM(amount) AS quantity
				FROM warehouse_movement_operations
				WHERE incoming_warehouse_id IS NOT NULL
				GROUP BY nomenclature_id, wh
			) AS TableAmount
		LEFT JOIN nomenclature nom ON nom.id = TableAmount.nom_id
        LEFT JOIN nomenclature_groups ON nom.group_id = nomenclature_groups.id
		LEFT JOIN equipment_type eqt ON nom.type_id = eqt.id
		WHERE 
			(nom.category = @category AND nom.category NOT IN ('equipment', 'additional'))
            OR
			(
				(nom.category = @category AND @category = 'equipment' AND '0' NOT IN (@equipments) AND nom.subtype_of_equipment IN (@equipments))
				OR
				(nom.category = @category AND @category = 'additional' AND '0' NOT IN (@additional) AND nom.group_id IN (@additional))
            )
		GROUP BY TableAmount.nom_id, TableAmount.wh 
		HAVING SUM(TableAmount.quantity) &gt; 0
	) AS q
LEFT JOIN measurement_units mu ON mu.id = q.unit_id
LEFT JOIN warehouses w ON w.id = q.incoming_wh
GROUP BY q.nomenclature_id
ORDER BY q.type, q.manufacturer</CommandText>
        <QueryParameters>
          <QueryParameter Name="category">
            <Value>={?category}</Value>
          </QueryParameter>
          <QueryParameter Name="equipments">
            <Value>={?equipments}</Value>
          </QueryParameter>
          <QueryParameter Name="additional">
            <Value>={?additional}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="nomenclature_id">
          <DataField>nomenclature_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
