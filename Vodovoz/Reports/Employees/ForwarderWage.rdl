        </Field>
        <Field Name="money">
          <DataField>money</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="reason">
          <DataField>reason</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ForwarderFines">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
SELECT 
     fines.date,
    money,
    GROUP_CONCAT(nomenclature.name SEPARATOR ', ') AS nom_name,
    route_lists.date AS rl_date,
    fines.fine_reason_string AS reason
FROM
    fines_items
        LEFT JOIN
    fines ON fines.id = fines_items.fine_id
        LEFT JOIN
    fines_nomenclatures ON fines_nomenclatures.fine_id = fines_items.fine_id
        LEFT JOIN
    nomenclature ON nomenclature.id = fines_nomenclatures.nomenclature_id
        LEFT JOIN
    route_lists ON route_lists.id = fines.route_list_id
WHERE
    fines_items.employee_id = @forwarder_id
        AND fines.date &gt;= @start_date
        AND fines.date &lt;= @end_date
GROUP BY fines.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="forwarder_id">
            <Value>={?forwarder_id}</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="money">
          <DataField>money</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="nom_name">
          <DataField>nom_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="rl_date">
          <DataField>rl_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="reason">
          <DataField>reason</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
