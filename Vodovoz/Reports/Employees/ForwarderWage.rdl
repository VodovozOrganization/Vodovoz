  <DataSets>
    <DataSet Name="ForwarderWage">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    route_lists.date AS date,
    route_lists.id AS route_list_number,
    SUM(bottles_movement_operations.returned) AS empty_bottles,
    SUM(bottles_movement_operations.delivered) AS full_bottles,
    SUM(route_list_addresses.forwarder_wage) AS wage,
    COUNT(IF(route_list_addresses.status = 'Overdue'
	OR route_list_addresses.status = 'Canceled', NULL, route_list_addresses.id)) AS addresses_count
FROM
    route_lists
        LEFT JOIN
    route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
        LEFT JOIN
    bottles_movement_operations ON bottles_movement_operations.order_id = route_list_addresses.order_id
WHERE
    route_lists.forwarder_id = @forwarder_id
        AND date &gt;= @start_date
        AND date &lt;= @end_date
        AND (route_lists.status = 'NotDelivered'
        OR route_lists.status = 'MileageCheck'
        OR route_lists.status = 'Closed')
GROUP BY route_lists.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="forwarder_id">
            <Value>=Parameters!forwarder_id</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="route_list_number">
          <DataField>route_list_number</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="empty_bottles">
          <DataField>empty_bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="full_bottles">
          <DataField>full_bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="wage">
          <DataField>wage</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="addresses_count">
          <DataField>addresses_count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
