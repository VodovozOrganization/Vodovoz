    <DataSet Name="Advances">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    wages_movement_operations.operation_time AS date,
    wages_movement_operations.money * (-1) AS advance
FROM
    wages_movement_operations
WHERE
    wages_movement_operations.operation_type = 'GivedAdvance'
        AND wages_movement_operations.employee_id = @forwarder_id
        AND wages_movement_operations.operation_time &gt;= @start_date
        AND wages_movement_operations.operation_time &lt;= DATE_ADD(@end_date, INTERVAL 1 DAY);</CommandText>
        <QueryParameters>
          <QueryParameter Name="forwarder_id">
            <Value>=Parameters!forwarder_id</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="advance">
          <DataField>advance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ForwarderWageBalance">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    SUM(wages_movement_operations.money) AS balance
FROM
    wages_movement_operations
        LEFT JOIN
    employees ON employees.id = wages_movement_operations.employee_id
WHERE
    wages_movement_operations.operation_time &lt;=  @end_date
        AND wages_movement_operations.employee_id = @forwarder_id
GROUP BY wages_movement_operations.employee_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="forwarder_id">
            <Value>=Parameters!forwarder_id</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="showbalance">
            <Value>=Parameters!showbalance</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="balance">
          <DataField>balance</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ForwarderPremiums">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	premiums.date,
    money,
    premiums.premium_reason_string AS reason
FROM
    premium_items
        LEFT JOIN
    premiums ON premiums.id = premium_items.premium_id        
WHERE
    premium_items.employee_id = @forwarder_id
        AND premiums.date &gt;= @start_date
        AND premiums.date &lt;= @end_date
GROUP BY premiums.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="forwarder_id">
            <Value>={?forwarder_id}</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
