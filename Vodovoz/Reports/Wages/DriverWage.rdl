    <DataSet Name="DriverPremiums">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	premiums.date,
    money,
    premiums.premium_reason_string AS reason
FROM
    premium_items
        LEFT JOIN
    premiums ON premiums.id = premium_items.premium_id        
WHERE
    premium_items.employee_id = @driver_id
        AND premiums.date &gt;= @start_date
        AND premiums.date &lt;= @end_date
GROUP BY premiums.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="money">
          <DataField>money</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="reason">
          <DataField>reason</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DriverFines">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
SELECT 
	fines.date,
    money,
    GROUP_CONCAT(nomenclature.name SEPARATOR ', ') AS nom_name,
    route_lists.date AS rl_date,
    fines.fine_reason_string AS reason
FROM
    fines_items
        LEFT JOIN
    fines ON fines.id = fines_items.fine_id
        LEFT JOIN
    fines_nomenclatures ON fines_nomenclatures.fine_id = fines_items.fine_id
        LEFT JOIN
    nomenclature ON nomenclature.id = fines_nomenclatures.nomenclature_id
        LEFT JOIN
    route_lists ON route_lists.id = fines.route_list_id
WHERE
    fines_items.employee_id = @driver_id
        AND fines.date &gt;= @start_date
        AND fines.date &lt;= @end_date
GROUP BY fines.id

</CommandText>
