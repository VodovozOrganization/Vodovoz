    <DataSet Name="Advances">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    wages_movement_operations.operation_time AS date,
    wages_movement_operations.money * (-1) AS advance,
    cash_expense.description AS description
FROM
    wages_movement_operations
INNER JOIN cash_expense ON cash_expense.wages_movement_operations_id = wages_movement_operations.id
