    <DataSet Name="UnclosedAdvances">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	cash_expense.date AS uaDate,
	cash_expense_category.name AS uaName,
	cash_expense.money AS uaCash,
	cash_advance_closing.money AS uaClosedCash,
	IFNULL(cash_expense.money, 0) - IFNULL(cash_advance_closing.money, 0) AS uaCashUnclosed,
	cash_expense.description as uaDescription
FROM
	cash_expense
LEFT JOIN
	cash_expense_category ON cash_expense.cash_expense_category_id = cash_expense_category.id
LEFT JOIN
	cash_advance_closing ON cash_expense.id = cash_advance_closing.expense_id
WHERE
	IFNULL(cash_expense.money, 0) - IFNULL(cash_advance_closing.money, 0) &gt; 0
	AND
	cash_expense.advance_closed = 0
	AND
	cash_expense.employee_id = @driver_id</CommandText>
