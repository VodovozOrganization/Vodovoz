    <DataSet Name="Image">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>/*
SET @id1 = 3; #VodovozLogo
SET @id2 = 4; #VISA
SET @id3 = 5; #MasterCard
SET @id4 = 6; #Пожалуйся директору
SET @id5 = 7; #рекламный баннер
SET @id6 = 8; #телефон
SET @id7 = 9; #VIP
SET @id8 = 10; #Договор оферты
SET @id9 = 13; #Безконтактная доставка
SET @id10 = 14; #Оплата по SMS
SET @id11 = 17; #Терминал
SET @id12 = 18; #Бесплатная доставка продуктов
*/

SELECT

stored_resource.id AS ID1,
stored_resource.binary_file AS IM1 ,

R2.id as ID2 ,
R2.binary_file as IM2 ,

R3.id as ID3 ,
R3.binary_file as IM3 ,

R4.id as ID4 ,
R4.binary_file as IM4 ,

R5.id as ID5 ,
R5.binary_file as IM5 ,

R6.id as ID6 ,
R6.binary_file as IM6 ,

R7.id as ID7 ,
R7.binary_file as IM7 ,

R8.id as ID8 ,
R8.binary_file as IM8 ,

R9.id as ID9 ,
R9.binary_file as IM9 ,

R10.id as ID10 ,
R10.binary_file as IM10,

R11.id as ID11 ,
R11.binary_file as IM11,

R12.id as ID12 ,
R12.binary_file as IM12,

R21.id as ID21 ,
R21.binary_file as IM21,

R22.id as ID22 ,
R22.binary_file as IM22

FROM stored_resource

