          </QueryParameter>
          <QueryParameter Name="id7">
            <Value>9</Value>
          </QueryParameter>
          <QueryParameter Name="id8">
            <Value>10</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="IM1">
          <DataField>IM1</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="IM2">
          <DataField>IM2</DataField>
          <TypeName>System.String</TypeName>
