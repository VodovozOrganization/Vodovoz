    <DataSet Name="deposits">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>30</Timeout>
        <CommandText>SELECT
	IF(nomenclature.official_name is NULL,'Бутыли',nomenclature.official_name) as name,
	order_deposit_items.deposit_sum as deposit,
	SUM(order_deposit_items.count) as count,
	SUM(order_deposit_items.deposit_sum * order_deposit_items.count) as sum,
    measurement_units.name as mesUnit
FROM 
	orders
	LEFT JOIN
	order_deposit_items ON order_deposit_items.order_id = orders.id
	LEFT JOIN
	nomenclature ON order_deposit_items.equip_nomenclature_id = nomenclature.id
	LEFT JOIN measurement_units ON measurement_units.id = nomenclature.unit_id
WHERE
	orders.id = @order_id
	AND order_deposit_items.payment_type = 'ToClient'
GROUP BY name</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="deposit">
          <DataField>deposit</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="sum">
          <DataField>sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="mesUnit">
          <DataField>mesUnit</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
