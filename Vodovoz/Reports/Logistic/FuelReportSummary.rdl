  <DataSets>
    <DataSet Name="FuelOther">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select
	ifnull(fuel_operations.date,fuel_documents.date) AS date,
	route_lists.id AS route_list_id,
	route_lists.date AS route_list_date,
	CONCAT_WS(' ', driver.last_name, driver.name) AS driver_name,
	IFNULL(fuel_documents.fuel_type_id,0) AS fuel_type,
	SUM(fuel_operations.liters_outlayed) AS liters_outlayed,
	SUM(IFNULL(fuel_documents.fuel_coupons, 0)) AS liters_coupons,
	cars.reg_number AS reg_number,
	IFNULL(author.last_name,'') AS author_name,
	IFNULL(last_editor.last_name,'') AS last_editor_name,
	IFNULL(fuel_documents.last_edit_date,'') AS last_edit_date
from
	route_lists
left join
	fuel_operations on fuel_operations.id = route_lists.fuel_outlayed_operation_id
left join
	fuel_documents on fuel_documents.id = route_lists.fuel_gived_document_id
left join
	employees as driver on driver.id = route_lists.driver_id
left join
	employees as author on author.id = fuel_documents.author_id
left join
	employees as last_editor on last_editor.id = fuel_documents.last_editor_id
left join
	cars on cars.id = route_lists.car_id
where
	(cars.type_of_use = 'Other' or cars.type_of_use is null)
	and ifnull(fuel_operations.date,fuel_documents.date) &gt;= @start_date
	and ifnull(fuel_operations.date,fuel_documents.date) &lt; @end_date
	AND (@author = -1 OR author_id = @author)
group by
	route_list_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="author">
            <Value>={?author}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="route_list_id">
          <DataField>route_list_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="route_list_date">
          <DataField>route_list_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="driver_name">
          <DataField>driver_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="fuel_type">
          <DataField>fuel_type</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="liters_outlayed">
          <DataField>liters_outlayed</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="liters_coupons">
          <DataField>liters_coupons</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="reg_number">
          <DataField>reg_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="author_name">
          <DataField>author_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="last_editor_name">
          <DataField>last_editor_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="last_edit_date">
          <DataField>last_edit_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="FuelTruck">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select
