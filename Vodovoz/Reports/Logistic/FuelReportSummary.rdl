    <DataSet Name="FuelSummary">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	IFNULL(fuel_documents.fuel_type_id,0) AS fuel_type,
	SUM(fuel_operations.liters_outlayed) AS liters_used,
	SUM(IFNULL(fuel_documents.fuel_coupons, 0)) AS liters_coupons,
	SUM(CASE
		WHEN fuel_documents.fuel_type_id = 3 THEN fuel_documents.fuel_coupons
	END) AS coupons_diesel,
	SUM(CASE
		WHEN fuel_documents.fuel_type_id &lt;&gt; 3 THEN fuel_documents.fuel_coupons
	END) AS coupons_benzene
FROM
	fuel_operations
LEFT JOIN
	fuel_documents ON fuel_documents.fuel_operation_id = fuel_operations.id
WHERE
	fuel_operations.date &gt;= @start_date
	AND fuel_operations.date &lt;= @end_date
	AND (@author = -1 OR author_id = @author)</CommandText>
