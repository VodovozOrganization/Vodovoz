  <DataSets>
    <DataSet Name="RouteLists">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	route_lists.id AS routeListId,
	route_lists.date AS routeListDate,
	route_lists.driver_id,
	CONCAT_WS(' ',
		employees.last_name,
		IF(employees.name = '',
			NULL,
			employees.name),
		IF(employees.patronymic = '',
			NULL,
			employees.patronymic)) AS driver_name,
	IFNULL(
		(SELECT
			SUM(total_cash) -  IFNULL(money,0) - ABS(SUM(deposits_collected)) - ABS(SUM(equipment_deposits_collected)) 
		FROM
			route_list_addresses
		WHERE
			route_list_addresses.status = 'Completed'
			AND route_list_addresses.route_list_id = routeListId), 0) AS nal,
	IFNULL(
		(SELECT 
			count(*)*2
		FROM
			route_list_addresses
		LEFT JOIN
			route_lists ON route_list_addresses.route_list_id = route_lists.id
		LEFT JOIN
			cars ON cars.id = route_lists.car_id
		WHERE
            (is_company_havings = 0 OR (is_company_havings = 1 AND (cars.type_of_use &lt;&gt; 'Truck' OR cars.type_of_use is null)))
            AND route_list_addresses.status = 'Completed'
			AND route_list_addresses.route_list_id = routeListId), 0)  AS paytelephone,
	(select
		sum(order_items.count)
	from
		route_list_addresses
	left join
		orders on orders.id = route_list_addresses.order_id
	left join
		order_items on order_items.order_id = orders.id
	left join
		nomenclature on nomenclature.id = order_items.nomenclature_id
	where
		nomenclature.category = 'water'
        and route_list_addresses.route_list_id = routeListId
        ) as bottles,
	sum(ifnull(orders.returned_tare,0)) as planned_returned_bottles,
    (
		SELECT 
			ROUND(IFNULL(SUM(warehouse_movement_operations.amount),0))   
		FROM 
			warehouse_movement_operations
			LEFT JOIN store_car_unload_document_items ON warehouse_movement_operations.id = store_car_unload_document_items.warehouse_movement_operation_id
			LEFT JOIN store_car_unload_documents ON store_car_unload_documents.id = store_car_unload_document_items.car_unload_document_id
            LEFT JOIN nomenclature ON nomenclature.id = warehouse_movement_operations.nomenclature_id
		WHERE
				nomenclature.category = 'bottle'
            AND	store_car_unload_documents.route_list_id = routeListId
	) as returned_bottles,
	closing_comment
FROM
	route_lists
LEFT JOIN
	employees ON employees.id = route_lists.driver_id
LEFT JOIN
	route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
LEFT JOIN
	orders ON route_list_addresses.order_id = orders.id
LEFT JOIN
	cash_income ON cash_income.route_list_id = route_list_addresses.route_list_id
WHERE
	route_lists.status = 'OnClosing'
	AND ((route_lists.date &lt; CURDATE()
	AND @todayRloff = 1)
	OR (@todayRloff = 0))
GROUP BY routeListId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@todayRloff">
            <Value>={?todayRloff}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
