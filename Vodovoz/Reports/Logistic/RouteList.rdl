    <DataSet Name="Orders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    route_list_addresses.route_list_id AS RouteListId, orders.id AS OrderId,
    CONCAT_WS('\n', orders.id, orders.daily_number_1c) AS order_and_daily_nr,
    orders.daily_number_1c, orders.has_comment_for_driver, route_list_addresses.need_to_reload AS Reloaded,
    (SELECT SUM(order_items.count * nomenclature.weight) FROM order_items
                                                                  LEFT JOIN nomenclature ON nomenclature.id=order_items.nomenclature_id
     WHERE order_items.order_id = orders.id) as weight,
    CONCAT(HOUR(delivery_schedule.from_time),'-', HOUR(delivery_schedule.to_time)) AS DeliveryTime,
    CONCAT(DATE_FORMAT(route_list_addresses.plan_time_start, '%H:%i'),'-', DATE_FORMAT(route_list_addresses.plan_time_end, '%H:%i')) AS PlanTime,
    IFNULL(orders.bottles_return, 0) + IFNULL(orders.bottles_by_stock_count, 0) AS empty_btls_from_client,
    IF(orders.payment_type in ('cash', 'BeveragesWorld'), orders.sum_to_receive, 0) AS Total,
    IF(orders.payment_type = 'Terminal',
        (
            SELECT SUM(IFNULL(actual_count, count) * price - discount_money)
            FROM order_items
            WHERE order_id = orders.id), 
        0) as terminals_sum,
    CONCAT_WS(' ', IFNULL(delivery_points.compiled_address_short, delivery_points.compiled_address),
          IF(route_list_addresses.status = 'Transfered',CONCAT_WS('','(Перенесен в МЛ №',TransferedAddresses.route_list_id,')'),'')) AS Address,
    counterparty.need_new_bottles AS NeedNewBottles,
    CONCAT_WS(
            ' ',
            GROUP_CONCAT(
                    DISTINCT
                    CONCAT(
                            ' ',
                            eq_qry.name,
                            IFNULL(
                                    CONCAT(': ', eq_qry.cnt),
                                    ''
                                )
                        )
                ),
            IF(
                        (orders.to_client_text IS NOT NULL) AND (eq_qry.name IS NOT NULL),
                        "; ",
                        ""
                ),
            orders.to_client_text
        ) AS ToClient,
    TRIM("\n" FROM
         CONCAT_WS(
                 ' ',
                 GROUP_CONCAT(DISTINCT
                              CONCAT(
                                      ' ',
                                      pickup_eq_qry.name,
                                      ': ',
                                      pickup_eq_qry.cnt
                                  )
                     ),
                 orders.from_client_text,
                 "\n",
                 IF(
                         (EXISTS(
                                  SELECT oi.id
                                  FROM order_items oi LEFT JOIN nomenclature n
                                                                ON (oi.nomenclature_id=n.id AND n.category IN ('water', 'equipment', 'spare_parts', 'additional', 'bottle', 'material'))
                                  WHERE oi.order_id=orders.id
                              ) OR
                          EXISTS(
                                  SELECT id
                                  FROM order_equipment
                                  WHERE direction = 'Deliver' AND order_equipment.order_id=orders.id
                              )),
                         "",
                         CONCAT("Тара: ", orders.bottles_return)
                     )
             ))AS FromClient,
#IF(deposit_qry.deposit IS NULL, 0, deposit_qry.deposit) AS BottlesDeposit,
#База в запросе выше на проде пустая, залоги гребутся из той, что в запросе ниже.
    IF(route_list_addresses.deposits_collected IS NULL, 0, route_list_addresses.deposits_collected) AS BottlesDeposit,
    route_list_addresses.status AS Status,
    orders.payment_type AS payment_type,
    CASE
        WHEN orders.payment_type IN ('cashless', 'barter', 'ContractDoc') THEN 'б/н'
        WHEN orders.contactless_delivery THEN 'Без
подписи'
        WHEN orders.payment_type = 'Terminal' THEN 'Терминал'
        ELSE ''
        END as client_signature_autofill,
        IF(delivery_points.lunch_time_from &lt;&gt; delivery_points.lunch_time_to , concat(DATE_FORMAT(delivery_points.lunch_time_from, "%H:%i"), "- ", DATE_FORMAT(delivery_points.lunch_time_to, "%H:%i")), "") AS lunch_time,
        IF(delivery_points.is_before_interval_delivery, 'Да', 'Нет') AS is_before_interval_delivery

<!--sql_select-->
FROM route_list_addresses
         LEFT JOIN orders ON orders.id = route_list_addresses.order_id
         LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
         LEFT JOIN delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
         LEFT JOIN counterparty ON counterparty.id = orders.client_id
         LEFT JOIN
     (SELECT
          route_list_addresses.id,
          route_list_addresses.route_list_id
      FROM route_list_addresses) as TransferedAddresses
     ON TransferedAddresses.id = route_list_addresses.transfered_to_id

/*LEFT JOIN ( 
SELECT order_id, (count * deposit_sum * IF(payment_type = 'ToClient', -1, 1)) AS deposit 
FROM order_deposit_items 
WHERE order_deposit_items.deposit_type = 'Bottles'
) AS deposit_qry ON deposit_qry.order_id = orders.id*/

         LEFT JOIN (
#SELECT order_equipment.order_id as order_id, nomenclature.id AS nomenclature_id, nomenclature.name as name, COUNT(nomenclature.id) AS cnt FROM order_equipment 
#LEFT JOIN equipment ON order_equipment.equipment_id = equipment.id
#LEFT JOIN nomenclature ON nomenclature.id = equipment.nomenclature_id 
#WHERE order_equipment.direction = 'Deliver'
#GROUP BY order_id, nomenclature_id
#Для SHORT версии.
    SELECT
        T.order_id AS order_id,
        nomenclature_id AS nomenclature_id,
        T.name AS name,
        IF(nomenclature_id = -1, NULL, SUM(T.cnt)) AS cnt
    FROM(
            SELECT
                order_items.order_id AS order_id,
                IF(
                        nomenclature.online_store_id IS NOT NULL,
                        -1, nomenclature_id) AS nomenclature_id,
                IF(
                        nomenclature.online_store_id IS NOT NULL,
                        CONCAT('Заказ ИМ ', o.e_shop_order), IFNULL(nomenclature.short_name, nomenclature.name)) AS name,
                IF(
                        nomenclature.online_store_id IS NOT NULL,
                        1, cast(SUM(order_items.count) as DECIMAL)) AS cnt
            FROM order_items
                     LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
                     LEFT JOIN orders o ON o.id = order_items.order_id
                     JOIN route_list_addresses ON route_list_addresses.order_id = order_items.order_id
            WHERE nomenclature.category IN ('equipment', 'additional', 'spare_parts') AND nomenclature.hide = 0
              AND route_list_addresses.route_list_id = @route_list_id
            GROUP BY order_id, nomenclature_id

            UNION ALL

            SELECT
                order_equipment.order_id AS order_id,
                nomenclature.id AS nomenclature_id,
                CONCAT(
                        IFNULL(nomenclature.short_name, nomenclature.name),
                        ' ',
                        CASE
                            WHEN order_equipment.own_type = 'Rent' THEN '(Аренда)'
                            WHEN order_equipment.own_type = 'Client' THEN '(Клиент)'
                            WHEN order_equipment.own_type = 'Duty' THEN '(Дежурный)'
                            ELSE order_equipment.own_type
                            END
                    ) AS name,
                SUM(order_equipment.count) AS cnt
            FROM order_equipment
                     LEFT JOIN nomenclature ON nomenclature.id = order_equipment.nomenclature_id
                     JOIN route_list_addresses ON route_list_addresses.order_id = order_equipment.order_id
            WHERE order_equipment.direction = 'Deliver' AND nomenclature.hide = 0
              AND route_list_addresses.route_list_id = @route_list_id
            GROUP BY order_id, nomenclature_id
        ) AS T
    GROUP BY order_id, nomenclature_id, name
) AS eq_qry ON eq_qry.order_id = orders.id

LEFT JOIN (
	SELECT
		order_equipment.order_id as order_id, 
		nomenclature.id AS nomenclature_id,
		CONCAT(
			IFNULL(nomenclature.short_name, nomenclature.name), 
			case
				when order_equipment.own_type = 'Rent' then '(Аренда)'
				when order_equipment.own_type = 'Client' then '(Клиент)'
				when order_equipment.own_type = 'Duty' then '(Дежурный)'
				else ''
			end
		) as name, 
		SUM(order_equipment.count) AS cnt 
	FROM order_equipment 
		LEFT JOIN nomenclature ON nomenclature.id = order_equipment.nomenclature_id
	WHERE order_equipment.direction = 'PickUp' AND nomenclature.hide = 0
	GROUP BY order_id, nomenclature_id
) AS pickup_eq_qry ON pickup_eq_qry.order_id = orders.id


LEFT JOIN (
SELECT
order_items.order_id AS order_id
<!--sql_select_subquery-->
FROM route_list_addresses
LEFT JOIN order_items ON route_list_addresses.order_id = order_items.order_id
LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
LEFT JOIN nomenclature_route_column ON nomenclature_route_column.id = nomenclature.route_column_id
WHERE route_list_addresses.route_list_id = @route_list_id AND nomenclature.route_column_id IS NOT NULL
GROUP BY order_id) AS wt_qry ON wt_qry.order_id = orders.id

WHERE route_list_addresses.route_list_id = @route_list_id
GROUP BY orders.id
ORDER BY order_in_route
;</CommandText>
