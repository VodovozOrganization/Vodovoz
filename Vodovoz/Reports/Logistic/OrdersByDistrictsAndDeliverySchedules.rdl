  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	IFNULL(d_name, '{район не размечен}') AS d_name,
	GROUP_CONCAT(
		IF(
			delivery_schedule = 'before_12',
			CONCAT(o_cnt, ' зак./', btls_cnt, ' бут.'),
			NULL
		)
	) AS before_12,
	SUM(
		IF(
			delivery_schedule = 'before_12',
			o_cnt,
			NULL
		)
	) AS orders_before_12,
	SUM(
		IF(
			delivery_schedule = 'before_12',
			btls_cnt,
			NULL
		)
	) AS bottles_before_12,
	GROUP_CONCAT(
		IF(
			delivery_schedule = 'before_15',
			CONCAT(o_cnt, ' зак./', btls_cnt, ' бут.'),
			NULL
		)
	) AS before_15,
	SUM(
		IF(
			delivery_schedule = 'before_15',
			o_cnt,
			NULL
		)
	) AS orders_before_15,
	SUM(
		IF(
			delivery_schedule = 'before_15',
			btls_cnt,
			NULL
		)
	) AS bottles_before_15,
	GROUP_CONCAT(
		IF(
			delivery_schedule = 'before_18',
			CONCAT(o_cnt, ' зак./', btls_cnt, ' бут.'),
			NULL
		)
	) AS before_18,
	SUM(
		IF(
			delivery_schedule = 'before_18',
			o_cnt,
			NULL
		)
	) AS orders_before_18,
	SUM(
		IF(
			delivery_schedule = 'before_18',
			btls_cnt,
			NULL
		)
	) AS bottles_before_18,
	GROUP_CONCAT(
		IF(
			delivery_schedule = 'after_18',
			CONCAT(o_cnt, ' зак./', btls_cnt, ' бут.'),
			NULL
		)
	) AS after_18,
	SUM(
		IF(
			delivery_schedule = 'after_18',
			o_cnt,
			NULL
		)
	) AS orders_after_18,
	SUM(
		IF(
			delivery_schedule = 'after_18',
			btls_cnt,
			NULL
		)
	) AS bottles_after_18,
	GROUP_CONCAT(
		IF(
			delivery_schedule = 'other',
			CONCAT(o_cnt, ' зак./', btls_cnt, ' бут.'),
			NULL
		)
	) AS other,
	SUM(
		IF(
			delivery_schedule = 'other', o_cnt, NULL)
	) AS orders_other,
	SUM(
		IF(
			delivery_schedule = 'other', btls_cnt, NULL)
	) AS bottles_other,
	GROUP_CONCAT(
		IF(
			delivery_schedule = 'self_delivery',
			CONCAT(o_cnt, ' зак./', btls_cnt, ' бут.'),
			NULL
		)
	) AS self_delivery,
	SUM(
		IF(
			delivery_schedule = 'self_delivery',
			o_cnt,
			NULL
		)
	) AS orders_self_delivery,
	SUM(
		IF(
			delivery_schedule = 'self_delivery',
			btls_cnt,
			NULL
		)
	) AS bottles_self_delivery,
	CONCAT(
		CAST(
			SUM(
				IF(
					delivery_schedule != 'self_delivery',
					o_cnt,
					0
				)
			) AS CHAR
		),
		' зак./',
		CAST(
			SUM(
				IF(
					delivery_schedule != 'self_delivery',
					btls_cnt,
					0
				)
			) AS CHAR
		),
		' бут.'
	) AS total_for_district
FROM(
	SELECT
		COUNT(o.id) AS o_cnt,
		SUM(i.count) AS btls_cnt,
		distr.name AS d_name,
		CASE
			WHEN o.self_delivery THEN 'self_delivery'
			WHEN HOUR(dsch.from_time) * 60 &gt;= 0 AND HOUR(dsch.to_time) * 60 &lt;= 720 THEN 'before_12'
			WHEN HOUR(dsch.from_time) * 60 &gt;= 0 AND HOUR(dsch.to_time) * 60 &gt; 720 AND HOUR(dsch.to_time) * 60 &lt;= 900 THEN 'before_15'
			WHEN HOUR(dsch.from_time) * 60 &gt;= 0 AND HOUR(dsch.to_time) * 60 &gt; 900 AND HOUR(dsch.to_time) * 60 &lt;= 1080 THEN 'before_18'
			WHEN HOUR(dsch.from_time) * 60 &gt;= 1080 AND HOUR(dsch.to_time) * 60 &lt;= 1439 THEN 'after_18'
			WHEN HOUR(dsch.from_time) * 60 &lt; 1080 AND HOUR(dsch.to_time) * 60 &lt;= 1439 THEN 'other'
			ELSE 'unknown'
		END AS delivery_schedule
	FROM
		orders o
	LEFT JOIN
		order_items i ON o.id = i.order_id
	LEFT JOIN
		delivery_points dp ON dp.id = o.delivery_point_id AND dp.latitude IS NOT NULL AND dp.longitude IS NOT NULL
	LEFT JOIN
		nomenclature n ON i.nomenclature_id = n.id
	LEFT JOIN
		logistics_area distr ON ST_CONTAINS(distr.logistics_district, POINT(dp.latitude, dp.longitude))
	LEFT JOIN
		delivery_schedule dsch ON o.delivery_schedule_id = dsch.id
	WHERE
		o.delivery_date = @date
	AND
		n.category = 'water' AND n.tare_volume = 'Vol19L'
	GROUP BY distr.id, delivery_schedule
) AS int_tbl
GROUP BY d_name
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>={?date}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
