  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT		
	distr.district_name AS d_name,		
	NULLIF(COUNT(DISTINCT IF(HOUR(dsch.to_time) &lt; 13,  o.id, NULL)), 0) AS orders_before_13,
	NULLIF(COUNT(DISTINCT IF(HOUR(dsch.to_time) &gt;= 13 AND HOUR(dsch.to_time) &lt;= 18, o.id, NULL)),0) AS orders_between_13_18,
	NULLIF(COUNT(DISTINCT IF(HOUR(dsch.to_time) &gt; 18, o.id, NULL)),0) AS orders_after_18,
	NULLIF(SUM(IF(HOUR(dsch.to_time) &lt; 13, IF(n.category = 'water' AND n.tare_volume = 'Vol19L', i.count, 0), 0)),0) AS bottles_before_13,
	NULLIF(SUM(IF(HOUR(dsch.to_time) &gt;= 13 AND HOUR(dsch.to_time) &lt;= 18, IF(n.category = 'water' AND n.tare_volume = 'Vol19L', i.count, 0), 0)),0) AS bottles_between_13_18,
	NULLIF(SUM(IF(HOUR(dsch.to_time) &gt; 18,IF(n.category = 'water' AND n.tare_volume = 'Vol19L', i.count, 0), 0)),0) AS bottles_after_18,
	COUNT(DISTINCT o.id) AS total_orders_count,
	SUM(IF(n.category = 'water' AND n.tare_volume = 'Vol19L', i.count, NULL)) AS total_bottles_count,
	HOUR(dsch.to_time) AS delivery_schedule
FROM
	orders o
LEFT JOIN
	order_items i ON o.id = i.order_id
LEFT JOIN
	delivery_points dp ON dp.id = o.delivery_point_id AND dp.latitude IS NOT NULL AND dp.longitude IS NOT NULL
LEFT JOIN
	nomenclature n ON i.nomenclature_id = n.id
LEFT JOIN
	schedule_restricted_districts distr ON ST_CONTAINS(distr.district_border, POINT(dp.latitude, dp.longitude))
LEFT JOIN
	delivery_schedule dsch ON o.delivery_schedule_id = dsch.id
WHERE
	o.order_status NOT IN ('Canceled','NotDelivered','DeliveryCanceled') 
	AND EXISTS (SELECT route_list_addresses.id FROM route_list_addresses WHERE route_list_addresses.order_id = o.id)
	AND o.delivery_date = @date
GROUP BY distr.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>={?date}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="d_name">
          <DataField>d_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="orders_before_13">
          <DataField>orders_before_13</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
