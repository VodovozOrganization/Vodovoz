        </Field>
        <Field Name="not_delivered">
          <DataField>not_delivered</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="wages">
          <DataField>wages</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="water6_amount">
          <DataField>water6_amount</DataField>
