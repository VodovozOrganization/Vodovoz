  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	CASE cars.type_of_use
		WHEN 'Largus' THEN '(25адр * 30руб) + (75бут * 10руб) + (70бут * 5руб) = 1850\n1850руб * 22 дн. = 40700 руб в месяц'
		WHEN 'GAZelle' THEN '(15адр * 30руб) + (100бут * 10руб) + (90бут * 5руб) = 1900\n1900руб*22 дн. = 41800руб в месяц'
		ELSE '(20адр * 50руб) + (75бут * 20руб) + (70бут * 5руб) = 2850\n2850руб*22 дн. = 62700 руб в месяц'
	END AS text,
	CASE cars.type_of_use
		WHEN 'Largus' THEN 'Ларгусы'
		WHEN 'GAZelle' THEN 'ГАЗели'
		ELSE 'Раскат'
	END AS type_of_use,
	cars.is_raskat,
	route_lists.id,
	route_lists.driver_id,
	CASE route_lists.status
		WHEN 'New' THEN 'Новый'
		WHEN 'InLoading' THEN 'На погрузке'
		WHEN 'EnRoute' THEN 'В пути'
		WHEN 'OnClosing' THEN 'Сдаётся'
		WHEN 'NotDelivered' THEN 'Не сдан'
		WHEN 'MileageCheck' THEN 'Проверка километража'
		WHEN 'Closed' THEN 'Закрыт'
	END AS status,
	reg_number,
	CONCAT_WS(' ',
			employees.last_name,
			IF(employees.name = '',
				NULL,
				employees.name),
			IF(employees.patronymic = '',
				NULL,
				employees.patronymic)) AS driver_name,
	(SELECT 
			GROUP_CONCAT(logistics_area.name SEPARATOR ', ')
		FROM
			driver_district_priorities
				LEFT JOIN
			logistics_area ON driver_district_priorities.district_id = logistics_area.id
		WHERE
			employees.id = driver_district_priorities.driver_id) AS driver_districts,
	GROUP_CONCAT(DISTINCT ' ',
		(SELECT 
				logistics_area.name
			FROM
				orders
					LEFT JOIN
				delivery_points ON orders.delivery_point_id = delivery_points.id
					LEFT JOIN
				logistics_area ON ST_WITHIN(GEOMFROMTEXT(CONCAT('POINT(', latitude, ' ', longitude, ')')),
						logistics_district)
			WHERE
				orders.id = route_list_addresses.order_id)) AS street_district,
	COUNT(route_list_addresses.id) AS address_count,
	(SELECT 
			COUNT(route_list_addresses.id)
		FROM
			route_list_addresses
		WHERE
			route_list_addresses.route_list_id = route_lists.id
				AND (route_list_addresses.status = 'Canceled'
				OR route_list_addresses.status = 'Transfered')) AS not_delivered,
	IFNULL(SUM((SELECT 
					SUM(order_items.count)
				FROM
					order_items
						LEFT JOIN
					nomenclature ON nomenclature.id = order_items.nomenclature_id
				WHERE
					order_items.order_id = route_list_addresses.order_id
						AND nomenclature.category = 'water')),
			0) AS water19_amount,
	IFNULL(SUM((SELECT 
					SUM(order_items.count)
				FROM
					order_items
						LEFT JOIN
					nomenclature ON nomenclature.id = order_items.nomenclature_id
				WHERE
					order_items.order_id = route_list_addresses.order_id
						AND nomenclature.category = 'disposableBottleWater'
						AND nomenclature.weight BETWEEN 5 AND 7
						)
					),
			0) AS water6_amount,
	SUM(IF((SELECT 
				SUM(order_items.count)
			FROM
				order_items
					LEFT JOIN
				nomenclature ON nomenclature.id = order_items.nomenclature_id
			WHERE
				order_items.order_id = route_list_addresses.order_id
					AND nomenclature.category = 'water') IS NULL,
		2,
		0)) AS water19_absent,
	IFNULL((SELECT 
					wages_movement_operations.money
				FROM
					wages_movement_operations
				WHERE
					route_lists.driver_wages_movement_operations_id = wages_movement_operations.id
						AND (route_lists.status = 'OnClosing'
						OR route_lists.status = 'MileageCheck'
						OR route_lists.status = 'Closed')),
			0) AS wages
FROM
	route_lists
		LEFT JOIN
	employees ON employees.id = route_lists.driver_id
		LEFT JOIN
	route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
		LEFT JOIN
	orders ON route_list_addresses.order_id = orders.id
		LEFT JOIN
	delivery_points ON orders.delivery_point_id = delivery_points.id
		LEFT JOIN
	logistics_area ON ST_WITHIN(GEOMFROMTEXT(CONCAT('POINT(', latitude, ' ', longitude, ')')),
			logistics_district)
		LEFT JOIN
	cars ON route_lists.car_id = cars.id
WHERE
	(route_lists.date BETWEEN @start_date AND @end_date)
		AND ((cars.is_company_havings = 1 AND (cars.type_of_use = 'Largus' OR cars.type_of_use = 'GAZelle')) OR cars.is_raskat = 1)
GROUP BY route_lists.id
ORDER BY driver_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
