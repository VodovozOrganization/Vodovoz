          <DataField>Total</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="EquipmentDeposit">
          <DataField>EquipmentDeposit</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <!--fields-->
      </Fields>
    </DataSet>
    <DataSet Name="TotalInfo">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    SUM(route_list_addresses.driver_wage + route_list_addresses.driver_wage_surcharge) AS driver_wage,
    SUM(route_list_addresses.driver_wage_surcharge) AS driver_wage_surcharge,
    SUM(route_list_addresses.forwarder_wage + route_list_addresses.forwarder_wage_surcharge) AS forwarder_wage,
    SUM(bottles_movement_operations.delivered) AS full_bottles,
    SUM(bottles_movement_operations.returned) AS empty_bottles,
    (IFNULL((SELECT 
            SUM(money)
        FROM
            cash_income
        WHERE
            cash_income.route_list_id = route_list_addresses.route_list_id GROUP BY cash_income.route_list_id), 0) - IFNULL((SELECT 
            SUM(money)
        FROM
            cash_expense
        WHERE
            cash_expense.route_list_id = route_list_addresses.route_list_id GROUP BY cash_expense.route_list_id), 0)) AS cash
FROM
    route_list_addresses
        LEFT JOIN
    orders ON orders.id = route_list_addresses.order_id
        LEFT JOIN
    bottles_movement_operations ON orders.bottles_movement_operation_id = bottles_movement_operations.id
WHERE
    route_list_addresses.route_list_id = @route_list_id
        AND route_list_addresses.status &lt;&gt; 'Transfered';
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@route_list_id">
            <Value>={?RouteListId}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="driver_wage">
          <DataField>driver_wage</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="driver_wage_surcharge">
          <DataField>driver_wage_surcharge</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="forwarder_wage">
          <DataField>forwarder_wage</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="cash">
          <DataField>cash</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="full_bottles">
          <DataField>full_bottles</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="empty_bottles">
          <DataField>empty_bottles</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
