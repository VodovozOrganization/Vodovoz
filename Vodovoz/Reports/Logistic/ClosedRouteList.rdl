    <DataSet Name="Orders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
SELECT 
route_list_addresses.route_list_id AS RouteListId, orders.id AS OrderId, orders.daily_number_1c,
CONCAT(HOUR(delivery_schedule.from_time),'-', HOUR(delivery_schedule.to_time)) AS DeliveryTime,
#SHORT В сокращенной версии без комментариев.
#orders.comment AS Comment, 
NULL AS Comment,
route_list_addresses.status AS Status,
 #orders.bottles_return AS EmptyBottles,
 route_list_addresses.bottles_returned AS EmptyBottles,
 route_list_addresses.total_cash AS Total,
CONCAT_WS(' ', IFNULL(delivery_points.compiled_address_short, delivery_points.compiled_address), 
	IF(route_list_addresses.status = 'Transfered',CONCAT_WS('','(Перенесен в МЛ №',TransferedAddresses.route_list_id,')'),'')) AS Address,
CONCAT_WS(' ', GROUP_CONCAT(CONCAT(' ', eq_qry.name, ' - ', eq_qry.cnt, 'шт')), IF((orders.to_client_text IS NOT NULL) AND (eq_qry.name IS NOT NULL), "; ", ""), orders.to_client_text) AS ToClient,
CONCAT_WS(' ', GROUP_CONCAT(CONCAT(' ', pickup_eq_qry.name, ' - ', pickup_eq_qry.cnt, 'шт')), orders.from_client_text) AS FromClient,
#IF(deposit_qry.deposit IS NULL, 0, deposit_qry.deposit) AS BottlesDeposit
#База в запросе выше на проде пустая, залоги гребутся из той, что в запросе ниже.
IF(route_list_addresses.deposits_collected IS NULL, 0, route_list_addresses.deposits_collected) AS BottlesDeposit,
IF(route_list_addresses.equipment_deposits_collected IS NULL, 0, route_list_addresses.equipment_deposits_collected) AS EquipmentDeposit
<!--sql_select-->
FROM route_list_addresses
LEFT JOIN orders ON orders.id = route_list_addresses.order_id
LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
LEFT JOIN delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
LEFT JOIN 
	(SELECT
    route_list_addresses.id,
    route_list_addresses.route_list_id    
    FROM route_list_addresses) as TransferedAddresses
ON TransferedAddresses.id = route_list_addresses.transfered_to_id

LEFT JOIN ( 
SELECT order_id, (count * deposit_sum * IF(payment_type = 'ToClient', -1, 1)) AS deposit 
FROM order_deposit_items 
WHERE order_deposit_items.deposit_type = 'Bottles'
) AS deposit_qry ON deposit_qry.order_id = orders.id

LEFT JOIN (
#SELECT order_equipment.order_id as order_id, nomenclature.id AS nomenclature_id, nomenclature.name as name, COUNT(nomenclature.id) AS cnt FROM order_equipment 
#LEFT JOIN equipment ON order_equipment.equipment_id = equipment.id
#LEFT JOIN nomenclature ON nomenclature.id = equipment.nomenclature_id 
#WHERE order_equipment.direction = 'Deliver'
#GROUP BY order_id, nomenclature_id
#Для SHORT версии.
SELECT order_items.order_id as order_id, nomenclature.id AS nomenclature_id,
IFNULL(nomenclature.short_name, nomenclature.name) as name, SUM(order_items.actual_count) AS cnt FROM order_items 
LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id 
WHERE nomenclature.category = 'equipment' OR nomenclature.category = 'additional' AND nomenclature.hide = 0
GROUP BY order_id, nomenclature_id
) AS eq_qry ON eq_qry.order_id = orders.id

LEFT JOIN (
SELECT order_equipment.order_id as order_id, nomenclature.id AS nomenclature_id,
IFNULL(nomenclature.short_name, nomenclature.name) as name, COUNT(nomenclature.id) AS cnt FROM order_equipment 
LEFT JOIN equipment ON order_equipment.equipment_id = equipment.id
LEFT JOIN nomenclature ON nomenclature.id = equipment.nomenclature_id 
WHERE order_equipment.direction = 'PickUp' AND nomenclature.hide = 0
GROUP BY order_id, nomenclature_id) AS pickup_eq_qry ON pickup_eq_qry.order_id = orders.id


LEFT JOIN (
SELECT
order_items.order_id AS order_id
<!--sql_select_subquery-->
FROM order_items
LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
LEFT JOIN nomenclature_route_column ON nomenclature_route_column.id = nomenclature.route_column_id
WHERE nomenclature.route_column_id IS NOT NULL
GROUP BY order_id) AS wt_qry ON wt_qry.order_id = orders.id

WHERE route_list_addresses.route_list_id = @route_list_id
GROUP BY orders.id
ORDER BY route_list_addresses.order_in_route;
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@route_list_id">
            <Value>={?RouteListId}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="DeliveryTime">
          <DataField>DeliveryTime</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="Comment">
          <DataField>Comment</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="Address">
          <DataField>Address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="ToClient">
          <DataField>ToClient</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="daily_number_1c">
          <DataField>daily_number_1c</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="FromClient">
          <DataField>FromClient</DataField>
