  <DataSets>
    <DataSet Name="Sales">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    orders.create_date,
    counterparty.name,
    SUM(order_items.price * IFNULL(order_items.actual_count, order_items.count)) AS SumWithoutDiscount,
    order_items.discount,
    order_items.discount_money,
    discount_reasons.name AS discount_name,
    nomenclature.name AS nomenclature_name,
    IFNULL(order_items.actual_count, order_items.count) AS count,
    orders.comment,
    GET_PERSON_NAME_WITH_INITIALS(author_employee.last_name, author_employee.name, author_employee.patronymic) AS author,
    GET_PERSON_NAME_WITH_INITIALS(accepted_employee.last_name, accepted_employee.name, accepted_employee.patronymic) AS accepted
FROM
    order_items
    JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
    JOIN discount_reasons ON discount_reasons.id = order_items.discount_reason_id
    JOIN orders ON orders.id = order_items.order_id
    JOIN counterparty ON counterparty.id = orders.client_id
    LEFT JOIN employees AS author_employee ON author_employee.id = orders.author_employee_id
    LEFT JOIN employees AS accepted_employee ON accepted_employee.id = orders.accepted_order_employee
WHERE
    (@StartDate = '' OR 
        (
            (@DateType = 'CreationDate' AND orders.create_date &gt;= @StartDate) 
            OR 
            (@DateType = 'DeliveryDate' AND orders.delivery_date &gt;= @StartDate)
        )
    )
    AND
    (@EndDate = '' OR 
        (
            (@DateType = 'CreationDate' AND orders.create_date &lt;= @EndDate) 
            OR 
            (@DateType = 'DeliveryDate' AND orders.delivery_date &lt;= @EndDate)
        )
    )
	AND ( 
		((order_items.discount_reason_id IN (@discountreason_include) AND 0 IN (@discountreason_exclude) )
		OR
		(order_items.discount_reason_id NOT IN (@discountreason_exclude) AND 0 IN (@discountreason_include)))
		OR (0 IN (@discountreason_include) AND 0 IN (@discountreason_exclude))
	)
GROUP BY
    order_items.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="StartDate">
            <Value>={?StartDate}</Value>
          </QueryParameter>
          <QueryParameter Name="EndDate">
            <Value>={?EndDate}</Value>
          </QueryParameter>
          <QueryParameter Name="discountreason_include">
            <Value>=Parameters!discountreason_include</Value>
          </QueryParameter>
          <QueryParameter Name="discountreason_exclude">
            <Value>=Parameters!discountreason_exclude</Value>
          </QueryParameter>
          <QueryParameter Name="DateType">
            <Value>={?DateType}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="create_date">
          <DataField>create_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
