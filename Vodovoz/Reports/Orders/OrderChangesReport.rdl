  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS</DataSourceName>
        <Timeout>1800</Timeout>
        <CommandText>SELECT
    source.comment_manager,
    sp.paid_date,
    source.order_id,
    (
        SELECT
            SUM(IFNULL(oi2.actual_count, oi2.count) * oi2.price - oi2.discount_money) as summary
        FROM order_items oi2
        WHERE oi2.order_id = source.order_id
    ) as summary,
    n2.name as nomenclature_name,
    CONCAT(c2.full_name, IF(c2.person_type = 'legal', CONCAT(' ИНН ', c2.inn), '')) as counterparty,
    source.shipped_date AS delivery_date,
    source.change_time,
    IF(source.operation = 'Delete', n2.official_name, IF(source.old_value = 'Наличная' and source.sms_new = 'True', 'Наличная,\nс оплатой по смс', source.old_value)) as old_value,
    IF(source.operation = 'Create', n2.official_name, source.new_value) as new_value,
    IF(hc2.user_login = 'vodovoz_service', 'Служба', GET_PERSON_NAME_WITH_INITIALS(e2.last_name, e2.name, e2.patronymic)) as author,
    GET_PERSON_NAME_WITH_INITIALS(driver.last_name, driver.name, driver.patronymic) as driver_fio
FROM
    (
        SELECT # Выборка изменений в Форме оплаты
               o.id as order_id,
               cc.organization_id,
               o.comment_manager,
               cc.counterparty_id,
               hce.changeset_id,
               hce.entity_class,
               hce.entity_id,
               hce.datetime as change_time,
               hce.operation,
               o.time_delivered as shipped_date,
               hc.type,
               'Форма оплаты' as field_name,
               IF(hce.operation IN ('Create', 'Delete'),
                  NULL,
                  CASE hc.old_value
                      WHEN 'cash' THEN 'Наличная'
                      WHEN 'cashless' THEN 'Безналичная'
                      WHEN 'barter' THEN 'Бартер'
                      WHEN 'ByCard' THEN 'По карте'
                      WHEN 'ContractDoc' THEN 'Контракт. док.'
                      WHEN 'BeveragesWorld' THEN 'Мир напитков'
                      WHEN 'Terminal' THEN 'Терминал'
                      ELSE hc.old_value
                      END
                   ) as old_value,
               IF(hce.operation IN ('Create', 'Delete'),
                  NULL,
                  CASE hc.new_value
                      WHEN 'cash' THEN 'Наличная'
                      WHEN 'cashless' THEN 'Безналичная'
                      WHEN 'barter' THEN 'Бартер'
                      WHEN 'ByCard' THEN 'По карте'
                      WHEN 'ContractDoc' THEN 'Контракт. док.'
                      WHEN 'BeveragesWorld' THEN 'Мир напитков'
                      WHEN 'Terminal' THEN 'Терминал'
                      ELSE hc.new_value
                      END
                   ) as new_value,
               (
	               select h.new_value
	               from history_changed_entities sub_hce
	               join history_changes h on sub_hce.id = h.chaged_entity_id
	               and h.field_name = 'PaymentBySms'
	               and h.type in ('Added', 'Changed')
                   where sub_hce.entity_id = o.id
                   and sub_hce.datetime &lt; o.time_delivered
                   order by h.id DESC
                   limit 1
               ) as sms_new
        FROM
            history_changed_entities hce
                INNER JOIN orders o ON o.id = hce.entity_id
                AND hce.entity_class = 'Order'
                AND hce.datetime &gt;= @start_date
                AND hce.datetime &lt; DATE_ADD(@end_date, INTERVAL 1 DAY)
                AND hce.operation = 'Change'
                INNER JOIN counterparty_contract cc ON cc.id = o.counterparty_contract_id
                AND cc.organization_id = @organization_id
                INNER JOIN history_changes hc_pay_type ON hc_pay_type.chaged_entity_id = hce.id
                AND hc_pay_type.type = 'Changed'
                AND hc_pay_type.field_name = 'PaymentType'
                AND
                (
                      (
                              (
                                          hc_pay_type.new_value IN ('cash', 'cashless')
                                      OR
                                          hc_pay_type.old_value IN ('cash', 'cashless')
                              )
                              AND FIND_IN_SET('PaymentType', @change_types) &gt; 0
                      )
                      OR
                      (
                              (
                                          hc_pay_type.new_value = 'Terminal'
                                      OR hc_pay_type.old_value = 'Terminal'
                              )
                              AND FIND_IN_SET('TerminalIssues', @issue_types) &gt; 0
                      )
                      OR
                      (
                              (
                                          hc_pay_type.new_value = 'ByCard'
                                      OR hc_pay_type.old_value = 'ByCard'
                              )
                              AND
                              (
                                  (
                                              FIND_IN_SET('ManagersIssues', @issue_types) &gt; 0
                                          AND o.payment_from_id &lt;&gt; GET_PARAMETER('sms_payment_by_card_from_id')
                                  )
                              )
                      )
                      OR
                      (
                                  FIND_IN_SET('SmsIssues', @issue_types) &gt; 0
                              AND o.payment_from_id = GET_PARAMETER('sms_payment_by_card_from_id')
                              AND hc_pay_type.new_value = 'ByCard'
                          )
                  )
                LEFT JOIN history_changes hc ON hc.chaged_entity_id = hce.id
        WHERE
                hce.datetime &gt; o.time_delivered
          AND hc.type = 'Changed'
          AND hc.field_name = 'PaymentType'
          AND (FIND_IN_SET('PaymentType', @change_types) &gt; 0 OR @issue_types &lt;&gt; '')
        UNION ALL
        SELECT # Выборка изменений в количестве или цене в строках заказов
               o.id as order_id,
               cc.organization_id,
               o.comment_manager,
               cc.counterparty_id,
               hce.changeset_id,
               hce.entity_class,
               hce.entity_id,
               hce.datetime as change_time,
               hce.operation,
               o.time_delivered as shipped_date,
               hc.type,
               IF(hce.operation IN ('Create', 'Delete'),
                  NULL,
                  CASE hc.field_name
                      WHEN 'ActualCount' THEN 'Факт. кол-во'
                      WHEN 'Price' THEN 'Цена'
                      ELSE hc.field_name
                      END
                   ) as field_name,
               IF(hce.operation IN ('Create', 'Delete'),
                  NULL,
                  CASE hc.old_value
                      WHEN 'cash' THEN 'Наличная'
                      WHEN 'cashless' THEN 'Безналичная'
                      WHEN 'barter' THEN 'Бартер'
                      WHEN 'ByCard' THEN 'По карте'
                      WHEN 'ContractDoc' THEN 'Контракт. док.'
                      WHEN 'BeveragesWorld' THEN 'Мир напитков'
                      WHEN 'Terminal' THEN 'Терминал'
                      ELSE hc.old_value
                      END
                   ) as old_value,
               IF(hce.operation IN ('Create', 'Delete'),
                  NULL,
                  CASE hc.new_value
                      WHEN 'cash' THEN 'Наличная'
                      WHEN 'cashless' THEN 'Безналичная'
                      WHEN 'barter' THEN 'Бартер'
                      WHEN 'ByCard' THEN 'По карте'
                      WHEN 'ContractDoc' THEN 'Контракт. док.'
                      WHEN 'BeveragesWorld' THEN 'Мир напитков'
                      WHEN 'Terminal' THEN 'Терминал'
                      ELSE hc.new_value
                      END
                   ) as new_value,
               (
                   select h.new_value
                   from history_changed_entities sub_hce
                   join history_changes h on sub_hce.id = h.chaged_entity_id
                       and h.field_name = 'PaymentBySms'
                       and h.type in ('Added', 'Changed')
                   where sub_hce.entity_id = o.id
                   and sub_hce.datetime &lt; o.time_delivered
                   order by h.id DESC
                   limit 1
               ) as sms_new
        FROM
            history_changed_entities hce
                INNER JOIN order_items oi ON oi.id = hce.entity_id
                AND hce.entity_class = 'OrderItem'
                AND hce.datetime &gt;= @start_date
                AND hce.datetime &lt; DATE_ADD(@end_date, INTERVAL 1 DAY)
                AND hce.operation IN ('Create','Change','Delete')
                INNER JOIN orders o ON o.id = oi.order_id
                INNER JOIN counterparty_contract cc ON cc.id = o.counterparty_contract_id
                AND cc.organization_id = @organization_id
                INNER JOIN (
                SELECT
                    o_pay_type.id as order_id,
                    hce_pay_type.datetime
                FROM history_changed_entities hce_pay_type
                         INNER JOIN orders o_pay_type ON o_pay_type.id = hce_pay_type.entity_id
                    AND hce_pay_type.entity_class = 'Order'
                    AND hce_pay_type.operation IN ('Create', 'Change')
                         INNER JOIN history_changes hc_pay_type ON hc_pay_type.chaged_entity_id = hce_pay_type.id
                    AND hc_pay_type.type IN ('Added', 'Changed')
                    AND hc_pay_type.field_name = 'PaymentType'
                    AND
                   (
                           (
                                   (
                                               hc_pay_type.new_value IN ('cash', 'cashless')
                                           OR
                                               hc_pay_type.old_value IN ('cash', 'cashless')
                                       )
                                   AND FIND_IN_SET('PaymentType', @change_types) &gt; 0
                               )
                           OR
                           (
                                   (
                                               hc_pay_type.new_value = 'Terminal'
                                           OR hc_pay_type.old_value = 'Terminal'
                                       )
                                   AND
                                   FIND_IN_SET('TerminalIssues', @issue_types) &gt; 0
                               )
                           OR
                           (
                                   (
                                               hc_pay_type.new_value = 'ByCard'
                                           OR
                                               hc_pay_type.old_value = 'ByCard'
                                       )
                                   AND
                                   (
                                               FIND_IN_SET('ManagersIssues', @issue_types) &gt; 0
                                           AND
                                               o_pay_type.payment_from_id &lt;&gt; GET_PARAMETER('sms_payment_by_card_from_id')
                                       )
                               )
                           OR
                           (
                                       FIND_IN_SET('SmsIssues', @issue_types) &gt; 0
                                   AND
                                       o_pay_type.payment_from_id = GET_PARAMETER('sms_payment_by_card_from_id')
                                   AND
                                       hc_pay_type.new_value = 'ByCard'
                               )
                       )
                GROUP BY hce_pay_type.entity_id
            ) payment_type_changes ON payment_type_changes.order_id = o.id
                LEFT JOIN history_changes hc ON hc.chaged_entity_id = hce.id
        WHERE
                hce.datetime &gt; o.time_delivered
          AND (
                (hc.type = 'Changed' AND hc.field_name IN ('Price', 'ActualCount') AND FIND_IN_SET('Price', @change_types) &gt; 0 AND FIND_IN_SET('ActualCount', @change_types) &gt; 0)
                OR
                (hce.operation IN ('Create','Delete') AND hc.field_name = 'Price' AND FIND_IN_SET('OrderItemsCount', @change_types) &gt; 0)
            )
    ) as source
        LEFT JOIN sms_payment sp ON sp.order_id = source.order_id AND sp.payment_status = 'Paid'
        LEFT JOIN history_changeset hc2 ON hc2.id = source.changeset_id
        LEFT JOIN employees e2 ON e2.user_id = hc2.user_id
        LEFT JOIN order_items oi3 ON oi3.id = source.entity_id AND source.entity_class = 'OrderItem'
        LEFT JOIN nomenclature n2 ON n2.id = oi3.nomenclature_id
        LEFT JOIN route_list_addresses rla ON source.order_id = rla.order_id
        LEFT JOIN route_lists rl ON rla.route_list_id = rl.id AND rla.status &lt;&gt; 'Transfered'
        LEFT JOIN employees driver ON rl.driver_id = driver.id
        LEFT JOIN counterparty c2 ON source.counterparty_id = c2.id
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Format(Convert.ToDateTime({?start_date}), "yyyy-MM-dd")</Value>
          </QueryParameter>
