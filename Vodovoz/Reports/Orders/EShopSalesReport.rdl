  </DataSources>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT o.e_shop_order AS eshop_order_id,
    n.online_store_external_id AS online_store_external_id,
    o.delivery_date AS delivery_date,
    n.official_name AS nomenclature_name,
    oi.price AS nomenclature_price,
    oi.count AS amount,
    (d.price - d.discount_money) AS delivery_price,
    oi.discount_money AS discount,
    (oi.price - oi.discount_money) * oi.count AS total_sum,
    CASE
        WHEN o.order_status = 'NewOrder' THEN 'Новый'
        WHEN o.order_status = 'WaitForPayment' THEN 'Ожидание оплаты'
        WHEN o.order_status = 'Accepted' THEN 'Принят'
        WHEN o.order_status = 'InTravelList' THEN 'В маршрутном листе'
        WHEN o.order_status = 'OnLoading' THEN 'На погрузке'
        WHEN o.order_status = 'OnTheWay' THEN 'В пути'
        WHEN o.order_status = 'Shipped' THEN 'Доставлен'
        WHEN o.order_status = 'UnloadingOnStock' THEN 'Выгрузка на складе'
        WHEN o.order_status = 'Closed' THEN 'Закрыт'
        WHEN o.order_status = 'Canceled' THEN 'Отменён'
        WHEN o.order_status = 'NotDelivered' THEN 'Недовоз'
        WHEN o.order_status = 'DeliveryCanceled' THEN 'Доставка отменена'
        ELSE ''
    END AS order_status
FROM order_items oi 
LEFT JOIN orders o ON o.id = oi.order_id
LEFT JOIN nomenclature n ON n.id = oi.nomenclature_id
LEFT JOIN order_items d ON oi.order_id = d.order_id
INNER JOIN nomenclature dn ON d.nomenclature_id = dn.id AND dn.id = (SELECT CAST(bp.str_value AS UNSIGNED INTEGER) FROM base_parameters bp WHERE bp.name = 'paid_delivery_nomenclature_id')
WHERE o.id IS NOT NULL
  AND o.e_shop_order IS NOT NULL
  AND o.delivery_date &gt;= @start_date
  AND o.delivery_date &lt;= @end_date
  AND n.online_store_external_id IS NOT NULL
  AND (@e_shop_id = -1 OR n.online_store_id = @e_shop_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="e_shop_id">
            <Value>=Parameters!e_shop_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="eshop_order_id">
          <DataField>eshop_order_id</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="online_store_external_id">
          <DataField>online_store_external_id</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="delivery_date">
          <DataField>delivery_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature_price">
          <DataField>nomenclature_price</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="delivery_price">
          <DataField>delivery_price</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="discount">
          <DataField>discount</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="total_sum">
          <DataField>total_sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="order_status">
          <DataField>order_status</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="EShopName" >
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT IF(@e_shop_id = -1, "Все", os.name) AS online_store_name
FROM online_stores os 
WHERE (@e_shop_id = -1 OR id = @e_shop_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="e_shop_id">
            <Value>=Parameters!e_shop_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="online_store_name">
          <DataField>online_store_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
