  <DataSets>
    <DataSet Name="Orders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	MainOrders.id,
	counterparty.name as CounterpartyName,
    delivery_points.compiled_address_short as AddressShort,
	MainOrders.delivery_date as DeliveryDate,
    (
		SELECT SUM(count) 
        FROM order_items as SumOrderItems
			LEFT JOIN nomenclature as Nom2 ON SumOrderItems.nomenclature_id = Nom2.id
        WHERE SumOrderItems.order_id = MainOrders.id AND Nom2.category = 'water'
	) as BottleCount,
    MainOrderItems.Price as Price,
	MainOrderItems.minPrice as PricePerOne,
    counterparty.comment as comment
FROM
	orders as MainOrders
	LEFT JOIN 
		(
			SELECT 
            order_items.id,
            order_id,
            nomenclature_id,
            price,
            (
				# Нахождение минимальной цены по количеству
				SELECT 
					nomenclature_price.price
				FROM
					nomenclature_price
				WHERE 
					nomenclature_price.nomenclature_id = 1 /* Цены выбираются по номенклатуре Семиозерье 19л (Id = 1) */ 
					AND nomenclature_price.min_count &lt;= order_items.count
				ORDER BY nomenclature_price.price ASC LIMIT 1 
             ) as minPrice,
            count,
            nomenclature.category
            FROM order_items
				LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
        ) as MainOrderItems 
        ON MainOrderItems.order_id = MainOrders.id
    LEFT JOIN counterparty ON MainOrders.client_id = counterparty.id
    LEFT JOIN delivery_points ON MainOrders.delivery_point_id = delivery_points.id
WHERE
	#Находит только те записи у которых цена ниже чем задана по прайсу
	MainOrderItems.price &lt; 
		(
			SELECT         
				(
					# Нахождение минимальной цены по количеству
					SELECT 
						nomenclature_price.price
					FROM
						nomenclature_price
					WHERE 
						nomenclature_price.nomenclature_id = 1 /* Цены выбираются по номенклатуре Семиозерье 19л (Id = 1) */ 
						AND nomenclature_price.min_count &lt;= oItems3.count
					ORDER BY nomenclature_price.price ASC LIMIT 1 
				) as minPrice
			FROM order_items as oItems3
				LEFT JOIN nomenclature as Nom3 ON oItems3.nomenclature_id = Nom3.id
			WHERE oItems3.order_id = MainOrders.id AND Nom3.category = 'water'
			ORDER BY minPrice ASC LIMIT 1
		)
	AND MainOrderItems.price &gt; 0
	AND MainOrderItems.category = 'water'
	AND MainOrders.order_status = 'Closed'
    AND (@DateFrom IS NULL OR MainOrders.delivery_date &gt;= @DateFrom) AND ( @DateTo IS NULL OR MainOrders.delivery_date &lt;= @DateTo)
    
    #Нахождение только последнего по дате заказа
    AND NOT EXISTS (
		SELECT 
			*
        FROM 
			orders as SubOrders
			LEFT JOIN order_items as oItems1 ON oItems1.order_id = SubOrders.id
			LEFT JOIN nomenclature as Nom1 ON oItems1.nomenclature_id = Nom1.id
        WHERE 
			MainOrders.delivery_point_id = SubOrders.delivery_point_id
			AND SubOrders.delivery_date &gt; MainOrders.delivery_date
            #Находит только те записи у которых цена ниже чем задана по прайсу
            AND oItems1.price &lt; 
				(
					SELECT         
						(
							# Нахождение минимальной цены по количеству
							SELECT 
								nomenclature_price.price
							FROM
								nomenclature_price
							WHERE 
								nomenclature_price.nomenclature_id = 1 /* Цены выбираются по номенклатуре Семиозерье 19л (Id = 1) */ 
								AND nomenclature_price.min_count &lt;= oItems3.count
							ORDER BY nomenclature_price.price ASC LIMIT 1 
						 ) as minPrice
					FROM order_items as oItems3
						LEFT JOIN nomenclature as Nom3 ON oItems3.nomenclature_id = Nom3.id
					WHERE oItems3.order_id = MainOrders.id AND Nom3.category = 'water'
					ORDER BY minPrice ASC LIMIT 1
				)
			AND oItems1.price &gt; 0
			AND Nom1.category = 'water'
			AND SubOrders.order_status = 'Closed'
    )
GROUP BY delivery_point_id ORDER BY DeliveryDate DESC, MainOrderItems.price
</CommandText>
        <QueryParameters>
          <QueryParameter Name="dateFrom">
            <Value>=Iif({?dateFrom}="", NULL, {?dateFrom})</Value>
          </QueryParameter>
          <QueryParameter Name="dateTo">
            <Value>=Parameters!dateTo</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
