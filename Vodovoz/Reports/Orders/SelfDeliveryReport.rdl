  <DataSets>
    <DataSet Name="orders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select
	orders.id,
	orders.code1c,
	counterparty.name,
	orders.sum_to_receive,
	orders.order_status,
	ifnull(group_concat(warehouses.name separator ','), '') as warehouse_name
from
	orders
left join
	counterparty on counterparty.id = orders.client_id
left join
	store_self_delivery_document on store_self_delivery_document.order_id = orders.id
left join
	warehouses on warehouses.id = store_self_delivery_document.warehouse_id
where
		self_delivery = 1
	and
		delivery_date = DATE(@date)
group by
	orders.id,
	orders.code1c,
	counterparty.name,
	orders.sum_to_receive,
	orders.order_status</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="code1c">
          <DataField>code1c</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="sum_to_receive">
          <DataField>sum_to_receive</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="order_status">
          <DataField>order_status</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="warehouse_name">
          <DataField>warehouse_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
