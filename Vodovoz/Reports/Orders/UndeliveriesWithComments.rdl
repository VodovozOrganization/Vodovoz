    <DataSet Name="Guilties">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	COUNT(undelivered_orders.id) AS qty,
	CASE guilty_is
		WHEN 'Department' THEN 'Отдел ВВ'
		WHEN 'Client' THEN 'Клиент'
		WHEN 'Driver' THEN 'Водитель'
		ELSE 'Не установленно'
	END AS guilty
FROM
	undelivered_orders
LEFT JOIN
	orders oldo ON undelivered_orders.undelivered_order_id = oldo.id
WHERE 
	oldo.delivery_date &gt;= @start_date AND oldo.delivery_date &lt;= @end_date
GROUP BY
	guilty_is
;</CommandText>
        <QueryParameters>
