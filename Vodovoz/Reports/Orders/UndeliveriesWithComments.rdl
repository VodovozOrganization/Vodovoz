  <DataSets>
    <DataSet Name="UndeliveriesAndComments">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	un_ord_subqry.N,
	CONCAT_WS(
		'\n',
		DATE_FORMAT(un_ord_subqry.old_order_date, '%e.%m'),
		cmnt.old_order_delivery_date
	) AS old_order_date,
	CONCAT_WS(
		'\n',
		un_ord_subqry.reason,
		cmnt.reason
	) AS reason,
	CONCAT_WS(
		'\n',
		un_ord_subqry.new_order_id,
		cmnt.action_with_invoice
	) AS action_with_invoice,
	CONCAT_WS(
		'\n',
		un_ord_subqry.transfered,
		cmnt.transfer_date_time
	) AS transfer_date_time,
	CONCAT_WS(
		'\n',
		un_ord_subqry.client,
		cmnt.client
	) AS client,
	CONCAT_WS(
		'\n',
		un_ord_subqry.address,
		cmnt.address
	) AS address,
	CONCAT_WS(
		'\n',
		IFNULL(un_ord_subqry.bottles, IFNULL(un_ord_subqry.goo_to_client, IFNULL(un_ord_subqry.goo_from_client, 'Другие товары'))),
		cmnt.undelivered_order_items
	) AS undelivered_order_items,
	CONCAT_WS(
		'\n',
		un_ord_subqry.old_interval,
		cmnt.old_delivery_schedule
	) AS old_delivery_schedule,
	CONCAT_WS(
		'\n',
		un_ord_subqry.old_order_author,
		cmnt.old_order_author
	) AS old_order_author,
	CONCAT_WS(
		'\n',
		un_ord_subqry.driver,
		cmnt.driver_name
	) AS driver_name,
	CONCAT_WS(
		'\n',
		un_ord_subqry.driver_call_time,
		cmnt.drivers_call
	) AS drivers_call,
	CONCAT_WS(
		'\n',
		un_ord_subqry.dispatcher_call_time,
		cmnt.dispatcher_call
	) AS dispatcher_call,
	CONCAT_WS(
		'\n',
		un_ord_subqry.registered_by,
		cmnt.registrator
	) AS registrator,
	CONCAT_WS(
		'\n',
		un_ord_subqry.undelivery_author,
		cmnt.undelivery_author
	) AS undelivery_author,
	CONCAT_WS(
		'\n',
		un_ord_subqry.guilty,
		cmnt.guilty
	) AS guilty,
	CONCAT_WS(
		'\n',
		un_ord_subqry.status,
		cmnt.status
	) AS status
FROM
#поля недовоза
(
	SELECT 
		un.id AS N,
		oldo.delivery_date AS old_order_date,
		un.reason AS reason,
		un.new_order_id AS new_order_id,
		CONCAT_WS('\n',newo.delivery_date, new_schedule.name) AS transfered,
		coun.name AS client,
		(
			IF(
				LOCATE(', пар', compiled_address_short) &gt; 0,
				SUBSTRING(compiled_address_short, 1, LOCATE(', пар', compiled_address_short)-1),
				IF(
					LOCATE(', эт', compiled_address_short) &gt; 0,
					SUBSTRING(compiled_address_short, 1, LOCATE(', эт', compiled_address_short)-1),
					IF(
						LOCATE(', кв', compiled_address_short) &gt; 0,
						SUBSTRING(compiled_address_short, 1, LOCATE(', кв', compiled_address_short)-1),
						compiled_address_short
					)
				)
			)
		) AS address,
		#кол-во воды к клиенту
		(
			SELECT SUM(order_items.count)
			FROM order_items
			LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
			WHERE order_items.order_id = oldo.id AND nomenclature.category = 'water'
		) AS bottles,
		#товары к клиенту
		(
			SELECT CONCAT(
				'К клиенту:\n',
				TRIM(
					GROUP_CONCAT(
						CONCAT(
							IF(nomenclature.short_name IS NULL, nomenclature.name, nomenclature.short_name),
							':',
							order_equipment.count
							)
						SEPARATOR '\n'
						)
					)
				)
			FROM order_equipment
			LEFT JOIN nomenclature ON nomenclature.id = order_equipment.nomenclature_id
			WHERE order_equipment.order_id = oldo.id AND order_equipment.direction = 'Deliver'
		) AS goo_to_client,
		#товары от клиента
		(
			SELECT CONCAT(
				'От клиента:\n',
				TRIM(
					GROUP_CONCAT(
						CONCAT(
							IF(nomenclature.short_name IS NULL, nomenclature.name, nomenclature.short_name),
							':',
							order_equipment.count
							)
						SEPARATOR '\n'
						)
					)
				)
			FROM order_equipment
			LEFT JOIN nomenclature ON nomenclature.id = order_equipment.nomenclature_id
			WHERE order_equipment.order_id = oldo.id AND order_equipment.direction = 'PickUp'
		) AS goo_from_client,
		old_schedule.name AS old_interval,
		TRIM(BOTH '.' FROM CONCAT_WS('.', LEFT(oaut.name, 1), LEFT(oaut.patronymic, 1), oaut.last_name)) AS old_order_author,
		#водители
		IFNULL(
			(SELECT TRIM(BOTH '.' FROM CONCAT_WS('.', LEFT(employees.name, 1), LEFT(employees.patronymic, 1), employees.last_name))
			FROM route_list_addresses
			LEFT JOIN route_lists ON route_list_addresses.route_list_id = route_lists.id
			LEFT JOIN employees ON employees.id = route_lists.driver_id
			WHERE route_list_addresses.order_id = oldo.id),
			'Не в МЛ'
		) AS driver,
		#время звонка водителя, если стаус старого заказа подходит для существования МЛ
		(
			IF(
				un.undelivered_order_status IN('NewOrder','WaitForPayment','Accepted'),
				'Не в МЛ',
				(
					CASE un.driver_call_type
						WHEN 'NoCall' THEN 'Не звонил'
						WHEN 'CallFromAddress' THEN  CONCAT_WS('\n', 'С адреса', DATE_FORMAT(un.driver_call_time, '%H:%i'))
						WHEN 'CallFromAnywhere' THEN CONCAT_WS('\n', 'Не с адреса', DATE_FORMAT(un.driver_call_time, '%H:%i'))
					END)
				)
		) AS driver_call_time,
		DATE_FORMAT(un.dispatcher_call_time, '%H:%i') AS dispatcher_call_time,
		TRIM(BOTH '.' FROM CONCAT_WS('.', LEFT(ureg.name, 1), LEFT(ureg.patronymic, 1), ureg.last_name)) AS registered_by,
		TRIM(BOTH '.' FROM CONCAT_WS('.', LEFT(uaut.name, 1), LEFT(uaut.patronymic, 1), uaut.last_name)) AS undelivery_author,
		CASE guilty_is
			WHEN 'Client' THEN 'Клиент'
			WHEN 'Driver' THEN 'Водитель'
			WHEN 'Department' THEN IF(dep.name IS NULL, 'Отдел ВВ', dep.name)
			ELSE 'Неизвестно'
		END
		AS guilty,
		CASE un.status
			WHEN 'InProcess' THEN 'В работе'
			WHEN 'Checking' THEN 'На проверке'
			WHEN 'Closed' THEN 'Закрыт'
		END AS status
	FROM undelivered_orders un
	LEFT JOIN orders oldo ON un.undelivered_order_id = oldo.id
	LEFT JOIN delivery_schedule old_schedule ON old_schedule.id = oldo.delivery_schedule_id
	LEFT JOIN orders newo ON un.new_order_id = newo.id
	LEFT JOIN delivery_schedule new_schedule ON new_schedule.id = newo.delivery_schedule_id
	LEFT JOIN counterparty coun ON coun.id = oldo.client_id
	LEFT JOIN delivery_points delp ON delp.id = oldo.delivery_point_id
	LEFT JOIN employees oaut ON oaut.id = oldo.author_employee_id
	LEFT JOIN employees ureg ON ureg.id = un.registered_by_employee_id
	LEFT JOIN employees uaut ON uaut.id = un.author_employee_id
	LEFT JOIN subdivisions dep ON dep.id = un.guilty_department_id
	LEFT JOIN route_list_addresses rla ON rla.order_id = oldo.id
	LEFT JOIN route_lists rl ON rla.route_list_id = rl.id
	WHERE 
		oldo.delivery_date &gt;= @start_date AND oldo.delivery_date &lt;= @end_date
	AND
		(oldo.id = @old_order_id OR @old_order_id = 0)
	AND
		(rl.driver_id = @driver_id AND @driver_id != 0 OR @driver_id = 0)
 	AND
		(oldo.client_id = @client_id OR @client_id = 0)
 	AND
		(oldo.delivery_point_id = @address_id OR @address_id = 0)
 	AND
		(oldo.author_employee_id = @old_order_author_id OR @old_order_author_id = 0)
 	AND
		(newo.delivery_date &gt;= @new_order_start_date AND newo.delivery_date &lt;= @new_order_end_date OR @new_order_start_date = '' OR @new_order_end_date = '')
 	AND
		(un.guilty_is = @guilty_side OR @guilty_side = '')
 	AND
		(un.guilty_department_id = @guilty_department_id OR @guilty_department_id = 0)
  	AND
		(un.new_order_id IS NOT NULL AND @new_invoice_created = 'true' OR un.new_order_id IS NULL AND @new_invoice_created = 'false' OR @new_invoice_created = '')
 	AND
		(un.status = @undelivery_status OR @undelivery_status = '')
 	AND
		(un.author_employee_id = @undelivery_author_id OR @undelivery_author_id = 0)
) AS un_ord_subqry
#комментарии к полям
LEFT JOIN (
	SELECT 
		undelivery_id AS uid,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'OldOrderDeliveryDate',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e.%m.%y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as old_order_delivery_date,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'Reason',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as reason,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'ActionWithInvoice',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as action_with_invoice,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'TransferDateTime',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as transfer_date_time,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'Client',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as client,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'Address',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as address,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'UndeliveredOrderItems',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as undelivered_order_items,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'OldDeliverySchedule',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as old_delivery_schedule,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'OldOrderAuthor',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as old_order_author,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'driverName',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as driver_name,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'DriversCall',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as drivers_call,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'DispatcherCall',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as dispatcher_call,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'Registrator',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as registrator,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'UndeliveryAuthor',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as undelivery_author,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'Guilty',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as guilty,
		CONCAT(
			'--комментарии--\n',
			GROUP_CONCAT(
				IF(
					field_name = 'Status',
					CONCAT(
						DATE_FORMAT(cm.date_and_time, '%e %b %Y, %k:%i:%s'),
						'\n',
						CONCAT_WS(' ', e.name, e.last_name),
						':\n',
						cm.comment
					),
					null
				)
				ORDER BY cm.date_and_time ASC
				SEPARATOR '\n'
			)
		) as status
	FROM undelivered_orders_comments cm
	LEFT JOIN employees e ON e.id = cm.employee_id
	GROUP BY undelivery_id
) AS cmnt ON cmnt.uid = un_ord_subqry.N
;</CommandText>
