  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	byOrders.*,
	(
		SELECT COUNT(*) = 0
		FROM route_lists
		WHERE route_lists.date = byOrders.date AND route_lists.status != 'Closed'
	) AS all_closed,
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM route_lists
		INNER JOIN store_car_load_documents ON store_car_load_documents.route_list_id = route_lists.id
		INNER JOIN store_car_load_document_items ON store_car_load_document_items.car_load_document_id = store_car_load_documents.id
		INNER JOIN warehouse_movement_operations ON store_car_load_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE route_lists.date = byOrders.date
			AND (nomenclature.category = 'water' AND !nomenclature.is_disposable_tare OR nomenclature.category = 'bottle')
			AND nomenclature.is_new_bottle = 0
			AND nomenclature.is_defective_bottle = 0
	) AS car_load_count,
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM route_lists
		INNER JOIN store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
		INNER JOIN store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
		INNER JOIN warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE route_lists.date = byOrders.date
			AND (nomenclature.category = 'water' AND !nomenclature.is_disposable_tare OR nomenclature.category = 'bottle')
			AND nomenclature.is_new_bottle = 0
			AND nomenclature.is_defective_bottle = 0
	) AS car_unload_count,
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM route_lists
		INNER JOIN store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
		INNER JOIN store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
		INNER JOIN warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE route_lists.date = byOrders.date
			AND nomenclature.category = 'bottle'
			AND nomenclature.is_defective_bottle = 1
	) +
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM store_incoming_invoice
		INNER JOIN store_incoming_invoice_items ON store_incoming_invoice_items.incoming_invoice_id = store_incoming_invoice.id
		INNER JOIN warehouse_movement_operations ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE store_incoming_invoice.time_stamp &gt;= byOrders.date
			AND store_incoming_invoice.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
			AND nomenclature.category = 'bottle'
			AND nomenclature.is_defective_bottle = 1
	) +
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM store_inventory
		INNER JOIN store_inventory_item ON store_inventory_item.store_inventory_id = store_inventory.id
		INNER JOIN warehouse_movement_operations ON store_inventory_item.warehouse_movement_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE store_inventory.time_stamp &gt;= byOrders.date
			AND store_inventory.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
			AND nomenclature.category = 'bottle'
			AND nomenclature.is_defective_bottle = 1
	) +
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM store_regrading_of_goods
		INNER JOIN store_regrading_of_goods_items ON store_regrading_of_goods_items.store_regrading_of_goods_id = store_regrading_of_goods.id
		INNER JOIN warehouse_movement_operations ON store_regrading_of_goods_items.warehouse_income_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE store_regrading_of_goods.time_stamp &gt;= byOrders.date
			AND store_regrading_of_goods.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
			AND nomenclature.category = 'bottle'
			AND nomenclature.is_defective_bottle = 1
	) AS defective_count,
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM route_lists
		INNER JOIN store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
		INNER JOIN store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
		INNER JOIN warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE route_lists.date = byOrders.date
			AND nomenclature.category = 'bottle'
			AND nomenclature.is_new_bottle = 1
	) +
	(
		SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
		FROM store_incoming_invoice
		INNER JOIN store_incoming_invoice_items ON store_incoming_invoice_items.incoming_invoice_id = store_incoming_invoice.id
		INNER JOIN warehouse_movement_operations ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
		INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
		WHERE store_incoming_invoice.time_stamp &gt;= byOrders.date
			AND store_incoming_invoice.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
			AND nomenclature.category = 'bottle'
			AND nomenclature.is_new_bottle = 1
	) AS new_bottle_count
FROM(
		SELECT
			route_lists.date,
			r.name,
			SUM(IF(
					bottles_movement_operations.delivered &gt; bottles_movement_operations.returned,
					bottles_movement_operations.delivered,
					0
			)) AS delivered,
			SUM(IF(
					bottles_movement_operations.delivered &gt; bottles_movement_operations.returned,
					bottles_movement_operations.returned,
					0
			)) AS returned
		FROM route_lists
		LEFT JOIN route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
		JOIN orders ON route_list_addresses.order_id = orders.id AND route_list_addresses.was_transfered = 0
		LEFT JOIN non_return_reasons r ON orders.tare_non_return_reason_id = r.id
		LEFT JOIN bottles_movement_operations ON orders.bottles_movement_operation_id = bottles_movement_operations.id
		WHERE route_lists.date BETWEEN @start_date AND @end_date
		GROUP BY route_lists.date, r.id
) byOrders
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="delivered">
          <DataField>delivered</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="returned">
          <DataField>returned</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="all_closed">
          <DataField>all_closed</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="car_load_count">
          <DataField>car_load_count</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="car_unload_count">
          <DataField>car_unload_count</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="defective_count">
          <DataField>defective_count</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="new_bottle_count">
          <DataField>new_bottle_count</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Totals">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	date,
	car_unload_count-car_load_count-SUM(T1.returned - T1.delivered) AS total_came,
	car_unload_count-car_load_count AS wh_delta,
	defective_count AS defective_total,
	car_unload_count-car_load_count-defective_count AS total_lost,
	new_bottle_count AS new_bottles_total
FROM(
	SELECT
		byOrders.*,
		(
			SELECT COUNT(*) = 0
			FROM route_lists
			WHERE route_lists.date = byOrders.date AND route_lists.status != 'Closed'
		) AS all_closed,
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM route_lists
			INNER JOIN store_car_load_documents ON store_car_load_documents.route_list_id = route_lists.id
			INNER JOIN store_car_load_document_items ON store_car_load_document_items.car_load_document_id = store_car_load_documents.id
			INNER JOIN warehouse_movement_operations ON store_car_load_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE route_lists.date = byOrders.date
				AND (nomenclature.category = 'water' AND !nomenclature.is_disposable_tare OR nomenclature.category = 'bottle')
				AND nomenclature.is_new_bottle = 0
				AND nomenclature.is_defective_bottle = 0
		) AS car_load_count,
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM route_lists
			INNER JOIN store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
			INNER JOIN store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
			INNER JOIN warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE route_lists.date = byOrders.date
				AND (nomenclature.category = 'water' AND !nomenclature.is_disposable_tare OR nomenclature.category = 'bottle')
				AND nomenclature.is_new_bottle = 0
				AND nomenclature.is_defective_bottle = 0
		) AS car_unload_count,
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM route_lists
			INNER JOIN store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
			INNER JOIN store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
			INNER JOIN warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE route_lists.date = byOrders.date
				AND nomenclature.category = 'bottle'
				AND nomenclature.is_defective_bottle = 1
		) +
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM store_incoming_invoice
			INNER JOIN store_incoming_invoice_items ON store_incoming_invoice_items.incoming_invoice_id = store_incoming_invoice.id
			INNER JOIN warehouse_movement_operations ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE store_incoming_invoice.time_stamp &gt;= byOrders.date
				AND store_incoming_invoice.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
				AND nomenclature.category = 'bottle'
				AND nomenclature.is_defective_bottle = 1
		) +
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM store_inventory
			INNER JOIN store_inventory_item ON store_inventory_item.store_inventory_id = store_inventory.id
			INNER JOIN warehouse_movement_operations ON store_inventory_item.warehouse_movement_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE store_inventory.time_stamp &gt;= byOrders.date
				AND store_inventory.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
				AND nomenclature.category = 'bottle'
				AND nomenclature.is_defective_bottle = 1
		) +
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM store_regrading_of_goods
			INNER JOIN store_regrading_of_goods_items ON store_regrading_of_goods_items.store_regrading_of_goods_id = store_regrading_of_goods.id
			INNER JOIN warehouse_movement_operations ON store_regrading_of_goods_items.warehouse_income_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE store_regrading_of_goods.time_stamp &gt;= byOrders.date
				AND store_regrading_of_goods.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
				AND nomenclature.category = 'bottle'
				AND nomenclature.is_defective_bottle = 1
		) AS defective_count,
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM route_lists
			INNER JOIN store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
			INNER JOIN store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
			INNER JOIN warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE route_lists.date = byOrders.date
				AND nomenclature.category = 'bottle'
				AND nomenclature.is_new_bottle = 1
		) +
		(
			SELECT IFNULL(SUM(warehouse_movement_operations.amount), 0)
			FROM store_incoming_invoice
			INNER JOIN store_incoming_invoice_items ON store_incoming_invoice_items.incoming_invoice_id = store_incoming_invoice.id
			INNER JOIN warehouse_movement_operations ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
			INNER JOIN nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
			WHERE store_incoming_invoice.time_stamp &gt;= byOrders.date
				AND store_incoming_invoice.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
				AND nomenclature.category = 'bottle'
				AND nomenclature.is_new_bottle = 1
		) AS new_bottle_count
	FROM(
			SELECT
				route_lists.date,
				r.name,
				SUM(IF(
						bottles_movement_operations.delivered &gt; bottles_movement_operations.returned,
						bottles_movement_operations.delivered,
						0
				)) AS delivered,
				SUM(IF(
						bottles_movement_operations.delivered &gt; bottles_movement_operations.returned,
						bottles_movement_operations.returned,
						0
				)) AS returned
			FROM route_lists
			LEFT JOIN route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
			JOIN orders ON route_list_addresses.order_id = orders.id AND route_list_addresses.was_transfered = 0
			LEFT JOIN non_return_reasons r ON orders.tare_non_return_reason_id = r.id
			LEFT JOIN bottles_movement_operations ON orders.bottles_movement_operation_id = bottles_movement_operations.id
			WHERE route_lists.date BETWEEN @start_date AND @end_date
			GROUP BY route_lists.date, r.id
	) byOrders
) AS T1
GROUP BY T1.date
;</CommandText>
