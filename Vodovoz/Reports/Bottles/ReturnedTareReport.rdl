  <DataSets>
    <DataSet Name="orders">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    orders.bottles_return,
    IF(orders.order_status = 'Closed', sum(rla.bottles_returned), 0) as bottles_returned,
	counterparty.full_name AS client,
	rtr.name as reason_name,
	rtrc.name as category_name,
	orders.comment_manager as comment,
    GROUP_CONCAT(
		CONCAT(
			'+7 ',
			dp_phones.number,
			IF(
				dp_phones.name IS NULL,
				'',
				CONCAT(' ', dp_phones.name)
			)
		)
		SEPARATOR '\n') AS delivery_point_contact,
	GROUP_CONCAT(
		CONCAT(
			'+7 ',
			client_phones.number,
			IF(
				client_phones.name IS NULL,
				'',
				CONCAT(' ', client_phones.name)
			)
		)
		SEPARATOR '\n') AS client_contact,
	CONCAT(employees.last_name, ' ', LEFT(employees.name, 1), '.', LEFT(employees.patronymic, 1), '.') AS author
FROM orders
LEFT JOIN counterparty ON counterparty.id = orders.client_id
LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
LEFT JOIN phones as dp_phones ON delivery_points.id = dp_phones.delivery_point_id
LEFT JOIN phones as client_phones ON counterparty.id = client_phones.counterparty_id
LEFT JOIN employees ON orders.author_employee_id = employees.id
LEFT JOIN route_list_addresses rla ON orders.id = rla.order_id
LEFT JOIN return_tare_reasons rtr ON orders.return_tare_reason_id = rtr.id
LEFT JOIN return_tare_reason_categories rtrc ON orders.return_tare_reason_category_id = rtrc.id
WHERE
	orders.bottles_return &gt; 0
	AND NOT EXISTS (
		SELECT oi2.id 
		FROM order_items oi2 
		LEFT JOIN nomenclature n2 ON oi2.nomenclature_id = n2.id 
		WHERE oi2.order_id = orders.id AND n2.category = 'water' AND n2.tare_volume = 'Vol19l'
	)
	AND orders.delivery_date BETWEEN @start_date AND @end_date
GROUP BY orders.id
ORDER BY orders.id DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="bottles_return">
          <DataField>bottles_return</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="bottles_returned">
          <DataField>bottles_returned</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="client">
          <DataField>client</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="reason_name">
          <DataField>reason_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="author">
          <DataField>author</DataField>
