        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="reasons_names">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>Select rtr.name as name, count(rtr.name) as count
From orders o
Left Join return_tare_reasons rtr On o.return_tare_reason_id = rtr.id
WHERE
	o.bottles_return &gt; 0
	AND rtr.name is not null
	AND NOT EXISTS (
		SELECT o2.id 
		From orders o2
		LEFT JOIN order_items oi2 ON oi2.order_id = o2.id
		LEFT JOIN nomenclature n2 ON n2.id = oi2.nomenclature_id
		WHERE o2.id = o.id
		AND n2.category = 'water' 
		AND n2.tare_volume = 'Vol19l'
	)
	AND o.delivery_date BETWEEN @start_date AND @end_date
GROUP BY rtr.name</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
