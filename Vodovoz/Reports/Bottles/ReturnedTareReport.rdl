        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="reasons_categories">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>Select 
	rtrc.name as category,
	count(rtrc.name) as count
From orders o
Left Join return_tare_reason_categories rtrc On o.return_tare_reason_category_id = rtrc.id
WHERE
	o.bottles_return &gt; 0
	AND rtrc.name is not null
	AND NOT EXISTS (
		SELECT o2.id 
		From orders o2
		LEFT JOIN order_items oi2 ON oi2.order_id = o2.id
		LEFT JOIN nomenclature n2 ON n2.id = oi2.nomenclature_id
		WHERE o2.id = o.id
		AND n2.category = 'water' 
		AND n2.tare_volume = 'Vol19l'
	)
	AND o.delivery_date BETWEEN @start_date AND @end_date
GROUP BY rtrc.name</CommandText>
