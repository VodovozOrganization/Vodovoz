  <DataSets>
    <DataSet Name="BottleDebt">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>120</Timeout>
        <CommandText>SELECT DISTINCT
	bottles_movement_operations.delivery_point_id AS pointid,
	bottles_movement_operations.counterparty_id AS clientid,
	delivery_points.comment AS comment,
	TRIM(full_name) AS fullname,
	compiled_address,
	(SELECT
		SUM(delivered) - SUM(returned)
	FROM
		bottles_movement_operations
	WHERE
		delivery_point_id = pointid) AS debt,
	(SELECT
		(SUM(delivered) - SUM(returned))
	FROM
		bottles_movement_operations
	WHERE
		counterparty_id = clientid) AS debtsum,
	IF(bottles_residue IS NOT NULL, 1, 0) AS enteredresidue,
	(SELECT 
		MAX(delivery_date)
	FROM
		orders
	LEFT JOIN
		order_items ON order_id = orders.id
	LEFT JOIN
		nomenclature ON nomenclature.id = order_items.nomenclature_id
	WHERE
		orders.delivery_point_id = pointid
		AND order_status &lt;&gt; ('Canceled' OR 'DeliveryCanceled')
		AND category = 'water') AS lastorder
FROM
	bottles_movement_operations
LEFT JOIN
	counterparty ON counterparty_id = counterparty.id
LEFT JOIN
	delivery_points ON delivery_point_id = delivery_points.id
LEFT JOIN
	doc_residue ON bottles_movement_operations.delivery_point_id = doc_residue.delivery_point_id
HAVING
	debt &lt;&gt; 0 
	AND ((@allshow = 0 
		AND enteredresidue= 1
		AND @withresidue =1)
	OR (@allshow = 0
		AND enteredresidue= 0
		AND @withresidue =0)
	OR (@allshow = 1 
		AND @withresidue = -1))
ORDER BY
	debtsum DESC,
	bottles_movement_operations.counterparty_id ASC,
	debt DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="allshow">
            <Value>={?allshow}</Value>
          </QueryParameter>
          <QueryParameter Name="withresidue">
            <Value>={?withresidue}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
