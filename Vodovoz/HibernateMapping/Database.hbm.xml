	 	</id>
	 	<property name="Name" column="name" />
 	</class>

 	<class name="NomenclaturePrice" lazy="false" table="nomenclature_price">
		<id name="Id" column="id">
			<generator class="native"></generator>
	 	</id>
	 	<property name="MinCount" column="min_count" />
