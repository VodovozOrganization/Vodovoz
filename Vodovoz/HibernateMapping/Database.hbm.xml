    		<key column="org_id"/>
    		<one-to-many class="QSBanks.Account" />
		</bag>
	 	<bag name="Phones" inverse="false" cascade="all-delete-orphan" lazy="true" >
    		<key column="org_id"/>
    		<one-to-many class="QSContacts.Phone" />
		</bag>
 	</class>

 	<class name="PaidRentEquipment" lazy="false" table="paid_rent_equipment">
