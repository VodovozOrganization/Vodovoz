 		</id>
		<property name="Name" column="name" />
	</class>

	<class name="EquipmentColors" lazy="false" table="equipment_colors">
		<id name="Id" column="id">
			<generator class="native"></generator>
	 	</id>
	 	<property name="Name" column="name" />
 	</class>

	<class name="EquipmentType" lazy="false" table="equipment_type">
		<id name="Id" column="id">
