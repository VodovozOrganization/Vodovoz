			}
		],
		"variables": {
			"var_logdir": "C:\\inetpub\\logs\\Development\\DriverApi-Test-02"
		},
		"targets": {
			"async": true,
			"logconsole": {
				"type": "Console"
			},
			"logfile": {
				"type": "File",
				"fileName": "${var_logdir}\\Api.log",
				"archiveFileName": "${var_logdir}\\Api.{#}.log",
				"archiveEvery": "Day",
				"archiveNumbering": "Rolling",
				"maxArchiveFiles": 3,
				"layout": "${longdate}|${level}|${logger}|${message} ${onexception:${newline}${exception:format=tostring}}"
			},
			"requestsLog": {
				"type": "File",
				"fileName": "${var_logdir}\\requests.log",
				"archiveFileName": "${var_logdir}\\requests.{#}.log",
				"archiveEvery": "Day",
				"archiveNumbering": "Rolling",
				"maxArchiveFiles": 3,
				"layout": "${longdate}|${level}|${message} ${onexception:${newline}${exception:format=tostring}}"
			},
			"advancedLog": {
				"type": "File",
				"fileName": "${var_logdir}\\advanced.log",
				"archiveFileName": "${var_logdir}\\advanced.{#}.log",
				"archiveEvery": "Day",
				"archiveNumbering": "Rolling",
				"maxArchiveFiles": 3,
				"layout": "${longdate}|${level}|${message} ${onexception:${newline}${exception:format=tostring}}"
			},
			"console": {
				"type": "ColoredConsole",
				"layout": "${longdate} ${uppercase:${level}} ${message}",
				"wordHighlightingRules": [
					{
						"text": "DEBUG",
						"foregroundColor": "DarkGreen"
					},
					{
						"text": "WARNING",
						"foregroundColor": "Yellow"
					},
					{
						"text": "ERROR",
						"foregroundColor": "Red"
					},
					{
						"text": "FATAL",
						"foregroundColor": "Red"
					}
				]
			}
		},
		"rules": [
			{
				"logger": "*",
				"minLevel": "Info",
				"writeTo": "seq"
			},
			{
				"logger": "Microsoft.*",
				"minLevel": "Trace",
				"final": true
			},
			{
				"logger": "DriverAPI.Middleware.RequestResponseLoggingMiddleware",
				"minLevel": "Trace",
				"writeTo": "requestsLog",
				"final": true
			},
			{
