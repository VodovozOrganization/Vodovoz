				"final": true
			},
			{
				"logger": "DriverAPI.Controllers.RegistrationsController",
				"minLevel": "Trace",
				"writeTo": "advancedLog",
				"final": true
			},
			{
				"logger": "DriverAPI.Controllers.V1.TokenController",
				"minLevel": "Trace",
				"writeTo": "advancedLog",
				"final": true
			},
			{
				"logger": "QS.Project.DB.LoggedMySqlClientDriver",
				"maxLevel": "Debug",
				"final": true
			},
			{
				"logger": "Vodovoz.Tools.LoggedMySqlClientDriver",
				"maxLevel": "Debug",
				"final": true
			},
			{
				"logger": "Vodovoz.Tools.LoggedMySqlClientDriver",
				"minLevel": "Error",
				"writeTo": "logfile",
				"final": true
			},
			{
				"logger": "*",
				"minLevel": "Debug",
				"writeTo": "logfile"
			},
			{
				"logger": "*",
				"minLevel": "Trace",
				"writeTo": "console"
			}
		]
