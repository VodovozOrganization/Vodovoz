<Project>
	<PropertyGroup>

		<Configuration>Release</Configuration>
		<Platform>Any CPU</Platform>
		<PublishProvider>FileSystem</PublishProvider>
		<PublishSingleFile>false</PublishSingleFile>
		<DeleteExistingFiles>false</DeleteExistingFiles>
		<ExcludeApp_Data>false</ExcludeApp_Data>
		<_TargetId>Folder</_TargetId>
		<TargetFramework>net5.0</TargetFramework>
		<SelfContained>false</SelfContained>

		<!-- В CI/CD настроена доставка основанная на этом пути -->
		<!-- Не менять без причины и согласования -->
		<PublishDir>bin\Release\net5.0\publish\</PublishDir>

		<!-- Этот параметр необходим для запуска в докер -->
		<!--<RuntimeIdentifier>linux-x64</RuntimeIdentifier>-->

		<!-- PublishUrl не используем -->
		<!-- Для Web и Worker проектов при публикации в каталог это поле игнорируется-->
		<!-- и публикация происходит всегда по пути bin\Release\net5.0\linux-x64\publish-->
		<!--<PublishUrl>bin\Release\net5.0\publish\</PublishUrl>-->
		
	</PropertyGroup>
</Project>
