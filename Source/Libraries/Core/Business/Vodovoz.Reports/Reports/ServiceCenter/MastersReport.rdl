  <DataSets>
    <DataSet Name="data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    route_lists.date,
    CONCAT(employees.last_name, ' ', employees.name) AS driver,
    wages_movement_operations.money AS wage_from_operations,
    cash_expense.money AS expense_advance,
    cash_expense.description AS expense_advance_desc,
    entries.*
FROM
    route_lists
        RIGHT JOIN
    (
        SELECT
            route_list_addresses.route_list_id AS rl_id,
            counterparty.name AS entry_name,
            delivery_points.compiled_address_short AS entry_address,
            IF(order_items.actual_count = 0 OR order_items.actual_count IS NULL,
               0,
               ROUND((order_items.price * order_items.actual_count - order_items.discount_money) / order_items.actual_count, 2)
                ) as entry_price,
            IFNULL(FORMAT(order_items.actual_count, mu.digits), 0) AS entry_count,
            orders.payment_type AS entry_payment_type,
            IF(order_items.actual_count = 0 OR order_items.actual_count IS NULL,
               0,
               ROUND(order_items.price * order_items.actual_count - order_items.discount_money, 2)
                ) as entry_sum,
            IF(order_items.actual_count = 0 OR order_items.actual_count IS NULL,
               0,
               ROUND((order_items.price * order_items.actual_count - order_items.discount_money) * nomenclature.percent_for_master * 0.01, 2)
                ) as entry_wage,
            IF(order_items.actual_count = 0 OR order_items.actual_count IS NULL,
               0,
               ROUND((order_items.price * order_items.actual_count - order_items.discount_money), 2) * (1 - nomenclature.percent_for_master * 0.01)
                ) as entry_profit,
            IFNULL(nomenclature.short_name, nomenclature.name) AS entry_nomenclatures
        FROM
            route_list_addresses
                LEFT JOIN
            orders ON orders.id = route_list_addresses.order_id
                LEFT JOIN
            counterparty ON counterparty.id = orders.client_id
                LEFT JOIN
            delivery_points ON delivery_points.id = orders.delivery_point_id
                LEFT JOIN
            order_items ON order_items.order_id = orders.id
                LEFT JOIN
            nomenclature ON nomenclature.id = order_items.nomenclature_id
                LEFT JOIN 
            measurement_units mu ON nomenclature.unit_id = mu.id
                LEFT JOIN
            order_equipment ON order_equipment.order_id = orders.id
        WHERE
            NOT(
                #Исключатся недовозы и отмены 
                    (orders.order_status IN ('NotDelivered', 'Canceled', 'DeliveryCanceled'))
                    OR
                    #Частичные недовозы
                    (
                                route_list_addresses.status = 'Completed'
                            AND
                                (
                                            nomenclature.category in ('master', 'service')
                                        AND nomenclature.serial = false
                                        AND order_items.count != IFNULL(order_items.actual_count, 0)
                                    )
                        )
                    OR
                    route_list_addresses.status = 'Transfered'
                )
    ) AS entries ON entries.rl_id = route_lists.id
        LEFT JOIN
    employees ON employees.id = route_lists.driver_id
        LEFT JOIN
    wages_movement_operations ON wages_movement_operations.id = route_lists.driver_wages_movement_operations_id
        LEFT JOIN
    cash_expense ON cash_expense.route_list_id = route_lists.id
WHERE
        route_lists.date &gt;= @start_date
  AND route_lists.date &lt; date_add(@end_date, INTERVAL 1 DAY)
  AND route_lists.driver_id = @driver_id
ORDER BY route_lists.date, route_lists.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>={?driver_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="driver">
          <DataField>driver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="rl_id">
          <DataField>rl_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="entry_name">
          <DataField>entry_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="entry_address">
          <DataField>entry_address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="entry_price">
          <DataField>entry_price</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="entry_count">
          <DataField>entry_count</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="entry_payment_type">
          <DataField>entry_payment_type</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="entry_sum">
          <DataField>entry_sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="entry_wage">
          <DataField>entry_wage</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="entry_profit">
          <DataField>entry_profit</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="expense_advance">
          <DataField>expense_advance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="entry_nomenclatures">
          <DataField>entry_nomenclatures</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="wage_from_operations">
          <DataField>wage_from_operations</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="expense_advance_desc">
          <DataField>expense_advance_desc</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="total_advance">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>30</Timeout>
        <CommandText>SELECT
	cash_expense.route_list_id,
	IFNULL(cash_expense.money, 0) AS expense_advance
FROM
	route_lists
LEFT JOIN
	cash_expense ON cash_expense.route_list_id = route_lists.id
WHERE
	route_lists.date &gt;= @start_date
	AND route_lists.date &lt; date_add(@end_date, INTERVAL 1 DAY)
	AND route_lists.driver_id = @driver_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>={?driver_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="route_list_id">
          <DataField>route_list_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="expense_advance">
          <DataField>expense_advance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="total_premiums">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select IFNULL(SUM(wmo.money), 0) as total_premiums
from wages_movement_operations wmo 
where wmo.operation_type = 'PremiumWage'
and wmo.employee_id = @driver_id
and wmo.operation_time &gt;= @start_date
and wmo.operation_time &lt; DATE_ADD(@end_date, INTERVAL 1 DAY);</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>={?driver_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="total_premiums">
          <DataField>total_premiums</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="total_fines">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT IFNULL(ABS(SUM(wmo.money)), 0) as total_fines
from wages_movement_operations wmo 
where wmo.operation_type = 'HoldedFine'
and wmo.employee_id = @driver_id
and wmo.operation_time &gt;= @start_date
and wmo.operation_time &lt; DATE_ADD(@end_date, INTERVAL 1 DAY);</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>={?driver_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="total_fines">
          <DataField>total_fines</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
