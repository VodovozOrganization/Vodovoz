    <DataSet Name="WriteOffs">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	pwo.`date` AS `date`,
	pwo.payment_number AS payment_number,
	pwo.reason AS reason,
	pwo.comment AS comment,
	cmo.expense AS expense_sum,
	fcg.title AS financial_expense_category_name,
	o.name AS organization_name
FROM payment_write_offs pwo
INNER JOIN cashless_movement_operations cmo ON cmo.id = pwo.cashless_movement_operation_id 
INNER JOIN organizations o ON o.id = pwo.organization_id 
INNER JOIN financial_categories_groups fcg ON pwo.financial_expense_category_id = fcg.id
LEFT JOIN counterparty c ON c.id = pwo.counterparty_id
LEFT JOIN counterparty_camefrom ccf ON ccf.id = c.counterparty_camefrom_id
WHERE pwo.counterparty_id = @counterparty_id
  AND (
                  (0 IN (@Organization_include) OR (o.id IN (@Organization_include)))
            AND (NOT (o.id IN (@Organization_exclude)) OR o.id IS NULL)
              )
  AND (@is_tender IS NULL OR (@is_tender = TRUE AND ccf.name = 'Тендер') OR (@is_tender = FALSE AND ccf.name != 'Тендер'))
  AND (@start_date = '' OR cmo.operation_time &gt;= @start_date)
  AND (@end_date = '' OR cmo.operation_time &lt;= @end_date)
ORDER BY `date` DESC;</CommandText>
