  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    0 AS income,
    pi.sum AS expense,
    p.payment_num AS payment_number,
    o.id AS order_id,
    o.delivery_date AS date,
    CASE o.order_status
        WHEN 'NewOrder' THEN 'Новый'
        WHEN 'Canceled' THEN 'Отменён'
        WHEN 'WaitForPayment' THEN 'Ожидание оплаты'
        WHEN 'Accepted' THEN 'Принят'
        WHEN 'InTravelList' THEN 'В маршрутном листе'
        WHEN 'OnLoading' THEN 'На погрузке'
        WHEN 'OnTheWay' THEN 'В пути'
        WHEN 'DeliveryCanceled' THEN 'Доставка отменена'
        WHEN 'Shipped' THEN 'Доставлен'
        WHEN 'UnloadingOnStock' THEN 'Выгрузка на складе'
        WHEN 'NotDelivered' THEN 'Недовоз'
        WHEN 'Closed' THEN 'Закрыт'
        ELSE o.order_status
        END AS doc_status,
    '' AS payment_description,
    2 AS sorting_rank_by_entities, # признак фильтрации по отчету, сначала неоплаченные/частично оплаченные заказы, затем платежи, затем полностью оплаченные заказы без платежей
    2 AS sorting_rank_payment_docs #признак фильтрации в платеже расходы идут после приходов
FROM orders o
         INNER JOIN payment_items pi ON o.id = pi.order_id
         INNER JOIN payments_from_bank_client p ON pi.payment_id = p.id
         INNER JOIN organizations org ON p.organization_id = org.id
         LEFT JOIN counterparty c on c.id = o.client_id
         LEFT JOIN counterparty_camefrom ccf ON ccf.id = c.counterparty_camefrom_id

WHERE
        o.client_id = @counterparty_id
  AND pi.payment_item_status != 'Cancelled'
  AND (
             ('0' IN (@OrderStatus_include) OR (o.order_status IN (@OrderStatus_include)))
				AND (NOT (o.order_status IN (@OrderStatus_exclude)) OR o.order_status IS NULL)
         )
  AND (
      	@is_closing_documents IS NULL 
      	OR (@is_closing_documents = FALSE AND (o.delivery_schedule_id != @closing_document_delivery_schedule_id OR o.delivery_schedule_id IS NULL)) 
      	OR (@is_closing_documents = TRUE AND o.delivery_schedule_id = @closing_document_delivery_schedule_id)
      )
  AND (
              (0 IN (@Organization_include) OR (org.id IN (@Organization_include)))
        AND (NOT (org.id IN (@Organization_exclude)) OR org.id IS NULL)
          )
  AND (@is_tender IS NULL OR (@is_tender = TRUE AND ccf.name = 'Тендер') OR (@is_tender = FALSE AND ccf.name != 'Тендер'))
  AND (@start_date = '' OR o.delivery_date &gt;= @start_date)
  AND (@end_date = '' OR o.delivery_date &lt;= @end_date)

UNION ALL

SELECT
    IFNULL(cmo.income, p.total_sum) AS income,
    0 AS expense,
    p.payment_num AS payment_number,
    '' AS order_id,
    IFNULL(cmo.operation_time, p.payment_date) AS date,
    CASE p.status
        WHEN 'undistributed' THEN 'Нераспределен'
        WHEN 'distributed' THEN 'Распределен'
        WHEN 'completed' THEN 'Завершен'
        WHEN 'Cancelled' THEN 'Отменен'
        ELSE p.status
        END AS doc_status,
    p.payment_purpose AS payment_description,
    2 AS sorting_rank_by_entities,
    1 AS sorting_rank_payment_docs
FROM payments_from_bank_client p
  LEFT JOIN cashless_movement_operations cmo ON p.cashless_movement_operation_id = cmo.id
  LEFT JOIN counterparty c on c.id = p.counterparty_id
  LEFT JOIN counterparty_camefrom ccf ON ccf.id = c.counterparty_camefrom_id
  INNER JOIN organizations org ON p.organization_id = org.id
WHERE
        p.counterparty_id = @counterparty_id
  AND p.status != 'Cancelled'
  AND (@is_tender IS NULL OR (@is_tender = TRUE AND ccf.name = 'Тендер') OR (@is_tender = FALSE AND ccf.name != 'Тендер'))
  AND (@start_date = '' OR IFNULL(cmo.operation_time, p.payment_date) &gt;= @start_date)
  AND (@end_date = '' OR IFNULL(cmo.operation_time, p.payment_date) &lt;= @end_date)
  AND (
              (0 IN (@Organization_include) OR (org.id IN (@Organization_include)))
        AND (NOT (org.id IN (@Organization_exclude)) OR org.id IS NULL)
          )

UNION ALL

SELECT
    0 AS income,
    (
        (
           SELECT round(sum(IFNULL(oi.actual_count, oi.count) * oi.price - oi.discount_money), 2)
            FROM order_items oi
            WHERE oi.order_id = o.id
        )
        -
        (
            SELECT IFNULL(sum(cmo.expense), 0)
            FROM payment_items pi
                 INNER JOIN cashless_movement_operations cmo ON pi.cashless_movement_operation_id = cmo.id
            WHERE pi.order_id = o.id
              AND cmo.cashless_movement_operation_status != 'Cancelled')
        ) AS expense,
    0 AS payment_number,
    o.id AS order_id,
    o.delivery_date AS date,
    CASE o.order_status
        WHEN 'NewOrder' THEN 'Новый'
        WHEN 'Canceled' THEN 'Отменён'
        WHEN 'WaitForPayment' THEN 'Ожидание оплаты'
        WHEN 'Accepted' THEN 'Принят'
        WHEN 'InTravelList' THEN 'В маршрутном листе'
        WHEN 'OnLoading' THEN 'На погрузке'
        WHEN 'OnTheWay' THEN 'В пути'
        WHEN 'DeliveryCanceled' THEN 'Доставка отменена'
        WHEN 'Shipped' THEN 'Доставлен'
        WHEN 'UnloadingOnStock' THEN 'Выгрузка на складе'
        WHEN 'NotDelivered' THEN 'Недовоз'
        WHEN 'Closed' THEN 'Закрыт'
        ELSE o.order_status
        END AS doc_status,
    IF((DATEDIFF(NOW(), o.delivery_date) - CAST(c.delay_days_for_buyers AS INT)) &gt; 0,
       CONCAT('Оплата по заказу просрочена на ', (DATEDIFF(NOW(), o.delivery_date) - CAST(c.delay_days_for_buyers AS INT)), ' дней'),
       '') AS payment_description,
    1 AS sorting_rank_by_entities,
    0 AS sorting_rank_payment_docs
FROM orders o
INNER JOIN counterparty c on c.id = o.client_id 
INNER JOIN counterparty_contract cc ON o.counterparty_contract_id = cc.id
INNER JOIN organizations org ON cc.organization_id = org.id
LEFT JOIN counterparty_camefrom ccf ON ccf.id = c.counterparty_camefrom_id
WHERE
        o.client_id = @counterparty_id
  AND o.order_payment_status != 'Paid'
  AND o.payment_type = 'Cashless'
  AND (
             ('0' IN (@OrderStatus_include) OR (o.order_status IN (@OrderStatus_include)))
				AND (NOT (o.order_status IN (@OrderStatus_exclude)) OR o.order_status IS NULL)
         )
  AND (
              (0 IN (@Organization_include) OR (org.id IN (@Organization_include)))
        AND (NOT (org.id IN (@Organization_exclude)) OR org.id IS NULL)
          )
  AND (
      	@is_closing_documents IS NULL 
      	OR (@is_closing_documents = FALSE AND (o.delivery_schedule_id != @closing_document_delivery_schedule_id OR o.delivery_schedule_id IS NULL)) 
      	OR (@is_closing_documents = TRUE AND o.delivery_schedule_id = @closing_document_delivery_schedule_id)
      )
  AND (@is_tender IS NULL OR (@is_tender = TRUE AND ccf.name = 'Тендер') OR (@is_tender = FALSE AND ccf.name != 'Тендер'))
  AND (@start_date = '' OR o.delivery_date &gt;= @start_date)
  AND (@end_date = '' OR o.delivery_date &lt;= @end_date)
HAVING expense &gt; 0

UNION ALL

SELECT
    0 AS income,
    (
        SELECT round(sum(IFNULL(oi.actual_count, oi.count) * oi.price - oi.discount_money), 2)
        FROM order_items oi
        WHERE oi.order_id = o.id) AS expense,
    0 AS payment_number,
    o.id AS order_id,
    o.delivery_date AS date,
    CASE o.order_status
        WHEN 'NewOrder' THEN 'Новый'
        WHEN 'Canceled' THEN 'Отменён'
        WHEN 'WaitForPayment' THEN 'Ожидание оплаты'
        WHEN 'Accepted' THEN 'Принят'
        WHEN 'InTravelList' THEN 'В маршрутном листе'
        WHEN 'OnLoading' THEN 'На погрузке'
        WHEN 'OnTheWay' THEN 'В пути'
        WHEN 'DeliveryCanceled' THEN 'Доставка отменена'
        WHEN 'Shipped' THEN 'Доставлен'
        WHEN 'UnloadingOnStock' THEN 'Выгрузка на складе'
        WHEN 'NotDelivered' THEN 'Недовоз'
        WHEN 'Closed' THEN 'Закрыт'
        ELSE o.order_status
        END AS doc_status,
    '' AS payment_description,
    3 AS sorting_rank_by_entities,
    0 AS sorting_rank_payment_docs
FROM orders o
INNER JOIN counterparty c on c.id = o.client_id 
INNER JOIN counterparty_contract cc ON o.counterparty_contract_id = cc.id
INNER JOIN organizations org ON cc.organization_id = org.id
LEFT JOIN counterparty_camefrom ccf ON ccf.id = c.counterparty_camefrom_id
WHERE
        o.client_id = @counterparty_id
  AND o.order_payment_status = 'Paid'
  AND o.payment_type = 'Cashless'
  AND (
             ('0' IN (@OrderStatus_include) OR (o.order_status IN (@OrderStatus_include)))
				AND (NOT (o.order_status IN (@OrderStatus_exclude)) OR o.order_status IS NULL)
         )
  AND (
              (0 IN (@Organization_include) OR (org.id IN (@Organization_include)))
        AND (NOT (org.id IN (@Organization_exclude)) OR org.id IS NULL)
          )
  AND (
      	@is_closing_documents IS NULL 
      	OR (@is_closing_documents = FALSE AND (o.delivery_schedule_id != @closing_document_delivery_schedule_id OR o.delivery_schedule_id IS NULL)) 
      	OR (@is_closing_documents = TRUE AND o.delivery_schedule_id = @closing_document_delivery_schedule_id)
      )
  AND (@is_tender IS NULL OR (@is_tender = TRUE AND ccf.name = 'Тендер') OR (@is_tender = FALSE AND ccf.name != 'Тендер'))
  AND (@start_date = '' OR o.delivery_date &gt;= @start_date)
  AND (@end_date = '' OR o.delivery_date &lt;= @end_date)
  AND NOT EXISTS (
      SELECT pi2.id
      FROM payment_items pi2 
      INNER JOIN cashless_movement_operations cmo2 on pi2.cashless_movement_operation_id = cmo2.id
      WHERE cmo2.cashless_movement_operation_status != 'Cancelled'
        AND pi2.order_id = o.id
  )
HAVING expense &gt; 0
ORDER BY date DESC;</CommandText>
