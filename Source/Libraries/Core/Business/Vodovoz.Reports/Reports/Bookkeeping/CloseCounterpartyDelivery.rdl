  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    counterparty.close_delivery_date,
    counterparty.name as client_name,
    counterparty.inn ,
    counterparty.close_delivery_comment ,
    CONCAT_WS
        (   '',
            CONCAT(employees.last_name, ' '),
            IF(employees.name = '',
                '',
                CONCAT(LEFT(employees.name, 1), '.')),
            IF(employees.patronymic = '',
                '',
                CONCAT(LEFT(employees.patronymic, 1), '.'))
        ) AS employee_name
FROM 
    counterparty
    LEFT JOIN employees ON employees.id = counterparty.close_delivery_employee_id
WHERE
    counterparty.is_deliveries_closed = true AND
    DATE(counterparty.close_delivery_date) &gt;= DATE(@start_date) AND
    DATE(counterparty.close_delivery_date) &lt;= DATE(@end_date)</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="close_delivery_date">
          <DataField>close_delivery_date</DataField>
        </Field>
        <Field Name="client_name">
          <DataField>client_name</DataField>
        </Field>
        <Field Name="inn">
          <DataField>inn</DataField>
        </Field>
        <Field Name="close_delivery_comment">
          <DataField>close_delivery_comment</DataField>
        </Field>
        <Field Name="employee_name">
          <DataField>employee_name</DataField>
        </Field>
      </Fields>
    </DataSet>
