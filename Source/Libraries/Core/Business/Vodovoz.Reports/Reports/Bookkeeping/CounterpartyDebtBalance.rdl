            <Value>={?RevenueStatus_include}</Value>
          </QueryParameter>
          <QueryParameter Name="RevenueStatus_exclude">
            <Value>={?RevenueStatus_exclude}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="inn">
          <DataField>inn</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="counterparty_name">
          <DataField>counterparty_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="org_name">
          <DataField>org_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="unallocated_balance">
          <DataField>unallocated_balance</DataField>
          <TypeName>System.Decimal</TypeName>
