  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT * FROM
    (SELECT
        c.inn AS inn,
        c.name AS counterparty_name,
        IF
        (
	        EXISTS(SELECT 1 FROM phones p WHERE p.counterparty_id = o.client_id AND p.digits_number IS NOT NULL),
	        (SELECT GROUP_CONCAT(DISTINCT CONCAT('+7', p.digits_number) SEPARATOR ',\n') FROM phones p WHERE p.counterparty_id = o.client_id ORDER BY p.id DESC),
	        GROUP_CONCAT(DISTINCT (SELECT CONCAT('+7', p.digits_number) FROM phones p WHERE p.id = o.contact_phone_id ORDER BY p.id DESC) SEPARATOR ',\n')	        
	    ) AS phones,
        org.name AS org_name,
        (
            (SELECT IFNULL(SUM(cmo.income), 0)
                FROM cashless_movement_operations cmo
                WHERE cmo.counterparty_id = c.id
                  AND cmo.cashless_movement_operation_status != 'Cancelled')
            -
            (SELECT IFNULL(SUM(pi.sum), 0)
                FROM payment_items pi
                INNER JOIN payments_from_bank_client pfbc ON pi.payment_id = pfbc.id
                WHERE pfbc.counterparty_id = c.id
                  AND pi.payment_item_status != 'Cancelled')
        ) AS unallocated_balance,
        (
            SELECT IFNULL(SUM(cmo.expense), 0)
            FROM payment_write_offs pwo
            INNER JOIN cashless_movement_operations cmo
            ON pwo.cashless_movement_operation_id = cmo.id
            WHERE pwo.counterparty_id = c.id
              AND pwo.organization_id = org.id
        )AS writeoffs_sum,
        IFNULL(
            ROUND(
                   SUM(
                       (SELECT SUM(oi.price * IFNULL(oi.actual_count, oi.count) - oi.discount_money)
                           FROM order_items oi
                           WHERE oi.order_id = o.id)),
                   2),
            0
        ) AS not_paid_orders_sum,
        IFNULL(
            SUM(
            (SELECT SUM(cmo.expense)
                FROM payment_items pi
                INNER JOIN cashless_movement_operations cmo ON pi.cashless_movement_operation_id = cmo.id
                WHERE pi.order_id = o.id
                  AND cmo.cashless_movement_operation_status != 'Cancelled')),
            0
        ) AS partial_paid_orders_sum,
        -- Просроченная дебиторская задолженность (сумма долгов с превышением отсрочки)
                IFNULL(
                    ROUND(
                        SUM(
                            CASE WHEN DATEDIFF(NOW(), o.delivery_date) &gt; c.delay_days_for_buyers THEN
                                (SELECT SUM(oi.price * IFNULL(oi.actual_count, oi.count) - oi.discount_money)
                                 FROM order_items oi
                                 WHERE oi.order_id = o.id)
                            ELSE 0 END),
                        2),
                    0
                ) AS overdue_debtor_debt,
                -- Дебиторская задолженность (непросроченная, т.е. общий долг минус просроченная)
                (IFNULL(
                    ROUND(
                           SUM(
                               (SELECT SUM(oi.price * IFNULL(oi.actual_count, oi.count) - oi.discount_money)
                                   FROM order_items oi
                                   WHERE oi.order_id = o.id)),
                           2),
                    0
                ) - (
                    (SELECT IFNULL(SUM(cmo.income), 0)
                        FROM cashless_movement_operations cmo
                        WHERE cmo.counterparty_id = c.id
                          AND cmo.cashless_movement_operation_status != 'Cancelled')
                    -
                    (SELECT IFNULL(SUM(pi.sum), 0)
                        FROM payment_items pi
                        INNER JOIN payments_from_bank_client pfbc ON pi.payment_id = pfbc.id
                        WHERE pfbc.counterparty_id = c.id
                          AND pi.payment_item_status != 'Cancelled')
                ) - IFNULL(
                    SUM(
                    (SELECT SUM(cmo.expense)
                        FROM payment_items pi
                        INNER JOIN cashless_movement_operations cmo ON pi.cashless_movement_operation_id = cmo.id
                        WHERE pi.order_id = o.id
                          AND cmo.cashless_movement_operation_status != 'Cancelled')),
                    0
                )) - 
                IFNULL(
                    ROUND(
                        SUM(
                            CASE WHEN DATEDIFF(NOW(), o.delivery_date) &gt; c.delay_days_for_buyers THEN
                                (SELECT SUM(oi.price * IFNULL(oi.actual_count, oi.count) - oi.discount_money)
                                 FROM order_items oi
                                 WHERE oi.order_id = o.id)
                            ELSE 0 END),
                        2),
                    0
                ) AS debtor_debt,
        c.delay_days_for_buyers,
        DATEDIFF(NOW(), o.delivery_date) AS max_delay_days_fact,
        CASE WHEN c.close_delivery_debt_type = 'Judicial' THEN "Судеб-ный"
              WHEN c.close_delivery_debt_type = 'WriteOff' THEN "Списание"
              WHEN c.close_delivery_debt_type = 'ShortTerm' THEN "Кратко-срочный"
         	  ELSE ""
         	  END as counterparty_debt_type,
        CASE WHEN c.revenue_status = 'Active' THEN "Нет"
        	 ELSE "Да"
        END as liquidating_info
    FROM orders o
    INNER JOIN counterparty c ON o.client_id = c.id
    LEFT JOIN counterparty_camefrom ccf ON c.counterparty_camefrom_id = ccf.id
    LEFT JOIN counterparty_contract cc ON o.counterparty_contract_id = cc.id
    LEFT JOIN organizations org ON cc.organization_id = org.id
    WHERE o.order_payment_status != 'Paid'
      AND o.payment_type = 'Cashless'
      AND c.person_type = 'legal'
      AND (@start_date = '' OR o.delivery_date &gt;= @start_date)
      AND (@end_date = '' OR o.delivery_date &lt;= @end_date)
      AND (
             (0 IN (@Counterparty_include) OR (o.client_id IN (@Counterparty_include)))
				AND (NOT (o.client_id IN (@Counterparty_exclude)) OR o.client_id IS NULL)
         )
      AND (
            (0 IN (@Organization_include) OR (org.id IN (@Organization_include)))
            AND (NOT (org.id IN (@Organization_exclude)) OR org.id IS NULL)
          )
      AND (
             (0 IN (@SalesManager_include) OR (c.sales_manager_id IN (@SalesManager_include)))
                AND (c.sales_manager_id IS NULL OR NOT (c.sales_manager_id IN (@SalesManager_exclude)))
      ) 
      AND
        (
            (
                ((('0' IN (@CounterpartyType_include))
                    AND (0 IN (@CounterpartySubtype_include)))
                OR (c.counterparty_type IN (@CounterpartyType_include)))
                AND NOT (c.counterparty_type IN (@CounterpartyType_exclude))
                AND (NOT (c.counterparty_subtype_id IN (@CounterpartySubtype_exclude)) OR c.counterparty_subtype_id IS NULL)
            )
            OR
            (
                ('0' IN (@CounterpartyType_include) OR NOT ('0' IN (@CounterpartySubtype_include)))
                AND c.counterparty_type = 'AdvertisingDepartmentClient'
                AND NOT ('AdvertisingDepartmentClient' IN (@CounterpartyType_exclude))
                AND (0 IN (@CounterpartySubtype_include) OR c.counterparty_subtype_id IN (@CounterpartySubtype_include))
                AND (NOT (c.counterparty_subtype_id IN (@CounterpartySubtype_exclude)) OR c.counterparty_subtype_id IS NULL)
            )
        )
      AND (
             ('0' IN (@OrderStatus_include) OR (o.order_status IN (@OrderStatus_include)))
				AND (NOT (o.order_status IN (@OrderStatus_exclude)) OR o.order_status IS NULL)
         )
      AND (('0' IN (@DebtType_include) OR c.close_delivery_debt_type IN (@DebtType_include))
      		AND (NOT c.close_delivery_debt_type IN (@DebtType_exclude) OR c.close_delivery_debt_type IS NULL)
      )
      AND
      (
          ('0' IN (@RevenueStatus_include) OR (c.revenue_status IN (@RevenueStatus_include)))
          AND (NOT (c.revenue_status IN (@RevenueStatus_exclude)) OR c.revenue_status IS NULL)
      )
      AND (
      	@is_closing_documents IS NULL 
      	OR (@is_closing_documents = FALSE AND (o.delivery_schedule_id != @closing_document_delivery_schedule_id OR o.delivery_schedule_id IS NULL)) 
      	OR (@is_closing_documents = TRUE AND o.delivery_schedule_id = @closing_document_delivery_schedule_id)
      )
      AND (@is_chain_stores IS NULL OR c.is_chain_store = @is_chain_stores)
      AND (
      	@is_expired IS NULL 
      	OR (@is_expired = FALSE AND DATEDIFF(NOW(), o.delivery_date) - CAST(c.delay_days_for_buyers AS INT) &lt;= 0)
      	OR (@is_expired = TRUE AND DATEDIFF(NOW(), o.delivery_date) - CAST(c.delay_days_for_buyers AS INT) &gt; 0)
      )
      AND (@is_liquidated IS NULL OR (c.revenue_status = 'Active' AND !@is_liquidated) OR (c.revenue_status != 'Active' AND @is_liquidated))
      AND (@is_tender IS NULL OR (@is_tender = TRUE AND ccf.id = GET_PARAMETER('counterparty_from_tender_id')) OR (@is_tender = FALSE AND (ccf.id IS NULL OR ccf.id != GET_PARAMETER('counterparty_from_tender_id'))))
      AND (SELECT SUM(oi.price * IFNULL(oi.actual_count, oi.count) - oi.discount_money) FROM order_items oi WHERE oi.order_id = o.id) &gt; 0
    GROUP BY org.id, c.id
    ) AS T
ORDER BY (T.not_paid_orders_sum - T.unallocated_balance - T.partial_paid_orders_sum) DESC
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="closing_document_delivery_schedule_id">
            <Value>={?closing_document_delivery_schedule_id}</Value>
          </QueryParameter>
          <QueryParameter Name="Counterparty_include">
            <Value>={?Counterparty_include}</Value>
          </QueryParameter>
          <QueryParameter Name="Counterparty_exclude">
            <Value>={?Counterparty_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="Organization_include">
            <Value>={?Organization_include}</Value>
          </QueryParameter>
          <QueryParameter Name="Organization_exclude">
            <Value>={?Organization_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="CounterpartyType_include">
            <Value>={?CounterpartyType_include}</Value>
          </QueryParameter>
          <QueryParameter Name="CounterpartyType_exclude">
            <Value>={?CounterpartyType_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="SalesManager_include">
            <Value>=Parameters!SalesManager_include</Value>
          </QueryParameter>
          <QueryParameter Name="SalesManager_exclude">
            <Value>=Parameters!SalesManager_exclude</Value>
          </QueryParameter>
          <QueryParameter Name="CounterpartySubtype_include">
            <Value>={?CounterpartySubtype_include}</Value>
          </QueryParameter>
          <QueryParameter Name="CounterpartySubtype_exclude">
            <Value>={?CounterpartySubtype_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="OrderStatus_include">
            <Value>={?OrderStatus_include}</Value>
          </QueryParameter>
          <QueryParameter Name="OrderStatus_exclude">
            <Value>={?OrderStatus_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="DebtType_include">
            <Value>={?DebtType_include}</Value>
          </QueryParameter>
          <QueryParameter Name="DebtType_exclude">
            <Value>={?DebtType_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="is_closing_documents">
            <Value>={?is_closing_documents}</Value>
          </QueryParameter>
          <QueryParameter Name="is_chain_stores">
            <Value>={?is_chain_stores}</Value>
          </QueryParameter>
          <QueryParameter Name="is_expired">
            <Value>={?is_expired}</Value>
          </QueryParameter>
          <QueryParameter Name="is_liquidated">
            <Value>={?is_liquidated}</Value>
          </QueryParameter>
          <QueryParameter Name="is_tender">
            <Value>={?is_tender}</Value>
