  <DataSets>
    <DataSet Name="Goods">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>30</Timeout>
        <CommandText>SELECT 
	IFNULL(special_nomenclature.special_id, nomenclature.id) AS id,
	nomenclature.official_name as name,
	measurement_units.okei AS unit_code,
	measurement_units.name AS mu_name,
	IFNULL(actual_count, count) AS count,
	order_items.price AS item_price,
	IFNULL(order_items.include_nds, 0) as include_nds,
	order_items.discount AS item_discount,
	order_items.discount_money AS item_discount_money,
    CASE
        WHEN organizations.is_usn_mode = 1 THEN
            COALESCE(vr_organization.vat_rate_value, 0)
        ELSE
            COALESCE(vr_nomenclature.vat_rate_value, 0)
    END / 100.0 + 1 AS vat
FROM
    order_items
        LEFT JOIN
    nomenclature ON order_items.nomenclature_id = nomenclature.id
        LEFT JOIN
    measurement_units ON nomenclature.unit_id = measurement_units.id
        LEFT JOIN
    orders ON orders.id = order_items.order_id
        LEFT JOIN
    counterparty_contract ON orders.counterparty_contract_id = counterparty_contract.id
        LEFT JOIN
    organizations ON organizations.id = counterparty_contract.organization_id
        LEFT JOIN
    special_nomenclature ON special_nomenclature.counterparty_id = orders.client_id AND special_nomenclature.nomenclature_id = nomenclature.id
        LEFT JOIN
    vat_rate_versions vrv_nomenclature ON vrv_nomenclature.nomenclature_id = nomenclature.id 
    AND ((vrv_nomenclature.start_date &lt;= orders.bill_date AND vrv_nomenclature.end_date IS NULL )
     OR (vrv_nomenclature.start_date &lt;= orders.bill_date AND vrv_nomenclature.end_date &gt;= orders.bill_date))
        LEFT JOIN
    vat_rates vr_nomenclature ON vr_nomenclature.id = vrv_nomenclature.vat_rate_id
        LEFT JOIN
    vat_rate_versions vrv_organization ON vrv_organization.organization_id = organizations.id 
        AND ((vrv_organization.start_date &lt;= orders.bill_date AND vrv_organization.end_date IS NULL )
         OR (vrv_organization.start_date &lt;= orders.bill_date AND vrv_organization.end_date &gt;= orders.bill_date))
        LEFT JOIN
    vat_rates vr_organization ON vr_organization.id = vrv_organization.vat_rate_id
WHERE
    order_items.order_id = @order_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
