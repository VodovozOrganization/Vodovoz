  <DataSets>
    <DataSet Name="FuelTransferDocument">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	fuel_transfer_documents.id as fuel_transfer_document_id,
    fuel_transfer_documents.creation_time as fuel_transfer_document_date,
    fuel_transfer_documents.transfered_liters as transfered_liters,
    CONCAT_WS('', CONCAT(authors.last_name, ' '), if(authors.name = '', '', CONCAT(LEFT(authors.name, 1),'.')), if(authors.patronymic = '', '', CONCAT(LEFT(authors.patronymic, 1),'.'))) as author_fio,
    CONCAT_WS('', CONCAT(drivers.last_name, ' '), if(drivers.name = '', '', CONCAT(LEFT(drivers.name, 1),'.')), if(drivers.patronymic = '', '', CONCAT(LEFT(drivers.patronymic, 1),'.'))) as driver_fio,
	CONCAT_WS('', CONCAT(senders.last_name, ' '), if(senders.name = '', '', CONCAT(LEFT(senders.name, 1),'.')), if(senders.patronymic = '', '', CONCAT(LEFT(senders.patronymic, 1),'.'))) as sender_fio,
    CONCAT_WS('', CONCAT(receivers.last_name, ' '), if(receivers.name = '', '', CONCAT(LEFT(receivers.name, 1),'.')), if(receivers.patronymic = '', '', CONCAT(LEFT(receivers.patronymic, 1),'.'))) as receiver_fio,
    subdivisions_from.name as subdivision_from,
    subdivisions_to.name as subdivision_to,
    fuel_types.name as fuel_name,
    CONCAT(cm.name, ' (', cars.reg_number, ')') as car_title
FROM
	fuel_transfer_documents
		LEFT JOIN
	employees as drivers ON drivers.id = fuel_transfer_documents.driver_id
    	LEFT JOIN
	employees as authors ON authors.id = fuel_transfer_documents.author_id
    	LEFT JOIN
	employees as senders ON senders.id = fuel_transfer_documents.cashier_sender_id
    	LEFT JOIN
	employees as receivers ON receivers.id = fuel_transfer_documents.cashier_receiver_id    
    	LEFT JOIN
	subdivisions as subdivisions_from ON subdivisions_from.id = fuel_transfer_documents.cash_subdivision_from_id
    	LEFT JOIN
	subdivisions as subdivisions_to ON subdivisions_to.id = fuel_transfer_documents.cash_subdivision_to_id
		LEFT JOIN 
	fuel_types ON fuel_types.id = fuel_transfer_documents.fuel_type_id
    	LEFT JOIN
	cars ON cars.id = fuel_transfer_documents.car_id
        LEFT JOIN
    car_models cm ON cars.model_id = cm.id
WHERE
	fuel_transfer_documents.id = @transfer_document_id
GROUP BY fuel_transfer_documents.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="transfer_document_id">
            <Value>=Parameters!transfer_document_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
