                    Cel = Cel &amp; "рубля "
                Case Is = "3"
                    Cel = Cel &amp; "рубля "
                Case Is = "4"
                    Cel = Cel &amp; "рубля "
                Case Else
                    Cel = Cel &amp; "рублей "
            End Select
