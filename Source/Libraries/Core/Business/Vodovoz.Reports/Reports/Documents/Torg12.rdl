                Cel = Cel &amp; "рублей "
        End Select
 
        'Копейки
        If Val(Mid$(Summa, 14, 1)) &lt;&gt; 1 Then
            Select Case Right$(Summa, 1)
                Case Is = "0"
                    KStr = KStr &amp; "копеек"
                Case Is = "1"
                    KStr = KStr &amp; "копейка"
