    <DataSet Name="Order">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    bwsfap.id AS bwsfap_id,
    DATE_FORMAT(bwsfap.create_date,'%d.%m.%Y') AS bill_date,
    counterparty.full_name AS client_name,
    counterparty.jur_address AS client_address,
    counterparty.inn,
    counterparty.kpp,
    counterparty.use_special_doc_fields,
    counterparty.special_cargo_receiver,
    counterparty.special_customer,
    IF(!IS_NULL_OR_WHITESPACE(counterparty.special_contract_number) and counterparty.special_contract_date is not null,
       CONCAT(counterparty.special_contract_name, ' №', counterparty.special_contract_number, ' от ', DATE_FORMAT(counterparty.special_contract_date, '%d.%m.%y')),
       counterparty.special_contract_name) AS special_contract_number,
    counterparty.payer_special_kpp,
    counterparty.special_gov_contract,
    counterparty.special_delivery_address,
    GROUP_CONCAT(DISTINCT
            CONCAT(
                    '+7 ',
                    counterparty_phones.number,
                    IF(
                            counterparty_phones.comment IS NULL,
                            '',
                            CONCAT(' ', counterparty_phones.comment)
                        )
                )
            SEPARATOR '; '
        ) AS client_contact,
    CONCAT(employees.last_name, ' ', employees.name, ' ', employees.patronymic) AS author,
    (
        select NUMBER_TO_STRING_DESCRIPTION(IFNULL(sum(ROUND(bwsfapi.count * bwsfapi.price - bwsfapi.discount_money, 2)), 0))
        from bill_without_shipment_for_advance_payment_items bwsfapi
        where bwsfapi.bill_ws_for_advance_payment_id = bwsfap.id
    ) AS bill_sum_string
FROM
    bills_without_shipment_for_advance_payment bwsfap
        LEFT JOIN
    counterparty ON bwsfap.client_id = counterparty.id
        LEFT JOIN
    phones as counterparty_phones ON counterparty_phones.counterparty_id = counterparty.id AND counterparty_phones.is_archive = 0
        LEFT JOIN
    employees ON employees.id = bwsfap.author_id
WHERE
        bwsfap.id = @bill_ws_for_advance_payment_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="bill_ws_for_advance_payment_id">
            <Value>=Parameters!bill_ws_for_advance_payment_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
