    <DataSet Name="Goods">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>30</Timeout>
        <CommandText>SELECT
    nomenclature.id AS id,
    nomenclature.official_name AS name,
    '--' AS unit_code,
    measurement_units.name AS mu_name,
    measurement_units.digits AS digits,
    bwsfapi.count AS count,
    bwsfapi.price AS item_price,
    IFNULL(bwsfapi.include_nds, 0) as include_nds,
    bwsfapi.discount,
    bwsfapi.discount_money,
    bwsfapi.price * count AS sum,
    bwsfapi.price * count - bwsfapi.discount_money AS total
FROM
    bill_without_shipment_for_advance_payment_items bwsfapi
        LEFT JOIN
    nomenclature ON bwsfapi.nomenclature_id = nomenclature.id
        LEFT JOIN
    measurement_units ON nomenclature.unit_id = measurement_units.id
WHERE
        bwsfapi.bill_ws_for_advance_payment_id = @bill_ws_for_advance_payment_id;</CommandText>
