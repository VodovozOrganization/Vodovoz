    <DataSet Name="Signatures">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	(SELECT binary_file FROM stored_resource WHERE id = 
	(SELECT 
	ov.signature_leader_id
FROM
	organizations
LEFT JOIN
    (SELECT ov.* FROM organization_versions ov
   	    INNER JOIN
            bills_without_shipment_for_advance_payment bill
        ON
            bill.id = @bill_ws_for_advance_payment_id
        AND
            ov.start_date &lt;= bill.create_date
        AND
            (ov.end_date is null OR ov.end_date &gt;= bill.create_date)
    ) AS ov ON organizations.id = ov.organization_id 
WHERE
    organizations.id = @organization_id)) as chief,
	(SELECT binary_file FROM stored_resource WHERE id = 
	(SELECT 
	ov.signature_accountant_id
FROM
	organizations
LEFT JOIN
    (SELECT ov.* FROM organization_versions ov
   	    INNER JOIN
            bills_without_shipment_for_advance_payment bill
        ON
            bill.id = @bill_ws_for_advance_payment_id
        AND
            ov.start_date &lt;= bill.create_date
        AND
            (ov.end_date is null OR ov.end_date &gt;= bill.create_date)
    ) AS ov ON organizations.id = ov.organization_id 
WHERE
    organizations.id = @organization_id)) as bookkeeper
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="organization_id">
            <Value>={?organization_id}</Value>
          </QueryParameter>
          <QueryParameter Name="bill_ws_for_advance_payment_id">
            <Value>={?bill_ws_for_advance_payment_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
