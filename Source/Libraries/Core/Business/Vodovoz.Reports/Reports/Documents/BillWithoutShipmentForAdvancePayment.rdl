    <DataSet Name="Organization">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    organizations.full_name,
    organizations.name as org_name,
    organizations.INN,
    organizations.KPP,
    accounts.`number` as account,
    ov.jur_address AS address,
    organizations.email AS email,
    CONCAT('+7 ', phones.number) AS phone,
    banks.name,
    banks.bik,
    cor_account.cor_account_number as cor_account,
    banks.city,
    accounts.number,
    leader.last_name AS leader_last_name,
    leader.name AS leader_name,
    leader.patronymic AS leader_patronymic,
    buh.last_name AS buh_last_name,
    buh.name AS buh_name,
    buh.patronymic AS buh_patronymic
FROM
    organizations
        LEFT JOIN
    (SELECT ov.*, bill.client_id as counterparty_id FROM organization_versions ov
   	    INNER JOIN
            bills_without_shipment_for_advance_payment bill
        ON
            bill.id = @bill_ws_for_advance_payment_id
        AND
            ov.start_date &lt;= bill.create_date
        AND
            (ov.end_date is null OR ov.end_date &gt;= bill.create_date)
    ) AS ov ON organizations.id = ov.organization_id
        LEFT JOIN
    counterparty ON ov.counterparty_id = counterparty.id
        LEFT JOIN
    accounts ON
    		(((NOT counterparty.use_special_doc_fields OR counterparty.our_organization_account_for_bills is NULL) and accounts.id = organizations.default_account_id) 
    		or (counterparty.use_special_doc_fields and accounts.id = counterparty.our_organization_account_for_bills))
    	LEFT JOIN
    banks_cor_accounts cor_account ON accounts.bank_cor_account_id = cor_account.id 
        LEFT JOIN
    banks ON banks.id = accounts.bank_id
        LEFT JOIN
    employees leader ON leader.id = ov.leader_id
        LEFT JOIN
    employees buh ON buh.id = ov.accountant_id 
        LEFT JOIN
    phones ON organizations.id = phones.org_id AND phones.is_archive = 0
WHERE
    organizations.id = @organization_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="organization_id">
            <Value>={?organization_id}</Value>
