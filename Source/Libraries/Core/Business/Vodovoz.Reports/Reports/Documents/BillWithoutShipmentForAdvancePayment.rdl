    <DataSet Name="Stamp">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT sr.binary_file as image
FROM organizations org
LEFT JOIN stored_resource sr ON org.stamp_id = sr.id
WHERE org.id = @organization_id</CommandText>
