    <DataSet Name="Order">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    orders.delivery_date AS order_date,
    orders.counterparty_external_order_id,
    counterparty_contract.number AS contract_number,
    DATE_FORMAT(counterparty_contract.issue_date,
                '%d.%m.%y') AS contract_date,
    counterparty.full_name AS client_name,
    counterparty.jur_address AS client_address,
    counterparty.inn AS client_inn,
    counterparty.kpp AS client_kpp,
    counterparty.use_special_doc_fields,
    counterparty.special_cargo_receiver,
    counterparty.special_customer,
    (
        IF(orders.order_payment_status = 'Paid',
            (
                with recursive PaymentsHierarchy AS (
                    select pfbc.id, pfbc.payment_num, pfbc.payment_date, pfbc.refunded_payment_id
                    from payments_from_bank_client pfbc
                             join payment_items pi on pfbc.id = pi.payment_id
                             join orders o on pi.order_id = o.id
                    where o.id = @order_id
                      and pi.payment_item_status != 'Cancelled'
                      and pfbc.payment_date &lt; ADDDATE(o.delivery_date, INTERVAL 1 DAY)

                    UNION ALL

                    select pfbc.id, pfbc.payment_num, pfbc.payment_date, pfbc.refunded_payment_id
                    from payments_from_bank_client pfbc, PaymentsHierarchy as a
                    where pfbc.Id = a.refunded_payment_id
                )
                select GROUP_CONCAT(CONCAT("№ ", payment_num, " от ", DATE_FORMAT(payment_date, '%d.%m.%Y')) SEPARATOR ", ") from PaymentsHierarchy
                where refunded_payment_id is null
            ),
            "")
        ) AS payments_info,
    IF(!IS_NULL_OR_WHITESPACE(counterparty.special_contract_number) and counterparty.special_contract_date is not null,
       CONCAT(counterparty.special_contract_name, ' №', counterparty.special_contract_number, ' от ', DATE_FORMAT(counterparty.special_contract_date, '%d.%m.%y')),
       counterparty.special_contract_name) AS special_contract_number,
    counterparty.payer_special_kpp,
    counterparty.special_gov_contract,
    counterparty.special_delivery_address,
    CONCAT(
            IFNULL(counterparty.inn, ''),
            '/',
            IFNULL(IF(counterparty.use_special_doc_fields AND @special_fields AND !IS_NULL_OR_WHITESPACE(counterparty.payer_special_kpp),
                      counterparty.payer_special_kpp, counterparty.kpp), '')
    ) AS inn_kpp,
    CASE
        WHEN counterparty.cargo_receiver_source = 'FromDeliveryPoint' THEN CONCAT(counterparty.full_name, ', ', counterparty.inn, '/',
                                                                                  IFNULL(delivery_points.KPP, counterparty.kpp), ', ',
                                                                                  COMPILE_ADDRESS(delivery_points.id))
        WHEN counterparty.cargo_receiver_source = 'Special' AND !IS_NULL_OR_WHITESPACE(counterparty.special_cargo_receiver)
            AND counterparty.use_special_doc_fields AND @special_fields THEN counterparty.special_cargo_receiver
        ELSE CONCAT(counterparty.full_name, ', ', counterparty.jur_address) END AS cargo_receiver,
    organizations.full_name AS org_name,
    ov.jur_address AS org_address,
    delivery_points.compiled_address_short AS del_point_address,
    CONCAT_WS(
            ', ',
            delivery_points.city,
            delivery_points.street,
            delivery_points.building
    ) AS del_point_address_composite,
    organizations.INN AS org_inn,
    organizations.KPP AS org_kpp,
    buhgalter.last_name AS buh_last_name,
    LEFT(buhgalter.name, 1) AS buh_name,
    LEFT(buhgalter.patronymic, 1) AS buh_patronymic,
    leader.last_name AS leader_last_name,
    LEFT(leader.name, 1) AS leader_name,
    LEFT(leader.patronymic, 1) AS leader_patronymic,
    IF(
            orders.order_status = 'Closed',
            MAX(route_list_addresses.bottles_returned),
            '    '
    ) AS bottles_in_fact,
    orders.self_delivery AS is_self_delivery,
    counterparty.inn,
    counterparty.kpp
FROM orders
         LEFT JOIN
     counterparty ON orders.client_id = counterparty.id
         LEFT JOIN
     route_list_addresses ON route_list_addresses.order_id = orders.id AND route_list_addresses.status = 'Completed'
         LEFT JOIN
     counterparty_contract ON orders.counterparty_contract_id = counterparty_contract.id
         LEFT JOIN
     organizations ON organizations.id = counterparty_contract.organization_id
         LEFT JOIN
     organization_versions ov
     ON
         organizations.id = ov.organization_id
             AND
         ov.start_date &lt;= orders.bill_date
             AND
         (ov.end_date is null OR ov.end_date &gt;= orders.bill_date)
         LEFT JOIN
     delivery_points ON orders.delivery_point_id = delivery_points.id
         LEFT JOIN
     employees leader ON leader.id = ov.leader_id
         LEFT JOIN
     employees buhgalter ON buhgalter.id = ov.accountant_id
WHERE
    orders.id = @order_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id</Value>
          </QueryParameter>
          <QueryParameter Name="special_fields">
            <Value>={?special}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
