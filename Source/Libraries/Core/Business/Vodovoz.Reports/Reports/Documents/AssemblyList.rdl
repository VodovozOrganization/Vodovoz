  <DataSets>
    <DataSet Name="Nomenclature">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    nomenclature.official_name AS name,
    nomenclature.id AS nomenclature_id,
    mu.digits AS digits,
    IFNULL(order_items.actual_count, order_items.count) AS items_count,
    nomenclature.storage_cell,
    order_items.price - (order_items.discount_money / IFNULL(order_items.actual_count,order_items.count)) AS price,
    GET_TOTAL_SUM_FOR_ORDER_ITEM(order_items.id) AS total_item_price
FROM order_items
    LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
    LEFT JOIN measurement_units mu ON nomenclature.unit_id = mu.id    
WHERE order_items.order_id = @order_id
  AND nomenclature.online_store_id IS NOT NULL
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>={?order_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature_id">
          <DataField>nomenclature_id</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="items_count">
          <DataField>items_count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="storage_cell">
          <DataField>storage_cell</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="price">
          <DataField>price</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="total_item_price">
          <DataField>total_item_price</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="digits">
          <DataField>digits</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="order_info">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	counterparty.name AS client,
	delivery_points.compiled_address_short AS address,
	orders.id AS order_id,
	orders.e_shop_order AS eshop_order,
	orders.comment,
	IF(dp_phones.id IS NOT NULL, 
		GROUP_CONCAT(CONCAT('+7 ',dp_phones.number) SEPARATOR '; '), 
		GROUP_CONCAT(CONCAT('+7 ',client_phones.number) SEPARATOR '; ')
	) AS phones,
	CONCAT_WS(', ', DATE_FORMAT(orders.delivery_date, "%d.%m.%Y"), sch.name) AS date_time,
	gg.name as geo_group
FROM orders
LEFT JOIN counterparty ON counterparty.id = orders.client_id
LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
LEFT JOIN phones AS client_phones ON client_phones.counterparty_id = counterparty.id AND client_phones.number IS NOT NULL AND client_phones.number != '' AND client_phones.is_archive = 0
LEFT JOIN phones AS dp_phones ON dp_phones.delivery_point_id = delivery_points.id AND dp_phones.number IS NOT NULL AND dp_phones.number != '' AND dp_phones.is_archive = 0
LEFT JOIN delivery_schedule sch ON sch.id = orders.delivery_schedule_id
LEFT JOIN districts ON districts.id = delivery_points.district_id
LEFT JOIN geo_groups gg ON gg.id = districts.geo_group_id
WHERE orders.id = @order_id
LIMIT 1
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>={?order_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="client">
          <DataField>client</DataField>
        </Field>
        <Field Name="address">
          <DataField>address</DataField>
        </Field>
        <Field Name="order_id">
          <DataField>order_id</DataField>
        </Field>
        <Field Name="eshop_order">
          <DataField>eshop_order</DataField>
        </Field>
        <Field Name="comment">
          <DataField>comment</DataField>
        </Field>
        <Field Name="phones">
          <DataField>phones</DataField>
        </Field>
        <Field Name="date_time">
          <DataField>date_time</DataField>
        </Field>
        <Field Name="geo_group">
          <DataField>geo_group</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="author">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    GET_PERSON_NAME_WITH_INITIALS(employees.last_name, employees.name, employees.patronymic) AS name
FROM
    employees
    JOIN orders ON orders.author_employee_id = employees.id AND orders.id = @order_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>={?order_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="eshop_chief">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    GET_PERSON_NAME_WITH_INITIALS(employees.last_name, employees.name, employees.patronymic) AS name
FROM
    employees
    JOIN subdivisions ON subdivisions.chief_id = employees.id
WHERE
    subdivisions.id = GET_PARAMETER('eshop_subdivision')</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PaymentTotal">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    IF(orders.payment_type = 'cash', GET_TOTAL_SUM_FOR_ORDER_ITEM(order_items.id), 0) AS total_payment
FROM order_items
LEFT JOIN orders ON orders.id = order_items.order_id
LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
WHERE orders.id = @order_id
    AND nomenclature.online_store_id IS NOT NULL 
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>={?order_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="total_payment">
          <DataField>total_payment</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
