  <DataSets>
    <DataSet Name="Order">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	orders.id AS order_id, 
	orders.delivery_date AS order_date, 
                case when (orders.order_address_type = 'Service') then 1 else 0 end AS is_service, 
    	CONCAT(counterparty.vod_internal_id, '-',counterparty_contract.subnumber) AS contract,
	counterparty.full_name AS client_name, 
	counterparty.jur_address AS client_address, 
	counterparty.inn, counterparty.kpp,
    IF(!IS_NULL_OR_WHITESPACE(counterparty.special_contract_number) and counterparty.special_contract_date is not null,
       CONCAT(counterparty.special_contract_name, ' №', counterparty.special_contract_number, ' от ', DATE_FORMAT(counterparty.special_contract_date, '%d.%m.%y')),
       counterparty.special_contract_name) AS special_contract_number,
	GROUP_CONCAT(CONCAT('+7 ', phones.digits_number), ', ') AS phone,
	delivery_points.compiled_address as delivery_address
FROM orders 
	LEFT JOIN counterparty ON orders.client_id = counterparty.id
    LEFT JOIN counterparty_contract ON counterparty_contract.id = orders.counterparty_contract_id
	LEFT JOIN delivery_points on delivery_points.id = orders.delivery_point_id
	LEFT JOIN phones ON delivery_points.id = phones.delivery_point_id AND phones.is_archive = 0
WHERE orders.id = @order_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="order_date">
          <DataField>order_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="is_service">
          <DataField>is_service</DataField>
          <TypeName>System.Boolean</TypeName>
        </Field>
        <Field Name="contract">
          <DataField>contract</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="client_name">
          <DataField>client_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="client_address">
          <DataField>client_address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="inn">
          <DataField>inn</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="kpp">
          <DataField>kpp</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="phone">
          <DataField>phone</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="delivery_address">
          <DataField>delivery_address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="special_contract_number">
          <DataField>special_contract_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MasterOrderItem">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT order_items.id as order_items_id,
       FORMAT(order_items.count, 0) as count,
       mu.digits as digits,
       FORMAT(order_items.price, 2, 'ru_RU') as price,
       nomenclature.name as name,
       order_items.order_id as order_id,
       nomenclature.category as category,
       FORMAT(order_items.price * order_items.count - order_items.discount_money, 2, 'ru_RU') as sum_price
FROM order_items
    LEFT JOIN nomenclature on order_items.nomenclature_id=nomenclature.id
    LEFT JOIN measurement_units mu on nomenclature.unit_id = mu.id
WHERE (nomenclature.category="master" OR nomenclature.category="spare_parts")
    AND order_items.order_id=@order_id
UNION ALL
SELECT '' as order_items_id,
       '' as count,
       '' as digits,
       '' as price,
       '' as name,
       '' as order_id,
       'spare_parts' as category,
       '' as sum_price
UNION ALL
SELECT '' as order_items_id,
       '' as count,
       '' as digits,
       '' as price,
       '' as name,
       '' as order_id,
       'master' as category,
       '' as sum_price</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
