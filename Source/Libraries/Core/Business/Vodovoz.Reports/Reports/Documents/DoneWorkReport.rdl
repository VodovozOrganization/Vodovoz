    <DataSet Name="images">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	stored_resource.id AS ID1,
	stored_resource.binary_file AS IM1
FROM stored_resource
WHERE 
stored_resource.id = (select str_value from base_parameters where name = "kuler_service_image_id")
;</CommandText>
