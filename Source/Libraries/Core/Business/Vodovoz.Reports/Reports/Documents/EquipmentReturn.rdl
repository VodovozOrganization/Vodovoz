  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT order_equipment.id, nomenclature.official_name, order_equipment.count FROM order_equipment 
LEFT JOIN nomenclature ON nomenclature.id = order_equipment.nomenclature_id
WHERE order_equipment.order_id = @order_id AND order_equipment.direction = 'PickUp' AND order_equipment.own_type = 'Rent' AND order_equipment.direction_reason = 'Rent'</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="official_name">
          <DataField>official_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Order">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	orders.id,
	orders.delivery_date,
	CONCAT(counterparty.vod_internal_id,
	'-',counterparty_contract.subnumber) AS contract_number,
	organizations.full_name as organization,
	counterparty.full_name as client,
    IF(!IS_NULL_OR_WHITESPACE(counterparty.special_contract_number) and counterparty.special_contract_date is not null,
       CONCAT(counterparty.special_contract_name, ' №', counterparty.special_contract_number, ' от ', DATE_FORMAT(counterparty.special_contract_date, '%d.%m.%y')),
       counterparty.special_contract_name) AS special_contract_number,
	leader.last_name AS leader_last_name,
    LEFT(leader.name, 1) AS leader_name,
    LEFT(leader.patronymic, 1) AS leader_patronymic
FROM orders
LEFT JOIN counterparty_contract on orders.counterparty_contract_id = counterparty_contract.id
LEFT JOIN counterparty on counterparty_contract.counterparty_id = counterparty.id
LEFT JOIN organizations on organizations.id = counterparty_contract.organization_id
LEFT JOIN
    organization_versions ov
    ON
        organizations.id = ov.organization_id 
    AND
        ov.start_date &lt;= orders.bill_date 
    AND
        (ov.end_date is null OR ov.end_date &gt;= orders.bill_date)
LEFT JOIN
     employees leader ON leader.id = ov.leader_id
WHERE orders.id = @order_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>={?order_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="delivery_date">
          <DataField>delivery_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="contract_number">
          <DataField>contract_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="organization">
          <DataField>organization</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="client">
          <DataField>client</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="leader_last_name">
          <DataField>leader_last_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="leader_name">
          <DataField>leader_name</DataField>
