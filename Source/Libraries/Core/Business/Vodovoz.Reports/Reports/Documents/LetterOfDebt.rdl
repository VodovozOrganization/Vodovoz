          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="contact_phone">
          <DataField>contact_phone</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="order_debt_formatted">
          <DataField>order_debt_formatted</DataField>
