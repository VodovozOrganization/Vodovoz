    <DataSet Name="Organization">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    organizations.full_name,
    organizations.name as org_name,
    organizations.INN,
    organizations.KPP,
    accounts.`number` as account,
    ov.jur_address AS address,
    phones.additional,
    banks.name,
    banks.bik,
    cor_account.cor_account_number as cor_account,
    accounts.number,
    leader.last_name AS leader_last_name,
    leader.name AS leader_name,
    leader.patronymic AS leader_patronymic,
    CASE 
        WHEN CONCAT('+7 ', phones.number) IS NOT NULL AND phones.additional IS NOT NULL AND phones.additional != ''
            THEN CONCAT('Тел. ', CONCAT('+7 ', phones.number), ', доб. ', phones.additional)
        WHEN CONCAT('+7 ', phones.number) IS NOT NULL 
            THEN CONCAT('Тел. ', CONCAT('+7 ', phones.number))
        ELSE ''
    END AS contact_info,
    CASE 
        WHEN organizations.full_name LIKE 'ООО%' THEN CONCAT('Общество с ограниченной ответственностью', SUBSTRING(organizations.full_name, 4))
        WHEN organizations.full_name LIKE 'ИП%' THEN CONCAT('Индивидуальный предприниматель', SUBSTRING(organizations.full_name, 3))
        ELSE organizations.full_name
    END AS full_name_formatted
FROM
    orders
        LEFT JOIN
    counterparty ON orders.client_id = counterparty.id
        LEFT JOIN
    counterparty_contract ON orders.counterparty_contract_id = counterparty_contract.id
        LEFT JOIN
    organizations ON organizations.id = counterparty_contract.organization_id
        LEFT JOIN
   organization_versions ov
        ON
            organizations.id = ov.organization_id 
        AND
            ov.start_date &lt;= orders.bill_date 
        AND
            (ov.end_date is null OR ov.end_date &gt;= orders.bill_date)
        LEFT JOIN
    accounts ON 
    		((counterparty.our_organization_account_for_bills is NULL and accounts.id = organizations.default_account_id) 
    		or (accounts.id = counterparty.our_organization_account_for_bills))
    	LEFT JOIN
    banks_cor_accounts cor_account ON accounts.bank_cor_account_id = cor_account.id 
    	LEFT JOIN
    banks ON banks.id = accounts.bank_id
        LEFT JOIN
    employees leader ON leader.id = ov.leader_id
        LEFT JOIN
    employees buh ON buh.id = ov.accountant_id 
        LEFT JOIN
    phones ON organizations.id = phones.org_id AND phones.is_archive = 0
WHERE
    orders.id = @order_id LIMIT 1;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>={?order_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
