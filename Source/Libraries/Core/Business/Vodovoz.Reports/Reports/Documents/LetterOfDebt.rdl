  <DataSets>
    <DataSet Name="Order">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    o.id AS order_id,
    doc.document_number AS document_number,
    DATE_FORMAT(o.delivery_date,'%d.%m.%y') AS order_date,
    DATE_FORMAT(o.bill_date,'%d.%m.%Y') AS bill_date,
    c.full_name AS client_name,
    c.inn,
    c.kpp,
    p.`number` AS contact_phone,
    (
        Select
            GROUP_CONCAT(
                    CONCAT(
                            '+7 ',
                            p_delivery_point.number,
                            IF(
                                    p_delivery_point.comment IS NULL,
                                    '',
                                    CONCAT(' ', p_delivery_point.comment)
                                )
                        )
                    SEPARATOR '; '
                )
        from phones p_delivery_point
        where dp.id = p_delivery_point.delivery_point_id
          and p_delivery_point.is_archive = 0
    )AS delivery_point_contact,
    (
        Select
            GROUP_CONCAT(
                    CONCAT(
                            '+7 ',
                            p_counterparty.number,
                            IF(
                                    p_counterparty.comment IS NULL,
                                    '',
                                    CONCAT(' ', p_counterparty.comment)
                            )
                    )
                    SEPARATOR '; '
            )
        from phones p_counterparty
        where c.id = p_counterparty.counterparty_id
          and p_counterparty.is_archive = 0
    )AS client_contact,
    CONCAT(
        FORMAT(
            (
                select sum(ROUND(IFNULL(oi.actual_count, oi.count) * oi.price - oi.discount_money, 2))
                from order_items oi
                where oi.order_id = o.id
            ) - 
            (
                select IFNULL(sum(pi2.sum), 0)
                from payment_items pi2
                where pi2.order_id = o.id
                  and pi2.payment_item_status != 'Cancelled'
            ), 2
        ),
        ' руб. (',
        NUMBER_TO_STRING_DESCRIPTION(
            (
                select sum(ROUND(IFNULL(oi.actual_count, oi.count) * oi.price - oi.discount_money, 2))
                from order_items oi
                where oi.order_id = o.id
            ) - 
            (
                select IFNULL(sum(pi2.sum), 0)
                from payment_items pi2
                where pi2.order_id = o.id
                  and pi2.payment_item_status != 'Cancelled'
            )
        ),
        ')'
    ) AS order_debt_formatted
FROM
    orders o
        LEFT JOIN
    counterparty c ON o.client_id = c.id
        LEFT JOIN
    delivery_points dp ON dp.id = o.delivery_point_id
        LEFT JOIN
    employees ON employees.id = o.author_employee_id
    	LEFT JOIN
    phones p ON o.contact_phone_id = p.id
      LEFT JOIN 
    order_documents od ON od.order_id = o.id AND od.type IN ('SpecialUPD', 'UPD') AND od.document_organization_counter_id IS NOT NULL
      LEFT JOIN 
    document_organization_counters doc ON doc.id = od.document_organization_counter_id
WHERE
        o.id = @order_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="order_id">
            <Value>=Parameters!order_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
