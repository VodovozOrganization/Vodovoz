  <DataSets>
    <DataSet Name="CashTransferDocument">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	cash_transfer_documents.id as cash_transfer_document_id,
    cash_transfer_documents.creation_date as cash_transfer_document_date,
    cash_transfer_documents.transfered_sum as transfered_sum,
    CONCAT_WS('', CONCAT(authtors.last_name, ' '), if(authtors.name = '', '', CONCAT(LEFT(authtors.name, 1),'.')), if(authtors.patronymic = '', '', CONCAT(LEFT(authtors.patronymic, 1),'.'))) as authtor_fio,
    CONCAT_WS('', CONCAT(drivers.last_name, ' '), if(drivers.name = '', '', CONCAT(LEFT(drivers.name, 1),'.')), if(drivers.patronymic = '', '', CONCAT(LEFT(drivers.patronymic, 1),'.'))) as driver_fio,
	CONCAT_WS('', CONCAT(senders.last_name, ' '), if(senders.name = '', '', CONCAT(LEFT(senders.name, 1),'.')), if(senders.patronymic = '', '', CONCAT(LEFT(senders.patronymic, 1),'.'))) as sender_fio,
    CONCAT_WS('', CONCAT(receivers.last_name, ' '), if(receivers.name = '', '', CONCAT(LEFT(receivers.name, 1),'.')), if(receivers.patronymic = '', '', CONCAT(LEFT(receivers.patronymic, 1),'.'))) as receiver_fio,
    subdivisions_from.name as subdivision_from,
    subdivisions_to.name as subdivision_to,
    CONCAT(cm.name, ' (', cars.reg_number, ')') as car_title
FROM
	cash_transfer_documents
		LEFT JOIN
	employees as drivers ON drivers.id = cash_transfer_documents.driver_id
    	LEFT JOIN
	employees as authtors ON authtors.id = cash_transfer_documents.authtor_id
    	LEFT JOIN
	employees as senders ON senders.id = cash_transfer_documents.cashier_sender_id
    	LEFT JOIN
	employees as receivers ON receivers.id = cash_transfer_documents.cashier_receiver_id    
    	LEFT JOIN
	subdivisions as subdivisions_from ON subdivisions_from.id = cash_transfer_documents.cash_subdivision_from_id
    	LEFT JOIN
	subdivisions as subdivisions_to ON subdivisions_to.id = cash_transfer_documents.cash_subdivision_to_id
    	LEFT JOIN
	cars ON cars.id = cash_transfer_documents.car_id
	    LEFT JOIN
    car_models cm ON cars.model_id = cm.id
WHERE
	cash_transfer_documents.id = @transfer_document_id
GROUP BY cash_transfer_documents.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="transfer_document_id">
            <Value>=Parameters!transfer_document_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
