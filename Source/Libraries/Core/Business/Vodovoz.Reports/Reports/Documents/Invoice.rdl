    <DataSet Name="DepositRefund">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
deposit_sum,
IFNULL(actual_count, count) as deposit_count, CASE deposit_type WHEN 'None' THEN '-' WHEN 'Bottles' THEN 'Залог за бутыли' WHEN 'Equipment' THEN 'Залог за оборудование' END AS deposit_type
FROM
order_deposit_items
WHERE
order_id = @order_id
AND 
payment_type='ToClient'</CommandText>
