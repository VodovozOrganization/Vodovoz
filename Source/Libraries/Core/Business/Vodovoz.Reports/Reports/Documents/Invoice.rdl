  <DataSets>
    <DataSet Name="Order">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    IFNULL(orders.e_shop_order, "") AS e_shop_order,
    orders.delivery_date AS order_date,
    delivery_points.organization as organization,
    orders.order_status AS order_status,
    (SELECT count(*) &gt; 0
     FROM fast_payments fp
     WHERE (fp.order_id = orders.id
         OR fp.external_id = orders.id
         OR fp.external_id = orders.online_order)
       AND fp.payment_status = 'Performed') AS fast_payment_paid,
    IF(is_bottle_stock,
       orders.bottles_by_stock_count,
       orders.bottles_return
    ) AS bottles_return,
    IF(orders.self_delivery = false,
       IFNULL(SUM(route_list_addresses.bottles_returned), 0),
       (SELECT
            CAST(ROUND(IFNULL(SUM(ssddr.amount), 0), 0) AS INT)
        FROM
            store_self_delivery_document_returned ssddr
                LEFT JOIN
            store_self_delivery_document ssdd ON ssdd.id = ssddr.store_self_delivery_document_id
        WHERE
            ssdd.order_id = orders.id
          AND
            ssddr.nomenclature_id = (SELECT str_value FROM base_parameters WHERE base_parameters.name = 'default_bottle_nomenclature')
       )
    ) AS bottles_return_fact,
    orders.sum_to_receive AS total,
    orders.comment AS order_comment,
    orders.comment_logist AS order_comment_for_logist,
    CASE orders.signature_type
        WHEN 'BySeal' THEN 'По печати'
        WHEN 'ByProxy' THEN 'По доверенности'
        WHEN 'ProxyOnDeliveryPoint' THEN 'Доверенность на адресе'
        ELSE orders.signature_type
        END AS order_signature_type,
    CASE orders.payment_type
        WHEN 'Cash' THEN 'Наличные'
        WHEN 'Terminal' THEN 'По терминалу (QR-код терминал)'
        WHEN 'DriverApplicationQR' THEN 'По QR-коду (приложение водителя)'
        WHEN 'SmsQR' THEN IF(
                (SELECT count(*) &gt; 0
                 FROM fast_payments fp
                 WHERE (fp.order_id = orders.id
                     OR fp.external_id = orders.id
                     OR fp.external_id = orders.online_order)
                   AND fp.payment_status = 'Performed'), 'СМС (QR-код)', 'По QR-коду (приложение водителя)')
        WHEN 'PaidOnline' THEN 'Оплачено онлайн'
        WHEN 'Barter' THEN 'Безденежно'
        WHEN 'ContractDocumentation' THEN ''
        WHEN 'Cashless' THEN 'Безналичная'
        ELSE orders.payment_type
        END AS pay_type,
    orders.payment_type AS order_payment_type,
    orders.online_order AS order_online_number,
    delivery_schedule.name AS delivery_schedule,
    counterparty_contract.number AS contract_number,
    DATE_FORMAT(counterparty_contract.issue_date,
                '%d.%m.%y') AS contract_date,
    counterparty.full_name AS client_name,
    counterparty.address AS client_address,
    organizations.full_name AS org_name,
    CONCAT(
            ov.jur_address,
            '\n',
            CONCAT('ИНН:', organizations.INN)
    ) AS org_address,
    (SELECT
         CONCAT('+7 ',
                COALESCE (
                        phones_data.contact_phone,
                        phones_data.delivery_point_phone_work,
                        phones_data.delivery_point_phone_general,
                        phones_data.delivery_point_phone_all,
                        phones_data.counterparty_phone_work,
                        phones_data.counterparty_phone_general,
                        phones_data.counterparty_phone_all)
         )
     FROM
         ((SELECT o.id,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE p.is_archive = 0 AND p.id = o.contact_phone_id
                   LIMIT 1
                  ) AS contact_phone,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE
                       p.is_archive = 0
                     AND p.delivery_point_id = o.delivery_point_id
                     AND p.type_id = (SELECT str_value FROM base_parameters bp WHERE bp.name = 'phone_work_type_id')
                   LIMIT 1
                  ) AS delivery_point_phone_work,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE
                       p.is_archive = 0
                     AND p.delivery_point_id = o.delivery_point_id
                     AND p.type_id = (SELECT str_value FROM base_parameters bp WHERE bp.name = 'phone_general_type_id')
                   LIMIT 1
                  ) AS delivery_point_phone_general,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE
                       p.is_archive = 0
                     AND p.delivery_point_id = o.delivery_point_id
                   LIMIT 1
                  ) AS delivery_point_phone_all,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE
                       p.is_archive = 0
                     AND p.counterparty_id = o.client_id
                     AND p.type_id = (SELECT str_value FROM base_parameters bp WHERE bp.name = 'phone_work_type_id')
                   LIMIT 1
                  ) AS counterparty_phone_work,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE
                       p.is_archive = 0
                     AND p.counterparty_id = o.client_id
                     AND p.type_id = (SELECT str_value FROM base_parameters bp WHERE bp.name = 'phone_general_type_id')
                   LIMIT 1
                  ) AS counterparty_phone_general,
                  (SELECT CONCAT(p.number, ' ', IFNULL(p.comment, ''))
                   FROM phones p
                   WHERE
                       p.is_archive = 0
                     AND p.counterparty_id = o.client_id
                   LIMIT 1
                  ) AS counterparty_phone_all
           FROM
               orders o
           WHERE
               o.id = @order_id) phones_data
             )) AS contact_phone,
    districts.district_name AS logistics_area,
    delivery_points.compiled_address_short AS del_point_address,
    orders.daily_number_1c AS daily_number,
    CONCAT_WS('',
              CONCAT(employees.last_name, ' '),
              IF(employees.name = '',
                 '',
                 CONCAT(LEFT(employees.name, 1), '.')),
              IF(employees.patronymic = '',
                 '',
                 CONCAT(LEFT(employees.patronymic, 1), '.'))) AS author,
    orders.trifle AS trifle,
    orders.self_delivery,
    IF(delivery_points.lunch_time_from &lt;&gt; delivery_points.lunch_time_to , concat(DATE_FORMAT(delivery_points.lunch_time_from, "%H:%i"), " - ", DATE_FORMAT(delivery_points.lunch_time_to, "%H:%i")), "") AS lunch_time,
   (
        select IFNULL(sum(ROUND(IFNULL(oi.actual_count, oi.count) * oi.price - oi.discount_money, 2)), 0)
        from order_items oi
        where oi.order_id = orders.id
    ) AS order_sum,
    (
            select IFNULL(sum(ROUND(IFNULL(odi.actual_count, odi.count) * odi.deposit_sum, 2)), 0)
            from order_deposit_items odi
            where odi.order_id = orders.id
    ) AS order_deposit_refund_sum,
    (
        CONCAT(
            IF(
                (select order_sum - order_deposit_refund_sum) &gt; 0,
                'Всего получить от клиента: ',
                'Всего вернуть клиенту: '),
            IF(orders.payment_type != 'PaidOnline',
                NUMBER_TO_STRING_DESCRIPTION(ABS((select order_sum - order_deposit_refund_sum))),
                NUMBER_TO_STRING_DESCRIPTION(0)
            ),
            ' (',
            IF(orders.payment_type != 'PaidOnline',
               ABS((select order_sum - order_deposit_refund_sum)),
               '0.00'
            ),
            'р.)'
        )
    ) as payment_string
FROM
    orders
        LEFT JOIN
    route_list_addresses ON route_list_addresses.order_id = orders.id AND route_list_addresses.status = 'Completed'
        LEFT JOIN
    counterparty ON orders.client_id = counterparty.id
        LEFT JOIN
    counterparty_contract ON counterparty_contract.counterparty_id = orders.client_id AND orders.counterparty_contract_id = counterparty_contract.id
        LEFT JOIN
    organizations ON organizations.id = counterparty_contract.organization_id
        LEFT JOIN
    organization_versions ov
    ON
        organizations.id = ov.organization_id
            AND
        ov.start_date &lt;= orders.bill_date
            AND
        (ov.end_date is null OR ov.end_date &gt;= orders.bill_date)
        LEFT JOIN
    delivery_schedule ON orders.delivery_schedule_id = delivery_schedule.id
        LEFT JOIN
    delivery_points ON orders.delivery_point_id = delivery_points.id
        LEFT JOIN
    districts ON delivery_points.district_id = districts.id
        LEFT JOIN
    employees ON employees.id = orders.author_employee_id
WHERE
    orders.id = @order_id
;
</CommandText>
