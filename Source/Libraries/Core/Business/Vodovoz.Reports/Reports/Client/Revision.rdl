  <Code>
  <![CDATA[
  Public Function NumberToWords(ByVal num As Decimal) As String
      Try
          Dim rub As Integer = Math.Floor(num)
          Dim kop As Integer = Math.Round((num - rub) * 100)
          
          ' Форматируем копейки как двузначное число
          Dim kopStr As String = kop.ToString("00")
          
          Return RublesToWords(rub) + " рублей " + kopStr + " копеек"
      Catch
          Return "ноль рублей 00 копеек"
      End Try
  End Function

  Private Function RublesToWords(ByVal rub As Integer) As String
      If rub = 0 Then Return "ноль"
      
      Dim units() As String = {"", "один", "два", "три", "четыре", "пять", "шесть", "семь", "восемь", "девять"}
      Dim teens() As String = {"десять", "одиннадцать", "двенадцать", "тринадцать", "четырнадцать", "пятнадцать", "шестнадцать", "семнадцать", "восемнадцать", "девятнадцать"}
      Dim tens() As String = {"", "", "двадцать", "тридцать", "сорок", "пятьдесят", "шестьдесят", "семьдесят", "восемьдесят", "девяносто"}
      Dim hundreds() As String = {"", "сто", "двести", "триста", "четыреста", "пятьсот", "шестьсот", "семьсот", "восемьсот", "девятьсот"}
      
      Dim result As String = ""
      Dim temp As Integer = rub
      
      'Тысячи
      Dim thousands As Integer = Math.Floor(temp / 1000)
      If thousands > 0 Then
          Dim thousandsWord As String = GetThousandsWord(thousands)
          result += HundredsToWords(thousands, True) + " " + thousandsWord + " "
          temp = temp Mod 1000
      End If
      
      'Сотни
      Dim hundred As Integer = Math.Floor(temp / 100)
      If hundred > 0 Then
          result += hundreds(hundred) + " "
          temp = temp Mod 100
      End If
      
      'Десятки и единицы
      If temp >= 20 Then
          Dim ten As Integer = Math.Floor(temp / 10)
          result += tens(ten) + " "
          temp = temp Mod 10
          If temp > 0 Then
              result += units(temp) + " "
          End If
      ElseIf temp >= 10 Then
          result += teens(temp - 10) + " "
      ElseIf temp > 0 Then
          result += units(temp) + " "
      End If
      
      Return result.Trim()
  End Function

  Private Function GetThousandsWord(ByVal num As Integer) As String
      Dim lastDigit As Integer = num Mod 10
      Dim lastTwoDigits As Integer = num Mod 100
      
      If lastTwoDigits >= 11 AndAlso lastTwoDigits <= 19 Then
          Return "тысяч"
      ElseIf lastDigit = 1 Then
          Return "тысяча"
      ElseIf lastDigit >= 2 AndAlso lastDigit <= 4 Then
          Return "тысячи"
      Else
          Return "тысяч"
      End If
  End Function

  Private Function HundredsToWords(ByVal num As Integer, ByVal isThousands As Boolean) As String
      Dim units() As String = {"", If(isThousands, "одна", "один"), If(isThousands, "две", "два"), "три", "четыре", "пять", "шесть", "семь", "восемь", "девять"}
      Dim teens() As String = {"десять", "одиннадцать", "двенадцать", "тринадцать", "четырнадцать", "пятнадцать", "шестнадцать", "семнадцать", "восемнадцать", "девятнадцать"}
      Dim tens() As String = {"", "", "двадцать", "тридцать", "сорок", "пятьдесят", "шестьдесят", "семьдесят", "восемьдесят", "девяносто"}
      Dim hundreds() As String = {"", "сто", "двести", "триста", "четыреста", "пятьсот", "шестьсот", "семьсот", "восемьсот", "девятьсот"}
      
      Dim result As String = ""
      Dim temp As Integer = num
      
      'Сотни
      Dim hundred As Integer = Math.Floor(temp / 100)
      If hundred > 0 Then
          result += hundreds(hundred) + " "
          temp = temp Mod 100
      End If
      
      'Десятки и единицы
      If temp >= 20 Then
          Dim ten As Integer = Math.Floor(temp / 10)
          result += tens(ten) + " "
          temp = temp Mod 10
          If temp > 0 Then
              result += units(temp) + " "
          End If
      ElseIf temp >= 10 Then
          result += teens(temp - 10) + " "
      ElseIf temp > 0 Then
          result += units(temp) + " "
      End If
      
      Return result.Trim()
  End Function
  ]]>
