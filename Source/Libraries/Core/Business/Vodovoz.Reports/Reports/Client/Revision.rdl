          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="document">
          <DataField>document</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Organization">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select 
        o.full_name
        from organizations o
        where o.id = @organization_id
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="organization_id">
            <Value>=Parameters!OrganizationId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="org_name">
          <DataField>full_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Counterparty">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    full_name,
    counterparty_contract.id as contract_id,
    issue_date
FROM
    counterparty
        JOIN
    counterparty_contract ON counterparty_contract.counterparty_id = counterparty.id
WHERE
    counterparty.id = @counterparty_id
        AND counterparty_contract.organization_id = (SELECT 
            str_value
        FROM
            base_parameters
        WHERE
            name = 'cashless_organization_id')</CommandText>
        <QueryParameters>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="counterparty_name">
          <DataField>full_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="contract_id">
          <DataField>contract_id</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="issue_date">
          <DataField>issue_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Initial">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
        SELECT 
            T.total
        FROM (
            SELECT
                IFNULL(SUM(IFNULL(cmo.income, 0)) - SUM(IFNULL(cmo.expense, 0)), 0) as total
            FROM 
                cashless_movement_operations cmo 
            WHERE
                cmo.counterparty_id = @counterparty_id
                AND cmo.organization_id = @organization_id
                AND cmo.operation_time &lt; @date_start
        ) as T
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyId</Value>
          </QueryParameter>
          <QueryParameter Name="date_start">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="date_end">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="organization_id">
            <Value>=Parameters!OrganizationId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="total">
          <DataField>total</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Payments">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
                SELECT
                IFNULL(SUM(deposit.dep), 0) as depositsum
                FROM orders o
                LEFT JOIN (
                SELECT
                IF(SUM(count * deposit_sum) &gt; 0, SUM(count * deposit_sum), 0) as dep,
                order_id
                FROM order_deposit_items
                GROUP BY order_id
                ) deposit ON deposit.order_id = o.id
                WHERE
                o.client_id = @counterparty_id
                AND o.delivery_date BETWEEN @start_date AND @end_date
            </CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="depositsum">
          <DataField>depositsum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
