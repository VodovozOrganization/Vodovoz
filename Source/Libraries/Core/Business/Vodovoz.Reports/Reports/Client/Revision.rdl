  <DataSets>
    <DataSet Name="Operations">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    COALESCE(orders.id,account_income.number) as num,
    CASE
		WHEN doc_residue.id IS NOT NULL
		THEN
			CONCAT ('Ввод остатков №', doc_residue.id)
		WHEN orders.id IS NOT NULL
		THEN
			CONCAT ('П №', orders.id)
		WHEN account_income.number IS NOT NULL
		THEN
			CONCAT ('Оплата №', account_income.number)
	END AS document,
    IFNULL(debt, 0) as debt,
    IFNULL(deposit, 0) as deposit,
    money,
    money_movement_operations.operation_time as date
FROM
    money_movement_operations
    LEFT JOIN orders 		 ON order_id = orders.id AND money_movement_operations.debt is not null
    LEFT JOIN account_income ON account_income.money_movement_operation_id = money_movement_operations.id
    LEFT JOIN doc_residue 	 ON money_movement_operations.id = doc_residue.money_movement_operation_id
WHERE 
	money_movement_operations.counterparty_id=@counterparty_id
	AND money_movement_operations.payment_type='Cashless'
	AND	@date_start &lt;= operation_time
	AND operation_time &lt;= DATE_ADD(@date_end, INTERVAL 1 DAY)
ORDER BY date ASC</CommandText>
        <QueryParameters>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyID</Value>
          </QueryParameter>
          <QueryParameter Name="date_start">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="date_end">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="num">
          <DataField>num</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="debt">
          <DataField>debt</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="money">
          <DataField>money</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="deposit">
          <DataField>deposit</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="document">
          <DataField>document</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Organization">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select 
full_name
 from organizations 
where id = (select str_value from base_parameters where name = "cashless_organization_id")</CommandText>
      </Query>
      <Fields>
        <Field Name="org_name">
          <DataField>full_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Counterparty">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    full_name,
    counterparty_contract.id as contract_id,
    issue_date
FROM
    counterparty
        JOIN
    counterparty_contract ON counterparty_contract.counterparty_id = counterparty.id
WHERE
    counterparty.id = @counterparty_id
        AND counterparty_contract.organization_id = (SELECT 
            str_value
        FROM
            base_parameters
        WHERE
            name = 'cashless_organization_id')</CommandText>
        <QueryParameters>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyID</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="counterparty_name">
          <DataField>full_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="contract_id">
          <DataField>contract_id</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="issue_date">
          <DataField>issue_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Initial">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    IFNULL(SUM(IFNULL(debt,0)) - SUM(IFNULL(money,0)),0) as total,
    NUMBER_TO_STRING_DESCRIPTION(ABS((select total))) as total_string
FROM
    money_movement_operations
where
    money_movement_operations.counterparty_id=@counterparty_id
  and
    money_movement_operations.payment_type='Cashless'
  and operation_time &lt; @date_start
;
</CommandText>
        <QueryParameters>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyID</Value>
          </QueryParameter>
          <QueryParameter Name="date_start">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="total">
          <DataField>total</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="total_string">
          <DataField>total_string</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
