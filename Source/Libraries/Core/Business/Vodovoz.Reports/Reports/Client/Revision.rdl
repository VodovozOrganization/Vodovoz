                Case Is = "07"
                    TrStr = TrStr &amp; "семь "
                Case Is = "08"
                    TrStr = TrStr &amp; "восемь "
                Case Is = "09"
                    TrStr = TrStr &amp; "девять "
                Case Is = "10"
                    TrStr = TrStr &amp; "десять "
                Case Is = "11"
                    TrStr = TrStr &amp; "одиннадцать "
                Case Is = "12"
                    TrStr = TrStr &amp; "двенадцать "
                Case Is = "13"
