  <DataSets>
    <DataSet Name="Operations">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
          SELECT *
          FROM (
          SELECT # Не оплаченные и частично оплаченные заказы
            0 as num,
            CONCAT('Продажа (', 
                    IFNULL(doc.document_number, o.id), 
                    ' от ', 
                    o.delivery_date, 
                    ')' 
            ) as document,
              (
                  (
                    SELECT round(sum(IFNULL(oi.actual_count, oi.count) * oi.price - oi.discount_money), 2)
                      FROM order_items oi
                      WHERE oi.order_id = o.id
                  )
                  -
                  (
                      SELECT IFNULL(sum(cmo.expense), 0)
                      FROM payment_items pi
                          INNER JOIN cashless_movement_operations cmo ON pi.cashless_movement_operation_id = cmo.id
                      WHERE pi.order_id = o.id
                        AND cmo.cashless_movement_operation_status != 'Cancelled')
                  ) as debt,
            0 as deposit,
              0 as money,
              o.delivery_date AS date,
            CASE
              WHEN (
                SELECT IFNULL(sum(cmo.expense), 0)
                FROM payment_items pi
                  INNER JOIN cashless_movement_operations cmo ON pi.cashless_movement_operation_id = cmo.id
                WHERE pi.order_id = o.id
                  AND cmo.cashless_movement_operation_status != 'Cancelled'
              ) = 0 THEN 'NotPaid'
              ELSE 'PartiallyPaid'
            END as payment_status,
            o.order_status as order_status
          FROM orders o
          LEFT JOIN counterparty_contract cc ON o.counterparty_contract_id = cc.id
          LEFT JOIN order_documents od ON od.order_id = o.id 
            AND od.type IN ('SpecialUPD', 'UPD') 
            AND od.document_organization_counter_id IS NOT NULL
          LEFT JOIN document_organization_counters doc ON doc.id = od.document_organization_counter_id
          WHERE
              o.client_id = @counterparty_id
              AND o.order_payment_status != 'Paid'
              AND o.payment_type = 'Cashless'
              AND cc.organization_id = @organization_id
              AND o.order_status IN ('Shipped', 'Closed', 'UnloadingOnStock')
          HAVING
            debt &gt; 0
          UNION ALL
          SELECT # Оплаченные заказы
              p.payment_num as num,
              CONCAT('Продажа (', 
                    IFNULL(doc.document_number, o.id), 
                    ' от ', 
                    o.delivery_date, 
                    ')' 
              ) as document,
              pi.sum as debt,
              0 as deposit,
              0 as money,
              o.delivery_date as date,
              'Paid' as payment_status,
              o.order_status as order_status
          FROM 
            orders o
          INNER JOIN 
            payment_items pi ON o.id = pi.order_id
          INNER JOIN 
            payments_from_bank_client p ON pi.payment_id = p.id
          LEFT JOIN counterparty_contract cc ON o.counterparty_contract_id = cc.id
          LEFT JOIN order_documents od ON od.order_id = o.id 
            AND od.type IN ('SpecialUPD', 'UPD') 
            AND od.document_organization_counter_id IS NOT NULL
          LEFT JOIN document_organization_counters doc ON doc.id = od.document_organization_counter_id
          WHERE
            o.client_id = @counterparty_id
            AND pi.payment_item_status != 'Cancelled'
            AND cc.organization_id = @organization_id
            AND o.order_status IN ('Shipped', 'Closed', 'UnloadingOnStock')
          UNION ALL
          SELECT # Депозит, кредит
            p.payment_num as num,
            CONCAT('Оплата (', p.payment_num , ' от ', IFNULL(cmo.operation_time, p.payment_date) , ')') as document, 
            0 as debt,
            cmo.income as deposit,
            0 as money,
            IFNULL(cmo.operation_time, p.payment_date) as date,
            '' as payment_status,
            '' as order_status
          FROM payments_from_bank_client p
            LEFT JOIN cashless_movement_operations cmo ON p.cashless_movement_operation_id = cmo.id
            INNER JOIN organizations o ON p.organization_id = o.id
          WHERE
            p.counterparty_id = @counterparty_id
            AND p.refund_payment_from_order_id is null
            AND (
              p.organization_id = @organization_id
            )) as t
          WHERE 
            t.`date` &gt;= @date_start
          AND 
            t.`date`  &lt; DATE_ADD(@date_end, INTERVAL 1 DAY)
          ORDER BY
            t.`date` ASC
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!CounterpartyId</Value>
          </QueryParameter>
          <QueryParameter Name="date_start">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="date_end">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="organization_id">
            <Value>=Parameters!OrganizationId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="num">
          <DataField>num</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="debt">
          <DataField>debt</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="money">
