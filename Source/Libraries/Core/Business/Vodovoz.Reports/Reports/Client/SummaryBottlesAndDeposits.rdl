          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="bottle_by_stock">
          <DataField>bottle_by_stock</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Signatures">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	(SELECT binary_file FROM stored_resource WHERE id = 15) as print,
	(SELECT binary_file FROM stored_resource WHERE id = 
		(SELECT ov.signature_leader_id FROM organization_versions ov 
		WHERE ov.organization_id = 1 AND ov.end_date IS NULL GROUP BY ov.organization_id DESC)
	) as chief
;</CommandText>
      </Query>
      <Fields>
        <Field Name="chief">
          <DataField>chief</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="print">
          <DataField>print</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
