  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    IF (scldi.item_expiration_date_percent IS NOT NULL
    	AND n.category = 'water',
    	CONCAT(n.name, " &gt;", scldi.item_expiration_date_percent, "% срока годности"),
    n.name) AS nomenclature,
    n.category,
    measurement_units.name AS unit,
    measurement_units.digits AS digits,
    n.tare_volume,
    CASE n.tare_volume
        WHEN 'Vol19L' THEN 0
        WHEN 'Vol6L' THEN 1
        WHEN 'Vol600ml' THEN 2
        ELSE 3
        END AS clmn_for_sorting,
    scldi.amount AS amount,
	c.full_name as client_name,
	dp.compiled_address_short as delivery_address,
	IFNULL(o.id, '0') as order_id,
	n.id as nomenclature_id,
	scldi.is_individual_set_for_order,
	ng.is_highlight_in_car_load_document as is_highlight_row
FROM
	store_car_load_document_items scldi 
	LEFT JOIN store_car_load_documents scld ON scld.id = scldi.car_load_document_id
	LEFT JOIN nomenclature n ON scldi.nomenclature_id = n.id
	LEFT JOIN measurement_units ON measurement_units.id = n.unit_id
	LEFT JOIN orders o ON scldi.order_id = o.id
	LEFT JOIN counterparty c ON o.client_id = c.id
	LEFT JOIN delivery_points dp ON o.delivery_point_id = dp.id
	LEFT JOIN nomenclature_groups ng ON n.group_id = ng.id
WHERE 
	scld.id = @id
	AND n.online_store_id IS NULL
GROUP BY nomenclature, order_id
ORDER BY clmn_for_sorting;</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
