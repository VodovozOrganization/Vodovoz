    <DataSet Name="Data1">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText># FIXME : Исправить фильтрацию складов по районам (сейчас привязана к подразделению )
SELECT
    nomenclature.name AS NomenclatureName,
    nomenclature.id AS ID,
    Equipment.ForClientCount,
    ROUND(ShippingNomenclature.NomenclatureItemSum) as NomenclatureSumForShipping,
    RL.NomenclatureItemSum as AtDriverNomenclatureCount ,
    RL.RouteListId  AS RouteLists ,
    ToAnotherWarehouse.WarehouseDirection ,
    ROUND(ToAnotherWarehouse.nomenclatureCount) AS MovementItemCount ,
    ROUND(SUM(StockBalance.quantity)) as Amount
FROM
    nomenclature

        LEFT JOIN #На отгрузку
        (
            SELECT
                order_items.nomenclature_id  as NomenclatureId,
                ROUND(SUM(order_items.count), measurement_units.digits)  AS NomenclatureItemSum
            FROM
                order_items
                    LEFT JOIN orders ON orders.id = order_items.order_id
                    LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
                    LEFT JOIN measurement_units ON nomenclature.unit_id = measurement_units.id
                    LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
                    LEFT JOIN districts ON districts.id = delivery_points.district_id
                    LEFT JOIN geo_groups ON geo_groups.id = districts.geo_group_id
            WHERE
                    orders.order_status IN ('Accepted', 'InTravelList', 'OnLoading' )
              AND orders.delivery_date = DATE(@date)
              AND geo_groups.id = @district
            GROUP BY (nomenclature.id)
        ) as ShippingNomenclature ON ShippingNomenclature.NomenclatureId = nomenclature.id

        LEFT JOIN #Оборудование к клиенту
        (
            SELECT
                SUM(order_equipment.count)  AS ForClientCount ,
                nomenclature.id AS NomenclatureId
            FROM order_equipment
                     LEFT JOIN orders ON orders.id = order_equipment.order_id
                     LEFT JOIN nomenclature ON nomenclature.id = order_equipment.nomenclature_id
                     LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
                     LEFT JOIN districts ON districts.id = delivery_points.district_id
                     LEFT JOIN geo_groups ON geo_groups.id = districts.geo_group_id
            WHERE
                    orders.order_status IN ('Accepted', 'InTravelList', 'OnLoading' )
              AND order_equipment.direction = 'Deliver'
              AND geo_groups.id = @district
              AND orders.delivery_date = DATE(@date)
            GROUP BY nomenclature.id
        ) as Equipment On Equipment.NomenclatureId = nomenclature.id

        LEFT JOIN #У Водителя
        (
            SELECT
                order_items.nomenclature_id as NomenclatureId,
                ROUND(SUM(order_items.count), measurement_units.digits)  AS NomenclatureItemSum ,
                GROUP_CONCAT(DISTINCT route_lists.id SEPARATOR ' ') AS RouteListId
            FROM
                order_items
                    LEFT JOIN orders ON orders.id = order_items.order_id
                    LEFT JOIN route_list_addresses On route_list_addresses.order_id = orders.id
                    LEFT JOIN route_lists ON route_lists.id = route_list_addresses.route_list_id
                    LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
                    LEFT JOIN measurement_units ON nomenclature.unit_id = measurement_units.id
                    LEFT JOIN cars ON cars.id = route_lists.car_id
                    LEFT JOIN geo_groups_to_entities ON geo_groups_to_entities.car_id = cars.id
            WHERE
                    orders.order_status IN ('Canceled', 'NotDelivered', 'DeliveryCanceled')
              AND route_list_addresses.transfered_to_id IS NULL
              AND route_lists.date &gt;  ADDDATE(CURTIME() , -14)
              AND geo_groups_to_entities.geo_group_id = @district
              AND NOT EXISTS
                (
                    SELECT
                        *
                    FROM
                        store_car_unload_documents
                    WHERE
                            store_car_unload_documents.route_list_id = route_lists.id
                )
            GROUP BY (nomenclature.id)
        ) AS RL ON RL.NomenclatureId = nomenclature.id

        LEFT JOIN #Складские остатки
        (
            SELECT
                nomenclature_id AS nom_id,
                warehouse_id AS wh,
                SUM(amount) AS quantity
            FROM goods_accounting_operations
                     LEFT JOIN warehouses ON warehouses.id = goods_accounting_operations.warehouse_id
                     LEFT JOIN subdivisions On subdivisions.id = warehouses.owning_subdivision
            WHERE
                    #AND geo_groups_to_entities.geo_group_id = @district После обновления раскоментить 
                    AND subdivisions.id = @subdivision OR subdivisions.parent_subdivision_id = @subdivision #До обновления 
            GROUP BY nomenclature_id   
        ) AS StockBalance ON StockBalance.nom_id = nomenclature.id

        LEFT JOIN #Отправлено на другой склад
        (
            SELECT
                store_movement_document_items.nomenclature_id AS NomenclatureId,
                SUM(store_movement_document_items.sended_amount) AS nomenclatureCount ,
                GROUP_CONCAT(DISTINCT CONCAT(WareFrom.Name , ' --&gt; ' , WareTo.Name) SEPARATOR ' ') as WarehouseDirection
            FROM
                store_movement_document_items
                    RIGHT JOIN store_movement_document ON store_movement_document.id = store_movement_document_items.movement_document_id
                    RIGHT JOIN warehouses AS WareFrom ON WareFrom.id = store_movement_document.warehouse_from_id
                    RIGHT JOIN warehouses AS WareTo ON WareTo.id = store_movement_document.warehouse_to_id
                    RIGHT JOIN nomenclature ON nomenclature.id = store_movement_document_items.nomenclature_id

                    RIGHT JOIN subdivisions On subdivisions.id = WareTo.owning_subdivision

            WHERE
                        store_movement_document.transportation_status = 'Submerged'
                    #AND geo_groups_to_entities.geo_group_id = @district После обновления раскоментить 
                    AND subdivisions.id = @subdivision OR subdivisions.parent_subdivision_id = @subdivision #До обновления 
                AND store_movement_document.time_stamp &gt;  ADDDATE(CURTIME() ,INTERVAL -14 DAY)

            GROUP BY nomenclature.id
        ) AS ToAnotherWarehouse ON ToAnotherWarehouse.NomenclatureId = nomenclature.id

WHERE nomenclature.category IN ('equipment' , 'additional' )
  AND nomenclature.is_archive = false
  AND (ShippingNomenclature.NomenclatureItemSum IS NOT NULL OR Equipment.ForClientCount IS NOT NULL)

GROUP BY (nomenclature.id)
ORDER BY (NomenclatureSumForShipping) DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>={?date}</Value>
          </QueryParameter>
          <QueryParameter Name="district">
            <Value>1</Value>
          </QueryParameter>
          <QueryParameter Name="subdivision">
            <Value>22</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="NomenclatureName">
          <DataField>NomenclatureName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NomenclatureSumForShipping">
          <DataField>NomenclatureSumForShipping</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AtDriverNomenclatureCount">
          <DataField>AtDriverNomenclatureCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RouteLists">
          <DataField>RouteLists</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WarehouseDirection">
          <DataField>WarehouseDirection</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MovementItemCount">
          <DataField>MovementItemCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ForClient">
          <DataField>ForClientCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
