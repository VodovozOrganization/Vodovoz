          <DataField>regrading_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="inventory_added">
          <DataField>inventory_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="inventory_removed">
          <DataField>inventory_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="warehouse_name">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT name FROM warehouses WHERE id = @id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?warehouse_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataMovements">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    	'writeoff' as direction, 
    	warehouses.name as warehouse, 
    	store_movement_wagon.name as wagon, 
    	store_movement_document.time_stamp as send_time, 
    	store_movement_document.delivered_time,
    	GROUP_CONCAT(concat(nomenclature.name, ' - ', FORMAT(-goods_accounting_operations.amount, measurement_units.digits), measurement_units.name) SEPARATOR ', ') as cargo
    FROM goods_accounting_operations 
    INNER JOIN store_movement_document_items ON store_movement_document_items.writeoff_movement_operation_id = goods_accounting_operations.id
    LEFT JOIN nomenclature ON nomenclature.id = goods_accounting_operations.nomenclature_id
    LEFT JOIN measurement_units ON nomenclature.unit_id = measurement_units.id
    LEFT JOIN store_movement_document ON store_movement_document.id = store_movement_document_items.movement_document_id 
    LEFT JOIN warehouses ON store_movement_document.warehouse_to_id = warehouses.id
    LEFT JOIN store_movement_wagon ON store_movement_wagon.id = store_movement_document.transportation_wagon_id
    WHERE
    	goods_accounting_operations.warehouse_id = @warehouse_id
    	AND goods_accounting_operations.operation_time &gt;= @startDate
    	AND goods_accounting_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)
    	AND store_movement_document.category = 'Transportation'
    	AND
    	(
    		((nomenclature.id IN (@nomenclature_include) AND 0 IN (@nomenclature_exclude) )
    		OR
    		(nomenclature.id NOT IN (@nomenclature_exclude) AND 0 IN (@nomenclature_include)))
    		OR (0 IN (@nomenclature_include) AND 0 IN (@nomenclature_exclude))
    	)
    	AND 
    	(
    		((nomenclature.category IN (@nomenclature_type_include) AND '0' IN (@nomenclature_type_exclude) )
    		OR
    		(nomenclature.category NOT IN (@nomenclature_type_exclude) AND '0' IN (@nomenclature_type_include)))
    		OR ('0' IN (@nomenclature_type_include) AND '0' IN (@nomenclature_type_exclude))
    	)
    GROUP BY store_movement_document.id
    UNION 
    SELECT 
    	'income' as direction, 
    	warehouses.name as warehouse, 
    	store_movement_wagon.name as wagon, 
    	store_movement_document.time_stamp as send_time, 
    	store_movement_document.delivered_time,
    	GROUP_CONCAT(concat(nomenclature.name, ' - ', FORMAT(goods_accounting_operations.amount, measurement_units.digits), measurement_units.name) SEPARATOR ', ') as cargo
    FROM goods_accounting_operations 
    INNER JOIN store_movement_document_items ON store_movement_document_items.income_movement_operation_id = goods_accounting_operations.id
    LEFT JOIN nomenclature ON nomenclature.id = goods_accounting_operations.nomenclature_id
    LEFT JOIN measurement_units ON nomenclature.unit_id = measurement_units.id
    LEFT JOIN store_movement_document ON store_movement_document.id = store_movement_document_items.movement_document_id 
    LEFT JOIN warehouses ON store_movement_document.warehouse_from_id = warehouses.id
    LEFT JOIN store_movement_wagon ON store_movement_wagon.id = store_movement_document.transportation_wagon_id
    WHERE
    	goods_accounting_operations.warehouse_id = @warehouse_id
    	AND goods_accounting_operations.operation_time &gt;= @startDate
    	AND goods_accounting_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)
    	AND store_movement_document.category = 'Transportation'
    	AND
    	(
    		((nomenclature.id IN (@nomenclature_include) AND 0 IN (@nomenclature_exclude) )
    		OR
    		(nomenclature.id NOT IN (@nomenclature_exclude) AND 0 IN (@nomenclature_include)))
    		OR (0 IN (@nomenclature_include) AND 0 IN (@nomenclature_exclude))
    	)
    	AND 
    	(
    		((nomenclature.category IN (@nomenclature_type_include) AND '0' IN (@nomenclature_type_exclude) )
    		OR
    		(nomenclature.category NOT IN (@nomenclature_type_exclude) AND '0' IN (@nomenclature_type_include)))
    		OR ('0' IN (@nomenclature_type_include) AND '0' IN (@nomenclature_type_exclude))
    	)
    GROUP BY store_movement_document.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="warehouse_id">
            <Value>={?warehouse_id}</Value>
          </QueryParameter>
          <QueryParameter Name="startDate">
            <Value>={?startDate}</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>={?endDate}</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_type_include">
