  <LeftMargin>.25in</LeftMargin>
  <RightMargin>.25in</RightMargin>
  <BottomMargin>.25in</BottomMargin>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SELECT
    nomenclature.name AS nomenclature_name,
    nomenclature.category,
    measurement_units.digits,
    ng1.name as group1_name,
    ng2.name as group2_name,
    ng3.name as group3_name,
    ng4.name as group4_name,
    ng5.name as group5_name,
    ng6.name as group6_name,
    movements.*,
    IFNULL((SELECT
                SUM(amount)
            FROM
                goods_accounting_operations
            WHERE
                    goods_accounting_operations.warehouse_id = @warehouse_id
              AND goods_accounting_operations.amount &gt; 0
              AND goods_accounting_operations.nomenclature_id = nomenclature.id
              AND goods_accounting_operations.operation_time &lt; @startDate),
           0) AS before_added,
    IFNULL((SELECT
                -SUM(amount)
            FROM
                goods_accounting_operations
            WHERE
                    goods_accounting_operations.warehouse_id = @warehouse_id
              AND goods_accounting_operations.amount &lt; 0
              AND goods_accounting_operations.nomenclature_id = nomenclature.id
              AND goods_accounting_operations.operation_time &lt; @startDate),
           0) AS before_removed
FROM
    nomenclature
        LEFT JOIN
    (SELECT
         goods_accounting_operations.nomenclature_id,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND goods_accounting_operations.amount &gt; 0, goods_accounting_operations.amount, 0)) AS all_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND goods_accounting_operations.amount &lt; 0, goods_accounting_operations.amount, 0)) AS all_removed,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND store_car_unload_document_items.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS drivers_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND (store_car_load_document_items.id IS NOT NULL), goods_accounting_operations.amount, 0)) AS drivers_removed,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND store_self_delivery_document_returned.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS selfdelivery_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND store_self_delivery_document_item.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS selfdelivery_removed,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND smd_income.transportation_wagon_id IS NOT NULL, goods_accounting_operations.amount, 0)) AS wagons_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND smd_writeoff.transportation_wagon_id IS NOT NULL, goods_accounting_operations.amount, 0)) AS wagons_removed,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND store_incoming_invoice_items.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS warehouse_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND store_writeoff_document_items.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS warehouse_removed,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND srgi_income.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS regrading_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND srgi_writeoff.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS regrading_removed,
         SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND goods_accounting_operations.amount &gt; 0
            AND store_inventory_item.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS inventory_added,
         -SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
            AND goods_accounting_operations.amount &lt; 0
            AND store_inventory_item.id IS NOT NULL, goods_accounting_operations.amount, 0)) AS inventory_removed
    FROM
         goods_accounting_operations
             LEFT JOIN store_car_unload_document_items ON store_car_unload_document_items.warehouse_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_car_load_document_items ON store_car_load_document_items.warehouse_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_car_load_documents ON store_car_load_documents.id = store_car_load_document_items.car_load_document_id
             LEFT JOIN store_self_delivery_document_item ON store_self_delivery_document_item.warehouse_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_self_delivery_document_returned ON store_self_delivery_document_returned.warehouse_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_movement_document_items smdi_writeoff ON smdi_writeoff.writeoff_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_movement_document_items smdi_income ON smdi_income.income_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_movement_document smd_writeoff ON smd_writeoff.id = smdi_writeoff.movement_document_id
             LEFT JOIN store_movement_document smd_income ON smd_income.id = smdi_income.movement_document_id
             LEFT JOIN store_incoming_invoice_items ON store_incoming_invoice_items.good_move_operation_id = goods_accounting_operations.id
             LEFT JOIN store_regrading_of_goods_items srgi_income ON srgi_income.warehouse_income_operation_id = goods_accounting_operations.id
             LEFT JOIN store_regrading_of_goods_items srgi_writeoff ON srgi_writeoff.warehouse_writeoff_operation_id = goods_accounting_operations.id
             LEFT JOIN store_inventory_item ON store_inventory_item.warehouse_movement_operation_id = goods_accounting_operations.id
             LEFT JOIN store_writeoff_document_items ON store_writeoff_document_items.write_off_goods_accounting_operation_id = goods_accounting_operations.id
    WHERE
             goods_accounting_operations.operation_time &gt;= @startDate
       AND goods_accounting_operations.operation_time &lt; DATE_ADD(@endDate, INTERVAL 1 DAY)
     GROUP BY goods_accounting_operations.nomenclature_id) movements ON movements.nomenclature_id = nomenclature.id
        LEFT JOIN
    measurement_units ON measurement_units.id = nomenclature.unit_id
        LEFT JOIN 
    nomenclature_groups as ng1 ON nomenclature.group_id = ng1.id
        LEFT JOIN 
    nomenclature_groups as ng2 ON ng1.parent_id = ng2.id
        LEFT JOIN 
    nomenclature_groups as ng3 ON ng2.parent_id = ng3.id
        LEFT JOIN
    nomenclature_groups as ng4 ON ng3.parent_id = ng4.id
        LEFT JOIN
    nomenclature_groups as ng5 ON ng4.parent_id = ng5.id
        LEFT JOIN
    nomenclature_groups as ng6 ON ng5.parent_id = ng6.id
WHERE
    (
            ((nomenclature.id IN (@nomenclature_include) AND 0 IN (@nomenclature_exclude))
                OR
             (nomenclature.id NOT IN (@nomenclature_exclude) AND 0 IN (@nomenclature_include)))
            OR (0 IN (@nomenclature_include) AND 0 IN (@nomenclature_exclude))
        )
  AND
    (
            ((nomenclature.category IN (@nomenclature_type_include) AND '0' IN (@nomenclature_type_exclude))
                OR
             (nomenclature.category NOT IN (@nomenclature_type_exclude) AND '0' IN (@nomenclature_type_include)))
            OR ('0' IN (@nomenclature_type_include) AND '0' IN (@nomenclature_type_exclude))
        )
  AND (
        ((IFNULL(ng1.id , 0) IN (@product_group_include) AND 0 IN (@product_group_exclude))
            OR
         (IFNULL(ng1.id , 0) NOT IN (@product_group_exclude) AND 0 IN (@product_group_include)))
        OR (0 IN (@product_group_include) AND 0 IN (@product_group_exclude))
    )
HAVING
            before_added - before_removed &lt;&gt; 0 OR IFNULL(all_added, 0) &lt;&gt; 0 OR IFNULL(all_removed, 0) &lt;&gt; 0
ORDER BY
	CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods'     THEN nomenclature.category END, 
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN nomenclature.name END,
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN ng6.name END,
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN ng5.name END,
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN ng4.name END,
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN ng3.name END,
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN ng2.name END,
		CASE WHEN @sortType = 'Type, Alphabet, GroupOfGoods' THEN ng1.name END,
	CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet'     THEN nomenclature.category END, 
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN ng6.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN ng5.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN ng4.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN ng3.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN ng2.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN ng1.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods, Alphabet' THEN nomenclature.name END,
	CASE WHEN @sortType = 'Type, Alphabet'                   THEN nomenclature.category END,
		CASE WHEN @sortType = 'Type, Alphabet'               THEN nomenclature.name END,
	CASE WHEN @sortType = 'Type, GroupOfGoods'               THEN nomenclature.category END,
		CASE WHEN @sortType = 'Type, GroupOfGoods'           THEN ng6.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods'           THEN ng5.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods'           THEN ng4.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods'           THEN ng3.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods'           THEN ng2.name END,
		CASE WHEN @sortType = 'Type, GroupOfGoods'           THEN ng1.name END,
	CASE WHEN @sortType = 'Type'                             THEN nomenclature.category END,
	CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet'     THEN ng6.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN ng5.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN ng4.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN ng3.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN ng2.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN ng1.name END, 
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN nomenclature.category END, 
		CASE WHEN @sortType = 'GroupOfGoods, Type, Alphabet' THEN nomenclature.name END,
	CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type'     THEN ng6.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN ng5.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN ng4.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN ng3.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN ng2.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN ng1.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN nomenclature.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet, Type' THEN nomenclature.category END,
	CASE WHEN @sortType = 'GroupOfGoods, Type'               THEN ng6.name END, 
		CASE WHEN @sortType = 'GroupOfGoods, Type'           THEN ng5.name END, 
		CASE WHEN @sortType = 'GroupOfGoods, Type'           THEN ng4.name END, 
		CASE WHEN @sortType = 'GroupOfGoods, Type'           THEN ng3.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type'           THEN ng2.name END, 
		CASE WHEN @sortType = 'GroupOfGoods, Type'           THEN ng1.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Type'           THEN nomenclature.category END,
	CASE WHEN @sortType = 'GroupOfGoods, Alphabet'           THEN ng6.name END, 
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet'       THEN ng5.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet'       THEN ng4.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet'       THEN ng3.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet'       THEN ng2.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet'       THEN ng1.name END,
		CASE WHEN @sortType = 'GroupOfGoods, Alphabet'       THEN nomenclature.name END,
	CASE WHEN @sortType = 'GroupOfGoods'                     THEN ng6.name END, 
		CASE WHEN @sortType = 'GroupOfGoods'                 THEN ng5.name END,
		CASE WHEN @sortType = 'GroupOfGoods'                 THEN ng4.name END,
		CASE WHEN @sortType = 'GroupOfGoods'                 THEN ng3.name END,
		CASE WHEN @sortType = 'GroupOfGoods'                 THEN ng2.name END,
		CASE WHEN @sortType = 'GroupOfGoods'                 THEN ng1.name END,
	CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods'     THEN nomenclature.name END, 
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN nomenclature.category END,
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN ng6.name END,
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN ng5.name END,
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN ng4.name END,
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN ng3.name END,
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN ng2.name END, 
		CASE WHEN @sortType = 'Alphabet, Type, GroupOfGoods' THEN ng1.name END,
	CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type'     THEN nomenclature.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN ng6.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN ng5.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN ng4.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN ng3.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN ng2.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN ng1.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods, Type' THEN nomenclature.category END,
	CASE WHEN @sortType = 'Alphabet, Type'                   THEN nomenclature.name END,
		CASE WHEN @sortType = 'Alphabet, Type'               THEN nomenclature.category END,
	CASE WHEN @sortType = 'Alphabet, GroupOfGoods'           THEN nomenclature.name END, 
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods'       THEN ng6.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods'       THEN ng5.name END, 
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods'       THEN ng4.name END, 
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods'       THEN ng3.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods'       THEN ng2.name END,
		CASE WHEN @sortType = 'Alphabet, GroupOfGoods'       THEN ng1.name END,
	CASE WHEN @sortType = 'Alphabet' OR @sortType = ''       THEN nomenclature.name END, nomenclature.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!startDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!endDate</Value>
          </QueryParameter>
          <QueryParameter Name="warehouse_id">
            <Value>=Parameters!warehouse_id</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_type_include">
            <Value>=Parameters!nomenclature_type_include</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_type_exclude">
            <Value>=Parameters!nomenclature_type_exclude</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_include">
            <Value>=Parameters!nomenclature_include</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_exclude">
            <Value>=Parameters!nomenclature_exclude</Value>
          </QueryParameter>
          <QueryParameter Name="product_group_include">
            <Value>=Parameters!product_group_include</Value>
          </QueryParameter>
          <QueryParameter Name="product_group_exclude">
            <Value>=Parameters!product_group_exclude</Value>
          </QueryParameter>
          <QueryParameter Name="sortType">
            <Value>=Parameters!sortType</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="nomenclature">
          <DataField>nomenclature_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="digits">
          <DataField>digits</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="before_added">
          <DataField>before_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="before_removed">
          <DataField>before_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="all_added">
          <DataField>all_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="all_removed">
          <DataField>all_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="drivers_added">
          <DataField>drivers_added</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="drivers_removed">
          <DataField>drivers_removed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
