    <DataSet Name="InstanceData">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    ni.id as instance_id,
    GET_INVENTORY_NUMBER(ni.is_used, ni.inventory_number) as inventory_number,
    n.name AS nomenclature,
    ROUND(amount_in_db) as amount_in_db,
    IF(is_missing, 0, 1) as amount_in_fact,
    ROUND((select amount_in_fact) - amount_in_db) as difference,
    store_shiftchange_instance_item.comment,
    CASE n.category
        WHEN 'rent' THEN 'Аренда кулеров'
        WHEN 'water' THEN 'Вода в многооборотной таре'
        WHEN 'deposit' THEN 'Залог'
        WHEN 'spare_parts' THEN 'Запчасти сервисного центра'
        WHEN 'equipment' THEN 'Оборудование'
        WHEN 'additional' THEN 'Товары'
        WHEN 'service' THEN 'Услуга'
        WHEN 'bottle' THEN 'Тара'
        WHEN 'material' THEN 'Сырьё'
        WHEN 'disposableBottleWater' THEN 'Вода в одноразовой таре'
        WHEN 'master' THEN 'Выезд мастера'
        ELSE ''
        END as category
FROM
    store_shiftchange_instance_item
        LEFT JOIN
    nomenclature_instances ni ON store_shiftchange_instance_item.nomenclature_instance_id = ni.id
        LEFT JOIN
    nomenclature n ON n.id = ni.nomenclature_id
WHERE
    store_shiftchange_instance_item.store_shiftchange_id = @document_id
ORDER BY IF(@order_by_nomenclature_name, n.name, store_shiftchange_instance_item.id);</CommandText>
