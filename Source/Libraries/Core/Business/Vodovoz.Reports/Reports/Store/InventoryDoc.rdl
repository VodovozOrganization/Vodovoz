    <DataSet Name="Doc">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    store_inventory.id,
    time_stamp,
    (
        CASE
            WHEN w.id is not null THEN w.name
            WHEN e.id is not null THEN CONCAT(e.last_name, ' ', LEFT(e.name, 1), ' ', LEFT(e.patronymic, 1))
            WHEN c.id is not null THEN CONCAT(cm.name, ' ', c.reg_number)
            END
        )as storage,
    store_inventory.comment
FROM
    store_inventory
        LEFT JOIN
    warehouses w ON w.id = store_inventory.warehouse_id
        LEFT JOIN
    employees e ON e.id = store_inventory.employee_id
        LEFT JOIN
    cars c ON c.id = store_inventory.car_id
        LEFT JOIN
    car_models cm ON cm.id = c.model_id
WHERE
        store_inventory.id = @inventory_id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="inventory_id">
            <Value>={?inventory_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="time_stamp">
          <DataField>time_stamp</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="storage">
          <DataField>storage</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="comment">
          <DataField>comment</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
