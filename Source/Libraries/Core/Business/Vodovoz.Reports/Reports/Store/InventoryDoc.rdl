    <DataSet Name="InstancesData">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    n.name AS nomenclature,
    mu.name as measurement_unit_name,
    mu.digits as measurement_unit_digits,
    ni.id AS instance_id,
    ROUND(amount_in_db, IFNULL(mu.digits, 0)) as amount_in_db,
    GET_INVENTORY_NUMBER(ni.is_used, ni.inventory_number) as inventory_number,
    IF(is_missing, 0, 1) as amount_in_fact,
    ROUND((select amount_in_fact) - amount_in_db, IFNULL(mu.digits, 0)) as difference,
    store_inventory_instance_item.comment
FROM
    store_inventory_instance_item
        LEFT JOIN
    nomenclature_instances ni on store_inventory_instance_item.nomenclature_instance_id = ni.id
        LEFT JOIN
    nomenclature n ON ni.nomenclature_id = n.id
        LEFT JOIN
    measurement_units mu ON mu.id = n.unit_id
WHERE
    store_inventory_instance_item.store_inventory_id = @inventory_id
ORDER BY
    CASE WHEN @sorted_by_nomenclature_name THEN n.official_name END ASC,
    CASE WHEN !@sorted_by_nomenclature_name THEN n.id END ASC</CommandText>
