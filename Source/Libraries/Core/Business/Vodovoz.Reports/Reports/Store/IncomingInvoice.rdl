  <DataSets>
    <DataSet Name="DocInfo">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	doc.id AS doc_id,
	doc.time_stamp AS create_date,
	GET_PERSON_NAME_WITH_INITIALS(auth.last_name, auth.name, auth.patronymic) AS author,
	(
		SELECT GET_PERSON_NAME_WITH_INITIALS(last_name, name, patronymic)
		FROM employees e
		WHERE e.id = @printed_by_id
	) AS printed_by,
	doc.invoice_number AS number,
	doc.waybill_number AS waybill_number,
	sup.name AS counterparty,
	w.name AS warehouse,
	doc.comment AS cmnt
FROM store_incoming_invoice doc
LEFT JOIN employees auth ON auth.id = doc.author_id
LEFT JOIN counterparty sup ON sup.id = doc.counterparty_id
LEFT JOIN warehouses w ON w.id = doc.warehouse_id
WHERE doc.id = @document_id
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@document_id">
            <Value>={?document_id}</Value>
          </QueryParameter>
          <QueryParameter Name="@printed_by_id">
            <Value>={?printed_by_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="doc_id">
          <DataField>doc_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="create_date">
          <DataField>create_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="author">
          <DataField>author</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="printed_by">
          <DataField>printed_by</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="number">
          <DataField>number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="waybill_number">
          <DataField>waybill_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="counterparty">
          <DataField>counterparty</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="warehouse">
          <DataField>warehouse</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="cmnt">
          <DataField>cmnt</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DocItems">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    i.nomenclature_id AS nom_id,
    IFNULL(n.short_name, n.name) AS nomenclature,
    IFNULL(GET_INVENTORY_NUMBER(ni.is_used, ni.inventory_number), '-') AS inventory_number,
    '-' AS sn,
    (CASE i.vat
         WHEN 'No' THEN '0%'
         WHEN 'Vat10' THEN '10%'
         WHEN 'Vat18' THEN '18%'
         WHEN 'Vat20' THEN '20%'
         ELSE i.vat
        END
        ) AS vat,
    op.amount AS qty,
    u.name AS unit,
    i.price AS prime_cost,
    i.price * op.amount AS summ
FROM store_incoming_invoice_items i
         LEFT JOIN goods_accounting_operations op ON op.id = i.good_move_operation_id
         LEFT JOIN nomenclature_instances ni ON ni.id = i.nomenclature_instance_id
         LEFT JOIN nomenclature n ON n.id = i.nomenclature_id
         LEFT JOIN measurement_units u ON u.id = n.unit_id
WHERE i.incoming_invoice_id = @document_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@document_id">
            <Value>={?document_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="nom_id">
          <DataField>nom_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="nomenclature">
          <DataField>nomenclature</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="sn">
          <DataField>sn</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="vat">
          <DataField>vat</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="qty">
          <DataField>qty</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="unit">
          <DataField>unit</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="prime_cost">
          <DataField>prime_cost</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="summ">
          <DataField>summ</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="inventory_number">
          <DataField>inventory_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
