  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>180</Timeout>
        <CommandText>WITH first_orders_data as(
SELECT
    c.id AS client_id,
    c.full_name AS client_name,
    o.id AS order_id,
    o.delivery_date AS order_delivery_date,
    o.delivery_point_id AS delivery_point_id,
    o.order_status as order_status,
    IFNULL(SUM(GET_TOTAL_SUM_FOR_ORDER_ITEM(oi.id)),0) AS order_total_sum,
    IFNULL(GET_PARAMETER('AdvancedPaymentNomenclatureId'),0) as advanced_payment_sum,
	(
		SELECT
			GROUP_CONCAT(DISTINCT IFNULL(ds.name, IFNULL(ps.discount_reason_info, '')) SEPARATOR ', ')
		FROM order_items oi
		LEFT JOIN discount_reasons ds ON oi.discount_reason_id = ds.id
		LEFT JOIN promotional_sets ps ON oi.promotional_set_id = ps.id
		WHERE
			oi.order_id = o.id
			AND (ds.id is not null OR ps.id is not null)
    ) AS discount_info
FROM counterparty c
JOIN orders o ON o.client_id = c.id
JOIN order_items oi ON oi.order_id = o.id
LEFT JOIN discount_reasons dr ON oi.discount_reason_id = dr.id
WHERE
	o.delivery_date &gt;= @start_date and o.delivery_date &lt; DATE_ADD(@end_date, INTERVAL 1 DAY)
	AND o.order_status IN (@first_order_statuses)
	AND o.is_first_order = 1
	AND (@author_employer_id = 0 OR o.author_employee_id = @author_employer_id)
	AND 
    (CASE
		WHEN @discount_id = 0 AND @has_promo_set = 0 THEN o.id is not null
		WHEN @discount_id = 0 AND @has_promo_set = 1 THEN oi.promotional_set_id is not null
		WHEN @discount_id = 1 AND @has_promo_set = 0 THEN dr.id = @discount_id
		WHEN @discount_id = 1 AND @has_promo_set = 1 THEN dr.id = @discount_id OR oi.promotional_set_id is not null
	END)
GROUP BY o.id
),

second_orders_data as(
SELECT
    c.id AS client_id,
    c.full_name AS client_name,
    o.id AS order_id,
    o.delivery_date AS order_delivery_date,
    o.order_status as order_status,
    IFNULL(SUM(GET_TOTAL_SUM_FOR_ORDER_ITEM(oi.id)),0) AS order_total_sum,
    IFNULL(GET_PARAMETER('AdvancedPaymentNomenclatureId'),0) as advanced_payment_sum
FROM orders o 
JOIN counterparty c ON o.client_id = c.id
JOIN order_items oi ON oi.order_id = o.id
WHERE
	o.delivery_date &gt;= @start_date and o.delivery_date &lt; DATE_ADD(@end_date, INTERVAL 1 DAY)
	AND o.client_id IN (SELECT client_id FROM first_orders_data)
	AND NOT o.id IN (SELECT order_id FROM first_orders_data)
GROUP BY o.id
)

SELECT
	IFNULL(
            (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
             FROM phones
             WHERE delivery_point_id = fod.delivery_point_id
             AND phones.is_archive = 0),
            (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
             FROM phones
             WHERE counterparty_id = fod.client_id
             AND phones.is_archive = 0)
    ) AS phones,
    (
		SELECT
			SUM(IFNULL((CAST(deposit_operations.received_deposit AS SIGNED) - CAST(deposit_operations.refund_deposit AS SIGNED)), 0)) as Balance
		FROM deposit_operations
		WHERE deposit_operations.deposit_type = 'Bottles' AND deposit_operations.order_id = fod.order_id
		GROUP BY deposit_operations.order_id
    ) AS deposit,
	fod.client_name as client_name,
	fod.order_id as first_order_id,
	fod.order_delivery_date as first_order_delivery_date,
	fod.order_total_sum as first_order_total_sum,
	fod.discount_info as first_order_discount_info,
	sod.order_id as second_order_id,
	sod.order_delivery_date as second_order_delivery_date,
	sod.order_total_sum as second_order_total_sum
FROM first_orders_data fod
LEFT JOIN second_orders_data sod
	ON fod.client_id = sod.client_id
		AND sod.order_total_sum &gt; sod.advanced_payment_sum
		AND sod.order_status IN (@second_order_statuses)
		AND sod.order_id = (SELECT MIN(order_id) FROM second_orders_data WHERE client_id = fod.client_id)
WHERE
	fod.order_total_sum &gt; fod.advanced_payment_sum
	AND (@show_only_client_with_one_order = 0 OR sod.order_id IS NULL)
ORDER BY fod.client_id
;</CommandText>
