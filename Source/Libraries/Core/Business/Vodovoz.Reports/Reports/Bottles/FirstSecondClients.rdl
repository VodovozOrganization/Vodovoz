  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    orders.delivery_date AS delivery_date,
    counterparty.name AS client_name,
    orders.sum_to_receive ,
    OrderDeposit.Balance AS deposit,
    (
        SELECT GROUP_CONCAT(DISTINCT IFNULL(ds.name, IFNULL(ps.discount_reason_info, '')) SEPARATOR ', ')
        FROM order_items oi
            LEFT JOIN discount_reasons ds ON oi.discount_reason_id = ds.id
            LEFT JOIN promotional_sets ps ON oi.promotional_set_id = ps.id
        WHERE oi.order_id = orders.id
            AND
            (
                ds.id is not null OR ps.id is not null    
            )
    ) AS discount,
    SecondOrder.delivery_date AS secondOrderDate,
    SecondOrder.Id AS secondOrderId,
    IFNULL(TransferTo.Balance,0) + IFNULL(TransferFrom.Balance,0) + OrderBottles.Balance as BottleBalance,
    IFNULL(
            (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
             FROM phones
             WHERE delivery_point_id = orders.delivery_point_id
             AND phones.is_archive = 0),
            (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
             FROM phones
             WHERE counterparty_id = orders.client_id
             AND phones.is_archive = 0)
        ) AS phones
FROM
    orders
        JOIN order_items ON orders.id = order_items.order_id
        LEFT JOIN discount_reasons ON order_items.discount_reason_id = discount_reasons.id
        JOIN counterparty ON orders.id = counterparty.first_order_id
        LEFT JOIN phones p1 ON p1.counterparty_id = orders.client_id
        LEFT JOIN phones p2 ON p2.delivery_point_id = orders.delivery_point_id

        LEFT JOIN
    (
        SELECT
            orders.delivery_date ,
            orders.client_id ,
            orders.id as Id
        FROM orders
                 LEFT JOIN counterparty ON counterparty.id = orders.client_id
        WHERE counterparty.first_order_id != orders.id
          AND
            (orders.order_status != 'NewOrder' AND
             orders.order_status != 'WaitForPayment' AND
             orders.order_status != 'Canceled' AND
             orders.order_status != 'NotDelivered' AND
             orders.order_status != 'DeliveryCanceled'
                )
        ORDER BY orders.id DESC
    ) as SecondOrder ON SecondOrder.client_id = orders.client_id

        LEFT JOIN
    (
        SELECT
            transfer_operations.to_client_id AS counterparty_id,
            IFNULL(SUM((CAST(bottles_movement_operations.delivered AS SIGNED) - CAST(bottles_movement_operations.returned AS SIGNED))),0) as Balance
        FROM
            transfer_operations
                JOIN bottles_movement_operations ON bottles_movement_operations.id = transfer_operations.incoming_bottle_operation
        GROUP BY
            transfer_operations.to_client_id
    ) AS TransferTo ON TransferTo.counterparty_id = orders.client_id

        LEFT JOIN
    (
        SELECT
            transfer_operations.from_client_id AS counterparty_id,
            IFNULL(SUM((CAST(bottles_movement_operations.delivered AS SIGNED) - CAST(bottles_movement_operations.returned AS SIGNED))),0) as Balance
        FROM
            transfer_operations
                JOIN bottles_movement_operations ON bottles_movement_operations.id = transfer_operations.outgoing_bottle_operation
        GROUP BY
            transfer_operations.from_client_id
    ) AS TransferFrom ON TransferFrom.counterparty_id = orders.client_id

        LEFT JOIN
    (
        SELECT
            bottles_movement_operations.order_id,
            IFNULL((CAST(bottles_movement_operations.delivered AS SIGNED) - CAST(bottles_movement_operations.returned AS SIGNED)),0) as Balance
        FROM
            bottles_movement_operations
        GROUP BY
            bottles_movement_operations.order_id
    ) AS OrderBottles ON OrderBottles.order_id = orders.id

        LEFT JOIN
    (
        SELECT
            deposit_operations.order_id,
            SUM(IFNULL((CAST(deposit_operations.received_deposit AS SIGNED) - CAST(deposit_operations.refund_deposit AS SIGNED)),0)) as Balance
        FROM
            deposit_operations
        WHERE
                deposit_operations.deposit_type = 'Bottles'
        GROUP BY
            deposit_operations.order_id
    ) AS OrderDeposit ON OrderDeposit.order_id = orders.id
WHERE
    orders.delivery_date BETWEEN @start_date AND @end_date AND
    (orders.order_status != 'NewOrder' AND
     orders.order_status != 'WaitForPayment' AND
     orders.order_status != 'Canceled' AND
     orders.order_status != 'NotDelivered' AND
     orders.order_status != 'DeliveryCanceled'
        ) AND
    (CASE 
        WHEN @discount_id = 0 AND !@has_promo_set THEN discount_reasons.id is not null
        WHEN @discount_id = 0 AND @has_promo_set THEN order_items.promotional_set_id is not null
        WHEN @discount_id = 1 AND !@has_promo_set THEN discount_reasons.id = @discount_id
        WHEN @discount_id = 1 AND @has_promo_set THEN discount_reasons.id = @discount_id OR order_items.promotional_set_id is not null
    END) AND
    (@author_employer_id = 0 OR orders.author_employee_id = @author_employer_id) AND
    (@show_only_client_with_one_order = 0 OR SecondOrder.Id IS NULL) AND
    NOT EXISTS
        (
            SELECT
                *
            FROM
                orders as o
            WHERE
                    o.client_id = orders.client_id AND
                    o.delivery_date &lt; orders.delivery_date AND
                (orders.order_status != 'NewOrder' AND
                 orders.order_status != 'WaitForPayment' AND
                 orders.order_status != 'Canceled' AND
                 orders.order_status != 'NotDelivered' AND
                 orders.order_status != 'DeliveryCanceled')
        )
GROUP BY
    orders.id DESC;</CommandText>
