  <DataSets>
    <DataSet Name="Orders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	bottles_movement_operations.delivery_point_id AS pointid,
	orders.id AS ordersIdAlias,
    #Автор заказа
	CONCAT_WS(
		'.',
		NULLIF(SUBSTR(employees_author.name,1,1),''),
		NULLIF(SUBSTR(employees_author.patronymic,1,1),''),
		CONCAT(' ', employees_author.last_name)
	) AS author,
	TRIM(counterparty.full_name) AS counterparty,
    #Адрес
	delivery_points.compiled_address_short AS address,
    #Водитель
	CONCAT_WS(
		'.',
		NULLIF(SUBSTR(employees.name,1,1),''),
		NULLIF(SUBSTR(employees.patronymic,1,1),''),
		CONCAT(' ', employees.last_name)
	) AS driver_name,
	orders.comment AS comment,
    #Количество 19л бутылей
	IFNULL((
		SELECT 
			SUM(IFNULL(order_items.actual_count, 0))
		FROM
			orders
		LEFT JOIN
			order_items ON order_id = orders.id
		LEFT JOIN
			nomenclature ON nomenclature.id = order_items.nomenclature_id
		WHERE
			orders.id = ordersIdAlias AND nomenclature.category = 'water' AND NOT nomenclature.is_disposable_tare
	),0)  AS actual_count,
    #Доставлено
	delivered,
    #Возвращено
	returned,
    #Разница между ожидаемым и фактическим возвратом тары
	TRUNCATE(
		IFNULL(CAST(bottles_return AS SIGNED),0) - IFNULL(CAST(returned AS SIGNED),0),0) AS bottles,
	bottles_return,
	driver_call_id,
	CASE driver_call_type
		WHEN 'NoCall'  THEN 'не звонил'
		WHEN 'CallFromAddress' THEN 'с адреса'
		WHEN 'CallFromAnywhere' THEN 'не с адреса'
	END AS driver_call_string,
    r.name AS reason_type_status,
	comment_manager,
    op_comment,
    driver_app_comment,
	bottle_reserv,
    #Ввод остатков
IF(delivery_points.have_residue = 0, "Нет", "Да") AS enteredresidue,
	#Долг по точке доставки
	(
		SELECT 
			SUM(delivered) - SUM(returned)
		FROM
			bottles_movement_operations
		WHERE
			pointid = bottles_movement_operations.delivery_point_id
	) AS debt
FROM
	orders
LEFT JOIN
	non_return_reasons r ON tare_non_return_reason_id = r.id
LEFT JOIN
	order_items ON order_id = orders.id
LEFT JOIN
	counterparty ON counterparty.id = orders.client_id
LEFT JOIN
	delivery_points ON delivery_points.id = orders.delivery_point_id
LEFT JOIN
	route_list_addresses ON route_list_addresses.order_id = orders.id AND route_list_addresses.status &lt;&gt; 'Transfered'
LEFT JOIN
	route_lists ON route_lists.id = route_list_addresses.route_list_id
LEFT JOIN
	employees ON employees.id = route_lists.driver_id
LEFT JOIN
	bottles_movement_operations ON bottles_movement_operations.id = orders.bottles_movement_operation_id
LEFT JOIN
	deposit_operations ON deposit_operations.order_id = orders.id
LEFT JOIN
	doc_residue ON bottles_movement_operations.delivery_point_id = doc_residue.delivery_point_id
LEFT JOIN 
	cars ON cars.id = route_lists.car_id
LEFT JOIN
	employees AS employees_author ON employees_author.id = orders.author_employee_id
WHERE
	orders.delivery_date = DATE(@date)
	AND (@reason_id = -1 OR @reason_id = tare_non_return_reason_id)
	AND (@driver_id = -1 OR @driver_id = employees.id)
    AND (bottles_return &gt; returned)
	AND (	
		(	
			@driver_call = 1 
			AND type_of_use = 'CompanyLargus' 
			AND driver_call_type = 'NoCall' 
		)
		OR 
		(
			@driver_call = 2 
			AND driver_call_type = 'NoCall'  
		)
		OR 
		(
			@driver_call = 3 
			AND driver_call_type = 'CallFromAnywhere'  
		)
		OR 
		(
			@driver_call = 0 
			AND type_of_use IS NULL 
			AND driver_call_type = 'NoCall' 
		)
		OR (@driver_call = -1)
	)
	AND orders.order_status IN ('Shipped', 'Closed', 'UnloadingOnStock')
GROUP BY ordersIdAlias
ORDER BY reason_type_status
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>={?date}</Value>
          </QueryParameter>
          <QueryParameter Name="reason_id">
            <Value>={?reason_id}</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>={?driver_id}</Value>
          </QueryParameter>
          <QueryParameter Name="driver_call">
            <Value>={?driver_call}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
