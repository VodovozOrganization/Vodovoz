  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    o.id AS order_id,
    o.delivery_date AS delivery_date,
    c.name AS client_name,
    CASE c.person_type
        WHEN 'natural' THEN 'ФЛ'
        WHEN 'legal' THEN 'ЮЛ'
        else c.person_type
        END AS person_type,
    dp.compiled_address_short AS address_of_delivery,
    GROUP_CONCAT(DISTINCT nomenclature.name SEPARATOR ', ') AS nomenclatures,
    CASE o.payment_type
        WHEN 'cash' THEN 'НАЛ'
        WHEN 'cashless' THEN 'БЕЗНАЛ'
        WHEN 'ByCard' THEN 'ПО КАРТЕ'
        WHEN 'Terminal' THEN 'ТЕР-МИНАЛ'
        ELSE o.payment_type
        END AS payment_type,
    IFNULL(
            GROUP_CONCAT(
                    DISTINCT
                    CONCAT(dp_ph.number, ' ', IFNULL(dp_ph.comment,''))
                    SEPARATOR '\n'
                ),
            GROUP_CONCAT(
                    DISTINCT
                    CONCAT(c_ph.number, ' ', IFNULL(c_ph.comment,''))
                    SEPARATOR '\n'
                )
        ) AS phone,
    #GROUP_CONCAT(DISTINCT d.name SEPARATOR ', ') AS discount,
    (
        SELECT GROUP_CONCAT(DISTINCT IFNULL(ps.discount_reason_info, IFNULL(d.name, '')) SEPARATOR ', ')
        FROM order_items i
                 LEFT JOIN discount_reasons d ON i.discount_reason_id = d.id
                 LEFT JOIN promotional_sets ps on i.promotional_set_id = ps.id
        WHERE i.order_id = o.id
          AND
            (
                    d.id is not null OR i.promotional_set_id is not null
                )
    ) AS discount,
    SUM(ROUND(IFNULL(i.actual_count, i.count) * i.price - i.discount_money, 2)) AS order_sum,
    ROUND((SELECT
         SUM(order_items.count)
     FROM
         order_items 
             LEFT JOIN
         nomenclature ON nomenclature.id = order_items.nomenclature_id
     WHERE
             order_items.order_id = o.id
       AND nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol19L'), 0) as count_19l_water,
    cc.name as came_from ,
    o.comment AS comment ,
    GET_PERSON_NAME_WITH_INITIALS(e.last_name, e.name, e.patronymic) AS order_creator,
    districts.district_name as delivery_district
FROM
    orders o
        INNER JOIN order_items i ON o.id = i.order_id
        INNER JOIN nomenclature ON nomenclature.id = i.nomenclature_id
        LEFT JOIN discount_reasons d ON i.discount_reason_id = d.id
        INNER JOIN counterparty c ON o.id = c.first_order_id
        INNER JOIN delivery_points dp ON dp.id = o.delivery_point_id
        LEFT JOIN employees e ON e.id = o.author_employee_id
        LEFT JOIN counterparty_camefrom cc ON cc.id = c.counterparty_camefrom_id
        INNER JOIN districts ON districts.id = dp.district_id
        LEFT JOIN geo_groups gg ON gg.id = districts.geo_group_id
        LEFT JOIN phones dp_ph ON dp_ph.delivery_point_id = dp.id AND dp_ph.is_archive = 0
        LEFT JOIN phones c_ph ON c_ph.counterparty_id = c.id AND c_ph.is_archive = 0
WHERE
    o.delivery_date BETWEEN @start_date AND @end_date
  AND(
    CASE
        WHEN @discount_id = 0 AND !@has_promotional_sets THEN d.id is not null
        WHEN @discount_id = 0 AND @has_promotional_sets THEN d.id is not null OR i.promotional_set_id is not null
        WHEN @discount_id != 0 AND !@has_promotional_sets THEN d.id = @discount_id
        WHEN @discount_id != 0 AND @has_promotional_sets THEN d.id = @discount_id OR i.promotional_set_id is not null
        END)
  AND (@district_id = '' OR districts.id = @district_id)
  AND (@order_status = 'All' OR (o.order_status = @order_status) )
  AND (@payment_type = 'All' OR (o.payment_type  = @payment_type) )
GROUP BY o.id DESC;</CommandText>
