    <DataSet Name="Stats">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    GROUP_CONCAT(concat_ws(': ' ,stats.came_from ,stats.orders_count ) SEPARATOR ' | ') as stats
FROM
    (
        SELECT
            COUNT(DISTINCT i.order_id)  as orders_count,
            IFNULL(cc.name, 'Не указано') as came_from
        FROM
            orders o
                INNER JOIN order_items i ON o.id = i.order_id
                INNER JOIN nomenclature ON nomenclature.id = i.nomenclature_id
                LEFT JOIN discount_reasons d ON i.discount_reason_id = d.id
                INNER JOIN counterparty c ON o.id = c.first_order_id
                INNER JOIN delivery_points dp ON dp.id = o.delivery_point_id
                LEFT JOIN counterparty_camefrom cc ON cc.id = c.counterparty_camefrom_id
                LEFT JOIN employees e ON e.id = o.author_employee_id
                INNER JOIN districts ON districts.id = dp.district_id
                LEFT JOIN geo_groups gg ON gg.id = districts.geo_group_id
                LEFT JOIN phones dp_ph ON dp_ph.delivery_point_id = dp.id AND dp_ph.is_archive = 0
                LEFT JOIN phones c_ph ON c_ph.counterparty_id = c.id AND c_ph.is_archive = 0
        WHERE
            o.delivery_date BETWEEN @start_date AND @end_date
          AND(
            CASE
                WHEN @discount_id = 0 AND !@has_promotional_sets THEN d.id is not null
                WHEN @discount_id = 0 AND @has_promotional_sets THEN d.id is not null OR i.promotional_set_id is not null
                WHEN @discount_id != 0 AND !@has_promotional_sets THEN d.id = @discount_id
                WHEN @discount_id != 0 AND @has_promotional_sets THEN d.id = @discount_id OR i.promotional_set_id is not null
                END)
          AND (@district_id = '' OR districts.id = @district_id)
        GROUP BY cc.id DESC
    ) as stats;</CommandText>
