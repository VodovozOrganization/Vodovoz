  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Undeliveries">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT count(*) as amount,
    CASE g.guilty_side
        WHEN 'Client' THEN 'Клиент'
        WHEN 'Driver' THEN 'Водитель'
        WHEN 'Department' THEN IF(dep.short_name IS NULL, 'Отдел ВВ', CONCAT('Отд: ', dep.short_name))
        WHEN 'ServiceMan' THEN 'Мастер СЦ'
        WHEN 'ForceMajor' THEN 'Форс-мажор'
        WHEN 'DirectorLO' THEN 'Доставка за час'
        WHEN 'DirectorLOCurrentDayDelivery' THEN 'Директор ЛО (Доставка в тот же день)'
        WHEN 'None' THEN 'Нет (не недовоз)' 
    ELSE
        'Неизвестно'
    END 
    AS guilty,
    uo.name AS object,
    uk.name AS kind,
    ud.name AS detalization

FROM undelivered_orders un
LEFT JOIN guilty_in_undelivered_orders g ON un.id = g.undelivery_id
LEFT JOIN subdivisions dep ON dep.id = g.guilty_department_id
LEFT JOIN undelivery_detalizations ud ON ud.id = un.undelivery_detalization_id
LEFT JOIN undelivery_kinds uk ON uk.id = ud.undelivery_kind_id
LEFT JOIN undelivery_objects uo ON uo.id = uk.undelivery_object_id
LEFT JOIN orders old_order on old_order.id = un.old_order_id
LEFT JOIN orders new_order on new_order.id = un.new_order_id
LEFT JOIN employees author on author.id = un.author_employee_id

WHERE
(@driver_id = 0 OR
        un.old_order_id IN (SELECT order_id FROM route_list_addresses INNER JOIN route_lists ON route_lists.id = route_list_addresses.route_list_id WHERE route_lists.driver_id = @driver_id))
AND
    (@old_order_id = 0 OR
        un.old_order_id = @old_order_id)
AND
    (@client_id = 0 OR
        old_order.client_id = @client_id)
AND
    (@address_id = 0 OR
        old_order.delivery_point_id = @address_id)
AND
    (@author_subdivision_id = 0 OR
        author.subdivision_id = @author_subdivision_id)
AND
    (@old_order_author_id = 0 OR
        old_order.author_employee_id = @old_order_author_id)
AND 
    (old_order.delivery_date &gt;= @old_order_start_date AND old_order.delivery_date &lt;= @old_order_end_date)
AND
	((new_order.delivery_date &gt;= @new_order_start_date OR @new_order_start_date = '') AND (new_order.delivery_date &lt;= @new_order_end_date OR @new_order_end_date = ''))
AND
	(@actions_with_invoice = '' OR (@actions_with_invoice = 'createdNew' AND un.new_order_id IS NOT NULL) OR (@actions_with_invoice = 'notCreated' AND un.new_order_id IS NULL))
AND
	(g.guilty_side IN (@guilty_sides) OR '0' IN (@guilty_sides))
AND
    (un.undelivered_order_status = @old_order_status OR @old_order_status = '')
AND
	(g.guilty_department_id = @guilty_department_id OR @guilty_department_id = 0)
AND
	(un.in_process_at = @in_process_at_subdivision_id OR @in_process_at_subdivision_id = 0)
AND
	(un.status = @undelivery_status OR @undelivery_status = '')
AND
	(un.author_employee_id = @undelivery_author_id OR @undelivery_author_id = 0)

GROUP BY guilty, object, kind, detalization

ORDER BY guilty, amount desc</CommandText>
