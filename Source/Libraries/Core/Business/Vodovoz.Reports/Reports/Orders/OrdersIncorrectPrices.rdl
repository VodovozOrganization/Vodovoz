          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="mainPrice">
          <DataField>mainPrice</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="mainMinPrice">
          <DataField>mainMinPrice</DataField>
