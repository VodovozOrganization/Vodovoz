  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	o.e_shop_order AS eshop_order_id,
    n.online_store_external_id AS online_store_external_id,
    o.delivery_date AS delivery_date,
    n.official_name AS nomenclature_name,
    mu.digits AS nomenclature_count_numbers,
    COALESCE(oi.price, 0) AS nomenclature_price,
    COALESCE(oi.count, 0) AS planned_amount,
    COALESCE(oi.actual_count, 0) AS actual_amount,
    IF(d.price IS NULL, 0, (d.price - d.discount_money)) AS delivery_price,
    ROW_NUMBER() OVER(PARTITION BY o.e_shop_order) AS eshop_order_row_number,
    o.id as order_id,
    oi.id as order_item_id,
    (if(oi.id = 
    	(SELECT oi2.id
    	FROM order_items oi2
    	JOIN nomenclature n2 ON oi2.nomenclature_id = n2.id
    	WHERE
    	order_id = o.id
    	AND n2.online_store_external_id IS NOT NULL
    	ORDER BY oi2.id LIMIT 1),
    1, 0)) AS is_first_item_in_order,
    oi.discount_money AS discount,
    COALESCE(oi.price, 0) * COALESCE(oi.count, 0) - IF(oi.count &gt; 0 AND oi.price &gt; 0, oi.discount_money, 0) AS planned_sum,
    COALESCE(oi.price, 0) * COALESCE(oi.actual_count, 0) - IF(oi.actual_count &gt; 0 AND oi.price &gt; 0 , oi.discount_money, 0) AS actual_sum,
    CASE
        WHEN o.order_status = 'NewOrder' THEN 'Новый'
        WHEN o.order_status = 'WaitForPayment' THEN 'Ожидание оплаты'
        WHEN o.order_status = 'Accepted' THEN 'Принят'
        WHEN o.order_status = 'InTravelList' THEN 'В маршрутном листе'
        WHEN o.order_status = 'OnLoading' THEN 'На погрузке'
        WHEN o.order_status = 'OnTheWay' THEN 'В пути'
        WHEN o.order_status = 'Shipped' THEN 'Доставлен'
        WHEN o.order_status = 'UnloadingOnStock' THEN 'Выгрузка на складе'
        WHEN o.order_status = 'Closed' THEN 'Закрыт'
        WHEN o.order_status = 'Canceled' THEN 'Отменён'
        WHEN o.order_status = 'NotDelivered' THEN 'Недовоз'
        WHEN o.order_status = 'DeliveryCanceled' THEN 'Доставка отменена'
        ELSE ''
    END AS order_status
FROM order_items oi 
LEFT JOIN orders o ON o.id = oi.order_id
LEFT JOIN nomenclature n ON n.id = oi.nomenclature_id
LEFT JOIN measurement_units mu ON n.unit_id = mu.id 
LEFT JOIN order_items d ON oi.order_id = d.order_id AND d.nomenclature_id = (SELECT CAST(bp.str_value AS UNSIGNED INTEGER) FROM base_parameters bp WHERE bp.name = 'paid_delivery_nomenclature_id')
WHERE o.id IS NOT NULL
  AND o.e_shop_order IS NOT NULL
  AND o.delivery_date &gt;= @start_date
  AND o.delivery_date &lt;= @end_date
  AND n.online_store_external_id IS NOT NULL
  AND (@e_shop_id = -1 OR n.online_store_id = @e_shop_id)
  AND (o.order_status IN (@order_statuses))
GROUP BY oi.id
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="e_shop_id">
            <Value>=Parameters!e_shop_id</Value>
          </QueryParameter>
          <QueryParameter Name="order_statuses">
            <Value>=Parameters!order_statuses</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="eshop_order_id">
          <DataField>eshop_order_id</DataField>
          <TypeName>System.String</TypeName>
