  <DataSets>
    <DataSet Name="Orders">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    delivery_schedule.name AS date,
    (SELECT 
        cast(SUM(order_items.count) as signed)
     FROM
         order_items
             LEFT JOIN
         nomenclature ON order_items.nomenclature_id = nomenclature.id
     WHERE
             order_items.order_id = orders.id
       AND nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol19L') AS bottles,
    (SELECT 
        cast(SUM(order_items.count) as signed)
     FROM
         order_items
             LEFT JOIN
         nomenclature ON order_items.nomenclature_id = nomenclature.id
     WHERE
             order_items.order_id = orders.id
       AND nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol6L') AS bottles6,
    (SELECT
         GROUP_CONCAT(CONCAT(nomenclature.name,
                             ':',
                             order_items.count)
                      SEPARATOR ', ')
     FROM
         order_items
             LEFT JOIN
         nomenclature ON order_items.nomenclature_id = nomenclature.id
     WHERE
             order_items.order_id = route_list_addresses.order_id
       AND (nomenclature.category = 'equipment' OR nomenclature.category = 'additional')
    ) AS nom_name,
    CONCAT(orders.from_client_text,
           ':',
           orders.to_client_text, ';' ) as clients_equipment,
    compiled_address AS address,
    last_name,
    employees.name AS first_name,
    districts.district_name AS district,
    counterparty.full_name AS client_name
FROM
    orders
        LEFT JOIN
    delivery_schedule ON orders.delivery_schedule_id = delivery_schedule.id
        LEFT JOIN
    delivery_points ON delivery_point_id = delivery_points.id
        LEFT JOIN
    districts ON districts.id = delivery_points.district_id
        LEFT JOIN
    route_list_addresses ON orders.id = route_list_addresses.order_id
        LEFT JOIN
    route_lists ON route_list_addresses.route_list_id = route_lists.id
        LEFT JOIN
    employees ON route_lists.driver_id = employees.id
        LEFT JOIN
    counterparty ON counterparty.id = orders.client_id
WHERE
    (delivery_date BETWEEN @start_date AND @end_date)
  AND districts.id = @id_district
  AND (orders.order_status &lt;&gt;  'Canceled' OR orders.order_status &lt;&gt; 'DeliveryCanceled')
  AND (route_list_addresses.status &lt;&gt; 'Transfered' OR route_list_addresses.status IS NULL)
  AND NOT orders.self_delivery
  AND delivery_points.city &lt;&gt; 'Семиозерье'
ORDER BY bottles , last_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="id_district">
            <Value>=Parameters!id_district</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
