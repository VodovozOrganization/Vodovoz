  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    route_lists.id,
    route_lists.driver_id,
    GET_PERSON_NAME_WITH_INITIALS(driver.last_name, driver.name, driver.patronymic) AS driver_name,
    counterparty.name AS counterparty_name,
    delivery_points.compiled_address_short,
    delivery_schedule.name AS delivery_schedule,
    IFNULL(orders.time_delivered,
           route_list_addresses.status_last_update) AS delivered_time,
IF(is_fast_delivery,
		TIMEDIFF(IFNULL(orders.time_delivered, route_list_addresses.status_last_update),
			(IF(@interval_select_mode = 'RouteListItemTransfered',
				route_list_addresses.creation_date,
				IF(@interval_select_mode = 'AddedInFirstRouteList',
					(SELECT rla.creation_date FROM route_list_addresses rla WHERE rla.order_id = orders.id ORDER BY creation_date LIMIT 1),
					orders.create_date
					)
				)
				+ INTERVAL (SELECT str_value FROM base_parameters WHERE base_parameters.name = 'fast_delivery_time') HOUR_SECOND )),
		TIMEDIFF(TIME(IFNULL(orders.time_delivered, route_list_addresses.status_last_update)), delivery_schedule.to_time)) AS delay,

    lar.name as late_reason_name,

    route_list_addresses.comment_for_fine as lo_comment,
    GET_PERSON_NAME_WITH_INITIALS(lo_comment_author.last_name, lo_comment_author.name, lo_comment_author.patronymic
            ) AS lo_comment_author_name,

     IFNULL(fine.total_money + ' р.','')  as fines
FROM
    route_lists
        INNER JOIN employees driver ON
                driver.id = route_lists.driver_id
        LEFT JOIN route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
        LEFT JOIN orders ON orders.id = route_list_addresses.order_id
        LEFT JOIN counterparty ON counterparty.id = orders.client_id
        LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
        LEFT JOIN delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
        LEFT JOIN late_arrival_reasons lar on route_list_addresses.late_arrival_reason_id = lar.id
        LEFT JOIN employees lo_comment_author ON lo_comment_author.id = route_list_addresses.comment_for_fine_author_id
        LEFT OUTER JOIN fines_to_route_list_addresses fine_route_list_adrs on route_list_addresses.id = fine_route_list_adrs.route_list_address_id
        LEFT OUTER JOIN fines fine on fine_route_list_adrs.fine_id = fine.id
        LEFT JOIN districts ON delivery_points.district_id = districts.id
        INNER JOIN cars c ON route_lists.car_id = c.id
        INNER JOIN car_models cm ON c.model_id = cm.id
        INNER JOIN car_versions cv ON c.id = cv.car_id
        	AND cv.start_date &lt;= route_lists.date
	        AND (cv.end_date IS NULL OR cv.end_date &gt;= route_lists.date)
WHERE route_lists.`date` BETWEEN @start_date AND @end_date
  AND (@order_select_mode = 'All' OR ((@order_select_mode = 'WithoutDeliveryInAnHour' AND is_fast_delivery = 0) OR (@order_select_mode = 'DeliveryInAnHour' AND is_fast_delivery = 1)))
  AND route_list_addresses.status = 'Completed'
  AND (@geographic_group_id = 0 OR districts.geo_group_id = @geographic_group_id)
  AND (
         IF('Delivery' IN (@RouteListOwnType_include), !driver.visiting_master AND cm.car_type_of_use != 'Truck' AND orders.order_address_type = 'Delivery' , '0' IN (@RouteListOwnType_include))
            OR IF('ChainStore' IN (@RouteListOwnType_include), !driver.visiting_master AND cm.car_type_of_use != 'Truck' AND orders.order_address_type = 'ChainStore', '0' IN (@RouteListOwnType_include))
            OR IF('Trucks' IN (@RouteListOwnType_include), cm.car_type_of_use = 'Truck', '0' IN (@RouteListOwnType_include))
            OR IF('ServiceCenter' IN (@RouteListOwnType_include), driver.visiting_master, '0' IN (@RouteListOwnType_include))
            OR IF('StorageLogistics' IN (@RouteListOwnType_include), orders.order_address_type = 'StorageLogistics', '0' IN (@RouteListOwnType_include))
        )
GROUP BY route_list_addresses.id
HAVING delay &gt; 0;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="geographic_group_id">
            <Value>={?geographic_group_id}</Value>
          </QueryParameter>
          <QueryParameter Name="order_select_mode">
            <Value>={?order_select_mode}</Value>
          </QueryParameter>
          <QueryParameter Name="interval_select_mode">
            <Value>={?interval_select_mode}</Value>
          </QueryParameter>
          <QueryParameter Name="RouteListOwnType_include">
            <Value>={?RouteListOwnType_include}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="driver_id">
          <DataField>driver_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="driver_name">
          <DataField>driver_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="counterparty_name">
          <DataField>counterparty_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="compiled_address_short">
          <DataField>compiled_address_short</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="delivery_schedule">
          <DataField>delivery_schedule</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="delivered_time">
          <DataField>delivered_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="delay">
          <DataField>delay</DataField>
          <rd:TypeName>System.TimeSpan</rd:TypeName>
        </Field>
        <Field Name="lo_comment">
          <DataField>lo_comment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lo_comment_author_name">
          <DataField>lo_comment_author_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="late_reason_name">
          <DataField>late_reason_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fines">
          <DataField>fines</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="k_adresam">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    count(*) as kadresam
FROM
    route_list_addresses
LEFT JOIN orders o on route_list_addresses.order_id = o.id
LEFT JOIN delivery_points ON delivery_points.id = o.delivery_point_id
LEFT JOIN districts ON delivery_points.district_id = districts.id

WHERE o.order_status != 'NotDelivered' and o.order_status != 'Canceled'
    AND delivery_date &gt;= @start_date
    AND delivery_date &lt;= @end_date
    AND (@geographic_group_id = 0 OR districts.geo_group_id = @geographic_group_id)</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="geographic_group_id">
            <Value>={?geographic_group_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="kadresam">
          <DataField>kadresam</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="LatenessSummary">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	Sum(Delays.delay &lt; '00:05:00') AS LessThan5Minutes,
	Sum(Delays.delay &lt; '00:30:00' AND Delays.delay &gt;= '00:05:00') AS LessThan30Minutes,
	Sum(Delays.delay &gt;= '00:30:00') AS MoreThan30Minutes,
	Sum((TIME(delivered_time) &gt;= '07:00:00') AND (TIME(delivered_time) &lt;= '18:00:00')) AS AtDay,
	SUM((TIME(delivered_time) &lt; '07:00:00' OR TIME(delivered_time) &gt; '18:00:00')) AS AtEvening
	FROM (
	SELECT
		route_lists.id AS routeList_id,
	    delivery_schedule.name AS delivery_schedule,
	    IFNULL(orders.time_delivered,
	           route_list_addresses.status_last_update) AS delivered_time,
		IF(is_fast_delivery,
			TIMEDIFF(IFNULL(orders.time_delivered, route_list_addresses.status_last_update),
				(IF(@interval_select_mode = 'RouteListItemTransfered',
					route_list_addresses.creation_date,
					IF(@interval_select_mode = 'AddedInFirstRouteList',
						(SELECT rla.creation_date FROM route_list_addresses rla WHERE rla.order_id = orders.id ORDER BY creation_date LIMIT 1),
						orders.create_date
						)
					)
					+ INTERVAL (SELECT str_value FROM base_parameters WHERE base_parameters.name = 'fast_delivery_time') HOUR_SECOND )),
			TIMEDIFF(TIME(IFNULL(orders.time_delivered, route_list_addresses.status_last_update)), delivery_schedule.to_time)) AS delay
	FROM route_lists
	        INNER JOIN employees driver
		 			ON driver.id = route_lists.driver_id
					AND driver.visiting_master = 0
	        LEFT JOIN route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
	        LEFT JOIN orders ON orders.id = route_list_addresses.order_id
	        LEFT JOIN counterparty ON counterparty.id = orders.client_id
	        LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
	        LEFT JOIN delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
	        LEFT JOIN late_arrival_reasons lar on route_list_addresses.late_arrival_reason_id = lar.id
	        LEFT JOIN employees lo_comment_author ON lo_comment_author.id = route_list_addresses.comment_for_fine_author_id
	        LEFT OUTER JOIN fines_to_route_list_addresses fine_route_list_adrs on route_list_addresses.id = fine_route_list_adrs.route_list_address_id
	        LEFT OUTER JOIN fines fine on fine_route_list_adrs.fine_id = fine.id
	        LEFT JOIN districts ON delivery_points.district_id = districts.id
	        INNER JOIN cars c ON route_lists.car_id = c.id
	        INNER JOIN car_models cm ON c.model_id = cm.id
	        INNER JOIN car_versions cv ON c.id = cv.car_id
        		AND cv.start_date &lt;= route_lists.date
            	AND (cv.end_date IS NULL OR cv.end_date &gt;= route_lists.date)
	WHERE route_lists.`date` BETWEEN @start_date AND @end_date
	  AND (@order_select_mode = 'All'
	  	OR ((@order_select_mode = 'WithoutDeliveryInAnHour' AND is_fast_delivery = 0)
		OR (@order_select_mode = 'DeliveryInAnHour' AND is_fast_delivery = 1)))
	  AND route_list_addresses.status = 'Completed'
	  AND (@geographic_group_id = 0 OR districts.geo_group_id = @geographic_group_id)
      AND (
           IF('Delivery' IN (@RouteListOwnType_include), !driver.visiting_master AND cm.car_type_of_use != 'Truck' AND orders.order_address_type = 'Delivery' , '0' IN (@RouteListOwnType_include))
              OR IF('Trucks' IN (@RouteListOwnType_include), cm.car_type_of_use = 'Truck', '0' IN (@RouteListOwnType_include))
              OR IF('ServiceCenter' IN (@RouteListOwnType_include), driver.visiting_master, '0' IN (@RouteListOwnType_include))
              OR IF('StorageLogistics' IN (@RouteListOwnType_include), orders.order_address_type = 'StorageLogistics', '0' IN (@RouteListOwnType_include))
          )	  
	GROUP BY route_list_addresses.id
	HAVING delay &gt; 0) AS Delays;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="geographic_group_id">
            <Value>={?geographic_group_id}</Value>
          </QueryParameter>
          <QueryParameter Name="order_select_mode">
            <Value>={?order_select_mode}</Value>
          </QueryParameter>
          <QueryParameter Name="interval_select_mode">
            <Value>={?interval_select_mode}</Value>
          </QueryParameter>
          <QueryParameter Name="RouteListOwnType_include">
            <Value>={?RouteListOwnType_include}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="LessThan5Minutes">
          <DataField>LessThan5Minutes</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="LessThan30Minutes">
          <DataField>LessThan30Minutes</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="MoreThan30Minutes">
