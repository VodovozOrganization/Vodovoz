  <DataSets >
    <DataSet Name="summary_report">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>60</Timeout>
        <CommandText>SELECT cm.car_type_of_use, cv.car_own_type, COUNT(c.id) as cars_count
FROM cars c
JOIN car_models cm ON c.model_id = cm.id 
JOIN car_versions cv ON c.id = cv.car_id 
WHERE
	c.is_archive = 0
    AND cv.start_date &lt;= @date
    AND (cv.end_date is null OR cv.end_date &gt; @date)
    #AND cm.car_type_of_use IN (@car_type_of_use)
    #AND cv.car_own_type IN (@car_own_type)
GROUP BY cm.car_type_of_use, cv.car_own_type;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@date">
            <Value>={?date}</Value>
          </QueryParameter>
          <QueryParameter Name="@car_type_of_use">
            <Value>={?car_type_of_use}</Value>
          </QueryParameter>
          <QueryParameter Name="@car_own_type">
            <Value>={?car_own_type}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="car_type_of_use">
          <DataField>car_type_of_use</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="car_own_type">
          <DataField>car_own_type</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="cars_count">
          <DataField>cars_count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="main">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>60</Timeout>
        <CommandText>SELECT cm.car_type_of_use, cv.car_own_type, COUNT(c.id) as cars_count
FROM cars c
JOIN car_models cm ON c.model_id = cm.id 
JOIN car_versions cv ON c.id = cv.car_id 
WHERE
	c.is_archive = 0
    AND cv.start_date &lt;= @date
    AND (cv.end_date is null OR cv.end_date &gt; @date)
    #AND cm.car_type_of_use IN (@car_type_of_use)
    #AND cv.car_own_type IN (@car_own_type)
GROUP BY cm.car_type_of_use, cv.car_own_type</CommandText>
        <QueryParameters>
          <QueryParameter Name="@date">
            <Value>={?date}</Value>
          </QueryParameter>
          <QueryParameter Name="@car_type_of_use">
            <Value>={?car_type_of_use}</Value>
          </QueryParameter>
          <QueryParameter Name="@car_own_type">
            <Value>={?car_own_type}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
