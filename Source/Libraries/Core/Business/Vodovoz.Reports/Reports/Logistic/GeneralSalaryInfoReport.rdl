    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>120</Timeout>
        <CommandText>SELECT
    master.emplID as empl_id,
    master.frlDate as first_rl,
    master.emplName as empl_name,
    master.emplCategory as category,
    master.emplDriverOf as driver_of,
    master.works_withName as works_with_name,
    -- Рабочие дни и адреса
    servant.countWD as days_worked,
    servant.addrCount as addresses_total,
    ROUND(servant.addrCount / servant.countWD, 0) as addresses_avg,
    -- Полные бутыли
    IFNULL(servant.delCount, 0) as filled_bottles,
    ROUND(IFNULL(servant.delCount, 0) / servant.countWD, 0) as filled_avg,
    -- Пустые бутыли
    IFNULL(servant.retCount, 0) as empty_bottles,
    ROUND(IFNULL(servant.retCount, 0) / servant.countWD, 0) as empty_avg,
    -- Расчет зп
    ROUND(master.moneySum, 0) as salary_fact,
    ROUND(master.moneySum / servant.countWD, 0) as salary_fact_avg,
    ROUND(master.moneySum / servant.countWD, 0) * 22 as estimated_salary
FROM
    (
        SELECT
            empl.id as emplID,
            frl.date as frlDate,
            CONCAT_WS('', CONCAT(empl.last_name, ' '), if(empl.name = '', '', CONCAT(LEFT(empl.name, 1),'.')), if(empl.patronymic = '', '', CONCAT(LEFT(empl.patronymic, 1),'.'))
                ) as emplName,
            (CASE empl.category
                 WHEN 'driver' THEN 'Вод.'
                 WHEN 'forwarder' THEN 'Эксп.'
                END) as emplCategory,
            IF(cm.id IS NOT NULL AND cv.id IS NOT NULL,
               CONCAT(
                       CASE cm.car_type_of_use
                           WHEN 'Largus' THEN 'Л'
                           WHEN 'Truck' THEN 'Ф'
                           WHEN 'GAZelle' THEN 'Г'
                           ELSE 'Неизв.'
                           END,
                       CASE cv.car_own_type
                           WHEN 'Company' THEN 'К'
                           WHEN 'Raskat' THEN 'Р'
                           WHEN 'Driver' THEN 'В'
                           ELSE 'Неизв.'
                           END
                   ),
               CONCAT(
                       CASE empl.driver_of_car_type_of_use
                           WHEN 'Largus' THEN 'Л'
                           WHEN 'Truck' THEN 'Ф'
                           WHEN 'GAZelle' THEN 'Г'
                           WHEN NULL THEN '-'
                           ELSE 'Неизв.'
                           END,
                       CASE empl.driver_of_car_own_type
                           WHEN 'Company' THEN 'К'
                           WHEN 'Raskat' THEN 'Р'
                           WHEN 'Driver' THEN 'В'
                           WHEN NULL THEN '-'
                           ELSE 'Неизв.'
                           END
                   )
                ) AS emplDriverOf,
            CONCAT_WS('', CONCAT(works_with.last_name, ' '), IF(works_with.name = '', '', CONCAT(LEFT(works_with.name, 1),'.')), IF(works_with.patronymic = '', '', CONCAT(LEFT(works_with.patronymic, 1),'.'))
                ) as works_withName,
           (
                Select SUM(IF(rl.`status` = 'Closed',
                    wmo.money,
                    IF(empl.category='driver',
                        (
                            SELECT SUM(rla.driver_wage + rla.driver_wage_surcharge)
                            FROM route_list_addresses rla
                            WHERE rla.status IN ('Completed', 'EnRoute')
                                and rla.route_list_id = rl.id
                            GROUP BY rla.route_list_id
                        ),
                        (
                            SELECT SUM(rla.forwarder_wage) AS planForwarderSum
                            FROM route_list_addresses rla
                            WHERE rla.status IN ('Completed', 'EnRoute')
                                and rla.route_list_id = rl.id
                            GROUP BY rla.route_list_id
                        )
                    )
                ))
            ) AS moneySum
        FROM employees empl
                 LEFT JOIN employees works_with ON (works_with.id = empl.default_forwarder_id AND empl.category ='driver')
            OR (works_with.default_forwarder_id = empl.id AND empl.category ='forwarder')
                 INNER JOIN route_lists rl ON rl.status &lt;&gt; 'New'  AND rl.date &gt;= @start_date AND rl.date &lt;= @end_date
            AND ((empl.category='forwarder' AND rl.forwarder_id = empl.id)
                OR (empl.category='driver' AND rl.driver_id = empl.id))
                 LEFT JOIN route_lists frl ON frl.id = (SELECT rl1.id FROM route_lists AS rl1 WHERE empl.id = rl1.driver_id ORDER BY rl1.date limit 1)
                 LEFT JOIN wages_movement_operations wmo ON wmo.id = IF(empl.category='driver', rl.driver_wages_movement_operations_id, rl.forwarder_wages_movement_operations_id)
                 LEFT JOIN cars c ON c.driver_id = empl.id
                 LEFT JOIN car_models cm ON c.model_id = cm.id
                 LEFT JOIN car_versions cv ON
                    c.id = cv.car_id
                AND cv.start_date &lt;= CURRENT_TIMESTAMP()
                AND (cv.end_date IS NULL OR cv.end_date &gt;= CURRENT_TIMESTAMP())
        WHERE
            (IF(@employee_id != '', empl.id = @employee_id, TRUE))
          AND (
                  CASE @employee_category
                      WHEN '' THEN empl.category IN ('driver', 'forwarder')
                      WHEN 'driver' AND @exclude_visiting_masters THEN empl.category = 'driver' AND !empl.visiting_master
                      WHEN 'driver' AND !@exclude_visiting_masters THEN empl.category = 'driver'
                      ELSE empl.category = 'forwarder'
                      END
              )
          AND (@driver_of_car_own_type = '' OR empl.driver_of_car_own_type = @driver_of_car_own_type)
          AND (@driver_of_car_type_of_use = '' AND empl.driver_of_car_type_of_use != 'Truck'
            OR empl.driver_of_car_type_of_use = @driver_of_car_type_of_use)
        GROUP BY empl.id
        ORDER BY empl.last_name
    ) as master
        INNER JOIN
    (
        SELECT
            empl.id as emplID,
            COUNT(rla.id) as addrCount,
            (
                Select SUM(oi.count)
                From order_items oi
                Join nomenclature n on n.id = oi.nomenclature_id and n.category = 'water' and !n.is_disposable_tare
                where oi.order_id = o.id) as orderBottles,
            (Select SUM(IF(rl.status = 'Closed', bmo.delivered, orderBottles))) AS delCount,
            SUM(IF(rl.status = 'Closed', bmo.returned, o.bottles_return)) AS retCount,
            COUNT(DISTINCT(DAY(rl.date))) as countWD
        FROM employees empl
                 INNER JOIN route_lists rl ON rl.status &lt;&gt; 'New'  AND rl.date  &gt;= @start_date AND rl.date  &lt;= @end_date
            AND ((empl.category='forwarder' AND rl.forwarder_id = empl.id)
                OR (empl.category='driver' AND rl.driver_id = empl.id))
                 INNER JOIN route_list_addresses rla ON rla.route_list_id = rl.id AND rla.status IN ('Completed', 'EnRoute')
                 LEFT JOIN bottles_movement_operations bmo ON bmo.order_id = rla.order_id
                 LEFT JOIN orders o ON o.id = rla.order_id
        WHERE
            (IF(@employee_id != '', empl.id = @employee_id, TRUE))
          AND (
            CASE @employee_category
                WHEN '' THEN empl.category IN ('driver', 'forwarder')
                WHEN 'driver' AND @exclude_visiting_masters THEN empl.category = 'driver' AND !empl.visiting_master
                WHEN 'driver' AND !@exclude_visiting_masters THEN empl.category = 'driver'
                ELSE empl.category = 'forwarder'
                END
            )
          AND (@driver_of_car_own_type = '' OR empl.driver_of_car_own_type = @driver_of_car_own_type)
          AND (@driver_of_car_type_of_use = '' AND empl.driver_of_car_type_of_use != 'Truck'
            OR empl.driver_of_car_type_of_use = @driver_of_car_type_of_use)
        GROUP BY empl.id
       ORDER BY empl.last_name
    ) as servant on servant.emplID = master.emplID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@driver_of_car_own_type">
            <Value>={?driver_of_car_own_type}</Value>
          </QueryParameter>
          <QueryParameter Name="@employee_category">
            <Value>={?employee_category}</Value>
          </QueryParameter>
          <QueryParameter Name="@creation_date">
            <Value>={?creation_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@employee_id">
            <Value>={?employee_id}</Value>
          </QueryParameter>
          <QueryParameter Name="@driver_of_car_type_of_use">
            <Value>={?driver_of_car_type_of_use}</Value>
          </QueryParameter>
          <QueryParameter Name="exclude_visiting_masters">
            <Value>={?exclude_visiting_masters}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
