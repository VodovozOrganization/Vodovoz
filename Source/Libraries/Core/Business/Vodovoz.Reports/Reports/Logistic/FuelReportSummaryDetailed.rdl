    <DataSet Name="FuelWriteoffDocuments">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	fuel_writeoff_documents.id as document_id,
    fuel_writeoff_documents.date as document_date,
    GET_PERSON_NAME_WITH_INITIALS(cashiers.last_name, cashiers.name, cashiers.patronymic) as cashier_fio,
	GET_PERSON_NAME_WITH_INITIALS(employees.last_name, employees.name, employees.patronymic) as employee_fio,
    subdivisions.name as subdivision,
    fuel_writeoff_documents.reason as reason,
    IFNULL(IF(fuel_writeoff_document_items.fuel_type_id = 3, fuel_writeoff_document_items.liters, 0), 0) as liters_diesel, 
    IFNULL(IF(fuel_writeoff_document_items.fuel_type_id = 1, fuel_writeoff_document_items.liters, 0), 0) as liters_benzin,
    financial_categories_groups.title as expense_category
FROM
	fuel_writeoff_documents
		LEFT JOIN
	financial_categories_groups ON financial_categories_groups.id = fuel_writeoff_documents.financial_expense_category_id
		LEFT JOIN
	fuel_writeoff_document_items ON fuel_writeoff_document_items.fuel_writeoff_document_id = fuel_writeoff_documents.id
		LEFT JOIN
	employees as cashiers ON cashiers.id = fuel_writeoff_documents.cashier_id
    	LEFT JOIN
	employees ON employees.id = fuel_writeoff_documents.employee_id    
    	LEFT JOIN
	subdivisions ON subdivisions.id = fuel_writeoff_documents.cash_subdivision_id
WHERE
	Date(fuel_writeoff_documents.date) &gt;= @start_date AND Date(fuel_writeoff_documents.date) &lt;= @end_date
	AND  (@author = -1 OR fuel_writeoff_documents.cashier_id = @author)
    AND 0 IN (@include_car_models)
GROUP BY fuel_writeoff_documents.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="author">
            <Value>={?author}</Value>
          </QueryParameter>
          <QueryParameter Name="include_car_models">
            <Value>={?include_car_models}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="document_date">
          <DataField>document_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="cashier_fio">
          <DataField>cashier_fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="employee_fio">
          <DataField>employee_fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="reason">
          <DataField>reason</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="liters_benzin">
          <DataField>liters_benzin</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="liters_diesel">
          <DataField>liters_diesel</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="expense_category">
          <DataField>expense_category</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
