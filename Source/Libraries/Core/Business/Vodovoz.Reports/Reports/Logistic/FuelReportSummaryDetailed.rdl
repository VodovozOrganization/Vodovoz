  <DataSets>
    <DataSet Name="Fuel">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>600</Timeout>
        <CommandText>#Получаем данные по операциям с топливом в ДВ
WITH fuel_operations_data as
(SELECT
	fuel_documents.car_id as car_id,
	fuel_documents.driver_id as driver_id,
    fuel_documents.date AS date,
    fuel_documents.fuel_limits AS liters_limits,
    IFNULL(fuel_operations.liters_outlayed, 0) AS liters_outlayed,
    route_lists.id AS route_list_id,
    route_lists.date AS route_list_date,
    fuel_documents.fuel_type_id AS fuel_type,
    fuel_documents.author_id as author_id,
    fuel_documents.last_editor_id as last_editor_id,
    fuel_documents.last_edit_date AS last_edit_date,
    fuel_documents.fuel_card AS fuel_card_number,
    IF(fuel_documents.fuel_payment_type = 'Cash', fuel_documents.payed_for_fuel, 0) AS payed_for_fuel,
    IF(fuel_documents.fuel_payment_type = 'Cashless', fuel_documents.payed_for_fuel, 0) AS cashless_payed_for_fuel,
    IFNULL(gived_operation.liters_gived,0) - IFNULL(fuel_documents.fuel_limits,0) AS liters_payed
FROM
fuel_documents
LEFT JOIN route_lists ON route_lists.id = fuel_documents.route_list_id
LEFT JOIN fuel_operations ON route_lists.fuel_outlayed_operation_id = fuel_operations.id
LEFT JOIN fuel_operations AS gived_operation ON fuel_documents.fuel_operation_id = gived_operation.id
WHERE
  fuel_documents.date &gt;= @start_date
  AND fuel_documents.date &lt; @end_date),

#Устанавливаем порядоквый номер МЛ за день для каждого автомобиля для сопоставления с транзакциями
fuel_operations_data_with_daily_number as
(
SELECT fod.*,
(
	SELECT
	COUNT(DISTINCT fod1.route_list_id)+1
	FROM fuel_operations_data fod1
	WHERE fod1.route_list_date = fod.route_list_date AND fod1.car_id=fod.car_id AND fod1.route_list_id &lt; fod.route_list_id
) as daily_rl_number,
#Проверяем, выдано ли топливо для данного МЛ в первый раз
(
	SELECT
	IF(COUNT(DISTINCT fod1.route_list_id)&gt;0,0,1)
	FROM fuel_operations_data fod1
	WHERE fod1.date &lt; fod.date AND fod1.route_list_id = fod.route_list_id
) as is_fuel_issued_first_time_by_rl
FROM
fuel_operations_data fod
),

#Получаем данные по транзакциям за указанный период
transactions_data AS
(
SELECT
	fuel_transactions.transaction_date as transaction_date,
	fuel_transactions.id as transaction_id,
	fuel_transactions.quantity as fuel_transactions_quantity,
	fuel_transactions.total_sum  as fuel_transactions_total_sum,
	fuel_transactions.card_number as fuel_card_number,
	fuel_card_versions.car_id as car_id,
	cars.driver_id as driver_id
FROM 
	fuel_transactions
	LEFT JOIN fuel_cards ON fuel_cards.card_id = fuel_transactions.card_id 
	LEFT JOIN fuel_card_versions ON fuel_cards.id = fuel_card_versions.fuel_card_id 
        AND fuel_card_versions.start_date &lt;= fuel_transactions.transaction_date 
        AND (fuel_card_versions.end_date IS NULL OR fuel_card_versions.end_date &gt;= fuel_transactions.transaction_date)
    LEFT JOIN cars ON fuel_card_versions.car_id = cars.id 
WHERE 
	fuel_transactions.transaction_date &gt;= @start_date
    AND fuel_transactions.transaction_date &lt;= @end_date
),

#Устанавливаем порядоквый номер транзакции за день для каждого автомобиля для сопоставления с операциями
transactions_data_numered as
(
SELECT
	DATE(transactions_data.transaction_date) as transaction_date,
	transactions_data.transaction_id,
	transactions_data.fuel_transactions_quantity,
	transactions_data.fuel_transactions_total_sum,
	transactions_data.fuel_card_number,
	transactions_data.car_id,
	transactions_data.driver_id as driver_id,
(
	SELECT
	COUNT(DISTINCT td.transaction_id)+1
	FROM transactions_data td
	WHERE DATE(td.transaction_date) = DATE(transactions_data.transaction_date) AND td.car_id=transactions_data.car_id AND td.transaction_date &lt; transactions_data.transaction_date
) as daily_transaction_number
FROM transactions_data
),

#Совмещаем данные по выдаче топлива и данные по транзакциям. Выполняем LEFT JOIN, поэтому часть транзакций в выборку не попадет
fuel_operations_part_of_transactions AS 
(
SELECT fod.*,
td.fuel_transactions_quantity,
td.fuel_transactions_total_sum,
td.transaction_id
FROM
	fuel_operations_data_with_daily_number fod
LEFT JOIN transactions_data_numered td 
	ON fod.car_id = td.car_id AND fod.route_list_date = td.transaction_date AND fod.daily_rl_number = td.daily_transaction_number AND fod.is_fuel_issued_first_time_by_rl = 1
),

#Оставшиеся не добавленными после LEFT JOIN транзакции добавляем в выборку
#Транзакция по возможности прикрепляется к последнему МЛ автомобиля, по которому была выдача топлива за день,
#но при этом значения выданного и истраченного топлива устанавливаем 0
fuel_operations_transactions as 
(
SELECT *
FROM 
(
SELECT 
	fot.car_id,
	fot.driver_id,
    fot.date,
    fot.liters_limits,
    fot.liters_outlayed,
    fot.route_list_id,
    fot.route_list_date,
    fot.fuel_type,
    fot.author_id,
    fot.last_editor_id,
    fot.last_edit_date,
    fot.fuel_card_number,
    fot.payed_for_fuel,
    fot.cashless_payed_for_fuel,
    fot.liters_payed,
    IFNULL(fot.fuel_transactions_quantity,0) as fuel_transactions_quantity,
	IFNULL(fot.fuel_transactions_total_sum,0) as fuel_transactions_total_sum
FROM
fuel_operations_part_of_transactions fot
UNION
SELECT 
	IFNULL(td.car_id,0) as car_id,
    IFNULL((SELECT driver_id FROM fuel_operations_part_of_transactions WHERE route_list_date = td.transaction_date AND car_id = td.car_id ORDER BY route_list_id DESC LIMIT 1),td.driver_id) as driver_id,
    td.transaction_date as date,
    0 as liters_limits,
    0 as liters_outlayed,
    IFNULL((SELECT route_list_id FROM fuel_operations_part_of_transactions WHERE route_list_date = td.transaction_date AND car_id = td.car_id ORDER BY route_list_id DESC LIMIT 1), 'Топлива в день заправки не выдавалось') as route_list_id,
    IFNULL((SELECT route_list_date FROM fuel_operations_part_of_transactions WHERE route_list_date = td.transaction_date AND car_id = td.car_id ORDER BY route_list_id DESC LIMIT 1), '') as route_list_date,
	'' as fuel_type,
    0 as author_id,
    0 as last_editor_id,
    '' as last_edit_date,
    td.fuel_card_number,
    0 as payed_for_fuel,
    0 as cashless_payed_for_fuel,
    0 as liters_payed,
    td.fuel_transactions_quantity,
	td.fuel_transactions_total_sum
FROM
transactions_data_numered td
WHERE NOT td.transaction_id IN (SELECT transaction_id FROM fuel_operations_part_of_transactions WHERE NOT transaction_id IS NULL)) t
),

#Получаем все необходимые данные и выполняем фильтрацию
result_data AS 
(
SELECT
	fuel_operations_transactions.car_id,
	fuel_operations_transactions.driver_id,
    DATE_FORMAT(fuel_operations_transactions.date, '%d.%m.%Y') as date,
    fuel_operations_transactions.liters_limits,
    fuel_operations_transactions.liters_outlayed,
    fuel_operations_transactions.route_list_id,
    DATE_FORMAT(fuel_operations_transactions.route_list_date, '%d.%m.%Y') as route_list_date,
    fuel_operations_transactions.fuel_type,
    fuel_operations_transactions.author_id,
    fuel_operations_transactions.last_editor_id,
    DATE_FORMAT(fuel_operations_transactions.last_edit_date, '%d.%m.%Y %H:%i:%s') as last_edit_date,
    fuel_operations_transactions.fuel_card_number,
    fuel_operations_transactions.payed_for_fuel,
    fuel_operations_transactions.cashless_payed_for_fuel,
    fuel_operations_transactions.liters_payed,
    fuel_operations_transactions.fuel_transactions_quantity,
	fuel_operations_transactions.fuel_transactions_total_sum,
	CONCAT_WS(' ', driver.last_name, driver.name) AS driver_name,
	GET_PERSON_NAME_WITH_INITIALS(author.last_name, author.name, author.patronymic) AS author_name,
	GET_PERSON_NAME_WITH_INITIALS(last_editor.last_name, last_editor.name, last_editor.patronymic) AS last_editor_name,
	IFNULL(car_versions.car_own_type,'no_car_own_type') as car_own_type,
	IFNULL(car_models.car_type_of_use,'no_car_type_of_use') as car_type_of_use,
	IFNULL(driver.category,'no_driver_category') as driver_category
FROM
	fuel_operations_transactions
	LEFT JOIN cars ON fuel_operations_transactions.car_id = cars.id
	LEFT JOIN employees AS driver ON driver.id = fuel_operations_transactions.driver_id
	LEFT JOIN employees AS author ON author.id = fuel_operations_transactions.author_id
	LEFT JOIN employees AS last_editor ON last_editor.id = fuel_operations_transactions.last_editor_id
	LEFT JOIN car_models ON cars.model_id = car_models.id
	LEFT JOIN car_versions ON cars.id = car_versions.car_id
        AND car_versions.start_date &lt;= fuel_operations_transactions.date
        AND (car_versions.end_date IS NULL OR car_versions.end_date &gt;= DATE(fuel_operations_transactions.date))
WHERE 
	(@author = - 1 OR author_id = @author)
    AND (
         (0 IN (@include_car_models) OR (car_models.id IN (@include_car_models)))
             AND (car_models.id IS NULL OR NOT (car_models.id IN (@exclude_car_models)))
      )
)
SELECT * FROM result_data
ORDER BY result_data.car_id, result_data.date;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="author">
            <Value>={?author}</Value>
          </QueryParameter>
          <QueryParameter Name="include_car_models">
            <Value>={?include_car_models}</Value>
          </QueryParameter>
          <QueryParameter Name="exclude_car_models">
            <Value>={?exclude_car_models}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
