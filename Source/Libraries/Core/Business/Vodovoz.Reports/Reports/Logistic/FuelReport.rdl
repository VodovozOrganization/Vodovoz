  <DataSets>
    <DataSet Name="Fuel">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SET @fuel_calibration_date := (
	SELECT start_date
	FROM car_events
	WHERE
		car_id = @car_id
		AND car_event_type_id = GET_PARAMETER('FuelBalanceCalibrationCarEventTypeId')
	    AND start_date &lt; @start_date
	ORDER BY id DESC
	LIMIT 1
);

SET @fuel_calibration_actual_balance := (
	SELECT actual_fuel_balance
	FROM car_events
	WHERE
		car_id = @car_id
		AND car_event_type_id = GET_PARAMETER('FuelBalanceCalibrationCarEventTypeId')
	    AND start_date &lt; @start_date
	ORDER BY id DESC
	LIMIT 1
);

SET @fuel_calibration_operation_id := (
	SELECT calibration_fuel_operation_id
	FROM car_events
	WHERE
		car_id = @car_id
		AND car_event_type_id = GET_PARAMETER('FuelBalanceCalibrationCarEventTypeId')
	    AND start_date &lt; @start_date
	ORDER BY id DESC
	LIMIT 1
);

WITH start_balance AS (SELECT
	CAST(IFNULL(@fuel_calibration_actual_balance, 0) + gived_to_fuel_card.liters_gived - fuel_operations_outlayed.liters_outlayed - written_off_without_rl.liters_outlayed AS DECIMAL(10,2)) as balance
FROM
(
# Выдано на топливную карту:
	(SELECT
		IFNULL(SUM(ft.quantity), 0) as liters_gived
	FROM fuel_transactions ft
	JOIN fuel_cards fc ON ft.card_id = fc.card_id 
	JOIN fuel_card_versions fcv ON fc.id = fcv.fuel_card_id AND fcv.start_date &lt;= ft.transaction_date  AND (fcv.end_date &gt; ft.transaction_date OR fcv.end_date is null)
	WHERE
		fcv.car_id  = @car_id
		AND (@fuel_calibration_date is null OR ft.transaction_date &gt;= @fuel_calibration_date)
		AND ft.transaction_date &lt; @start_date
	) as gived_to_fuel_card,

# Потрачено
	(SELECT
		IFNULL(SUM(fuel_operations_out.liters_outlayed),0) AS liters_outlayed
	FROM
		(    
			SELECT 
				fuel_operations.id,
				fuel_operations.date,
				fuel_operations.driver_id,
				fuel_operations.liters_outlayed,
				fuel_operations.car_id,
				route_lists.id as route_listID,
				route_lists.date as route_listDate,
				route_lists.driver_id as rl_driver
			FROM fuel_operations
			LEFT JOIN route_lists ON fuel_operations.id = route_lists.fuel_outlayed_operation_id
			WHERE fuel_operations.is_fine = 0
		) as fuel_operations_out
	WHERE	
		DATE(fuel_operations_out.date) &lt; @start_date
		AND NOT (fuel_operations_out.id = @fuel_calibration_operation_id)
		AND (@fuel_calibration_date is null OR fuel_operations_out.date &gt;= @fuel_calibration_date)
		AND ((fuel_operations_out.driver_id = @driver_id) OR (@car_id &lt;&gt; -1 AND fuel_operations_out.car_id = @car_id))) as fuel_operations_outlayed,

# Списание километража без МЛ
	(SELECT
	        IFNULL(SUM(mwo.liters_outlayed),0) AS liters_outlayed
		FROM 
			mileage_write_off mwo 
			LEFT JOIN employees e ON mwo.driver_id = e.id 
			LEFT JOIN cars c on mwo.car_id = c.id 
		WHERE
			mwo.write_off_date &lt; @start_date
			AND (@fuel_calibration_date is null OR mwo.write_off_date &gt;= @fuel_calibration_date)
	        AND ((@driver_id &lt;&gt; -1 AND mwo.driver_id = @driver_id) OR (@car_id &lt;&gt; -1 AND mwo.car_id = @car_id))
	    ) as written_off_without_rl
)),

#Получаем данные по операциям с топливом в ДВ
all_fuel_operations as
((SELECT
        date,
        route_list_id,
        route_list_date,
        driver,
        MAX(distance) as distance,
        SUM(liters_outlayed) as liters_outlayed,
        SUM(liters_gived) as liters_gived,
        SUM(payed_for_fuel) as payed_for_fuel,
        SUM(cashless_payed_for_fuel) as cashless_payed_for_fuel,
        SUM(liters_limits) as liters_limits,
        reg_number,
        car_id,
        is_fine,
        is_fuel_calibration,
        calibration_actual_fuel_balance
    FROM
        (
            #Выбираем полученное топливо
            (SELECT
                DATE(fuel_operations.date) AS date,
                IFNULL(route_lists.id, IF(car_events.id IS NULL,'Коррек-ка','Калибровка баланса')) AS route_list_id,
                route_lists.date AS route_list_date,
                CONCAT_WS(' ', employees.last_name, employees.name) AS driver,
                0 as distance, # Расстояние берется из таблицы с потраченным топливом
                IFNULL(fuel_operations.liters_outlayed,0) AS liters_outlayed,
                IFNULL(fuel_operations.liters_gived,0) AS liters_gived,
                IF(fuel_documents.fuel_payment_type = 'Cash', fuel_documents.payed_for_fuel, 0) AS payed_for_fuel,
                IF(fuel_documents.fuel_payment_type = 'Cashless', fuel_documents.payed_for_fuel, 0) AS cashless_payed_for_fuel,
                IFNULL(fuel_documents.gived_fuel_limits_amount, 0) AS liters_limits,
                cars.reg_number AS reg_number,
                cars.id AS car_id,
                0 as is_fine,
                IF(car_events.id IS NULL, 0, 1) as is_fuel_calibration,
                IF(car_events.id IS NULL, 0, car_events.actual_fuel_balance) as calibration_actual_fuel_balance
	            FROM
                fuel_operations
                LEFT JOIN fuel_documents ON fuel_operations.id = fuel_documents.fuel_operation_id
                LEFT JOIN route_lists ON fuel_documents.route_list_id = route_lists.id
                LEFT JOIN employees ON IFNULL(fuel_operations.driver_id, route_lists.driver_id) = employees.id
                LEFT JOIN cars ON IFNULL(fuel_operations.car_id, route_lists.car_id) = cars.id
                LEFT JOIN car_events ON car_events.calibration_fuel_operation_id = fuel_operations.id
            WHERE
                DATE(fuel_operations.date) &gt;= @start_date
                AND DATE(fuel_operations.date) &lt;= @end_date
                AND (fuel_operations.liters_gived &gt; 0 OR fuel_operations.payed_liters &gt; 0) AND fuel_operations.liters_outlayed = 0
                AND ((@driver_id &lt;&gt; -1 AND fuel_operations.driver_id = @driver_id) 
                        OR (@car_id &lt;&gt; -1 AND fuel_operations.car_id = @car_id))
            )
            #Конец выбора полученного топлива
        UNION ALL
            #Выбираем потраченное топливо
            (SELECT
                DATE(fuel_operations.date) AS date,
                IFNULL(route_lists.id, IF(fuel_operations.is_fine,'Штраф', IF(car_events.id IS NULL,'Коррек-ка','Калибровка баланса'))) AS route_list_id,
                route_lists.date AS route_list_date,
                CONCAT_WS(' ', employees.last_name, employees.name) AS driver,
                IF(route_lists.confirmed_distance = 0 OR route_lists.confirmed_distance is null, route_lists.actual_distance , route_lists.confirmed_distance) AS distance,
                IFNULL(fuel_operations.liters_outlayed,0) AS liters_outlayed,
                IFNULL(fuel_operations.liters_gived,0)    AS liters_gived,
                IF(fuel_documents.fuel_payment_type = 'Cash', fuel_documents.payed_for_fuel, 0) AS payed_for_fuel,
                IF(fuel_documents.fuel_payment_type = 'Cashless', fuel_documents.payed_for_fuel, 0) AS cashless_payed_for_fuel,
                IFNULL(fuel_documents.gived_fuel_limits_amount, 0) AS liters_limits,
                cars.reg_number AS reg_number,
                cars.id AS car_id,
                fuel_operations.is_fine as is_fine,
                IF(car_events.id IS NULL, 0, 1) as is_fuel_calibration,
                IF(car_events.id IS NULL, 0, car_events.actual_fuel_balance) as calibration_actual_fuel_balance
             FROM
                fuel_operations
                LEFT JOIN fuel_documents ON fuel_operations.id = fuel_documents.fuel_operation_id
                LEFT JOIN route_lists ON fuel_operations.id = route_lists.fuel_outlayed_operation_id
                LEFT JOIN employees ON IFNULL(fuel_operations.driver_id, route_lists.driver_id) = employees.id
                LEFT JOIN cars ON IFNULL(fuel_operations.car_id, route_lists.car_id) = cars.id 
                LEFT JOIN car_events ON car_events.calibration_fuel_operation_id = fuel_operations.id
            WHERE
                DATE(fuel_operations.date) &gt;= @start_date
                AND DATE(fuel_operations.date) &lt;= @end_date
                AND fuel_operations.liters_outlayed &gt; 0 AND fuel_operations.liters_gived = 0
                AND ((@driver_id &lt;&gt; -1 AND fuel_operations.driver_id = @driver_id) 
                        OR (@car_id &lt;&gt; -1 AND fuel_operations.car_id = @car_id))
            )
            #Конец выбора потраченного топлива
        ) as fuelOperations
        GROUP BY date, route_list_id
        ORDER BY date, route_list_id)        
	    UNION ALL
	    #Выбираем списание километража без МЛ
	    (SELECT    	
	        DATE(mwo.write_off_date) AS date,
	        'Списание километража без МЛ' AS route_list_id,
	        NULL AS route_list_date,
	        CONCAT_WS(' ', e.last_name, e.name) AS driver,
	        mwo.distance_km as distance, # Расстояние берется из таблицы с потраченным топливом
	        mwo.liters_outlayed AS liters_outlayed,
	        0 AS liters_gived,
	        0 AS payed_for_fuel,
	        0 AS cashless_payed_for_fuel,
            0 AS liters_limits,
	        cars.reg_number AS reg_number,
	        cars.id AS car_id,
            0 as is_fine,
            0 as is_fuel_calibration,
            0 as calibration_actual_fuel_balance
		FROM 
			mileage_write_off mwo 
			LEFT JOIN employees e ON mwo.driver_id = e.id 
			LEFT JOIN cars on mwo.car_id = cars.id 
		WHERE
			mwo.write_off_date &gt;= @start_date AND mwo.write_off_date &lt;= @end_date
	        AND ((@driver_id &lt;&gt; -1 AND mwo.driver_id = @driver_id) 
	                OR (@car_id &lt;&gt; -1 AND mwo.car_id = @car_id))
	    )
	    #Конец выбора списания километража без МЛ
),

#Нумеруем операции
fuel_operations_data AS
(
SELECT
    afo.date as date,
    afo.route_list_id as route_list_id,
    afo.route_list_date as route_list_date,
    afo.driver as driver,
    afo.distance as distance,
    afo.liters_outlayed as liters_outlayed,
    afo.liters_gived as liters_gived,
    afo.payed_for_fuel as payed_for_fuel,
    afo.cashless_payed_for_fuel as cashless_payed_for_fuel,
    afo.liters_limits as liters_limits,
    afo.reg_number as reg_number,
    afo.car_id as car_id,
    afo.is_fine as is_fine,
    afo.is_fuel_calibration as is_fuel_calibration,
    afo.calibration_actual_fuel_balance,
	#Устанавливаем порядковый номер МЛ за день для сопоставления с транзакциями
	(SELECT COUNT(*)+1 FROM all_fuel_operations WHERE NOT route_list_date IS NULL AND afo.route_list_date = route_list_date AND afo.route_list_id &gt; route_list_id) as daily_rl_number
FROM
	all_fuel_operations afo    
),

#Получаем данные по транзакциям за указанный период для выбранного авто
all_transactions AS
(
SELECT
	fuel_transactions.transaction_date as transaction_date,
	fuel_transactions.id as transaction_id,
	fuel_transactions.card_id as card_id,
	fuel_transactions.quantity as fuel_transactions_quantity,
	fuel_transactions.total_sum  as fuel_transactions_total_sum,
	cars.reg_number as reg_number,
	cars.id as car_id,
	CONCAT_WS(' ', employees.last_name, employees.name) as driver,
	gfp.gazprom_product_name as gazprom_product_name
FROM 
	fuel_transactions
	LEFT JOIN fuel_cards ON fuel_cards.card_id = fuel_transactions.card_id 
	LEFT JOIN fuel_card_versions ON fuel_cards.id = fuel_card_versions.fuel_card_id 
        AND fuel_card_versions.start_date &lt;= fuel_transactions.transaction_date 
        AND (fuel_card_versions.end_date IS NULL OR fuel_card_versions.end_date &gt;= fuel_transactions.transaction_date )
    LEFT JOIN cars ON cars.id = fuel_card_versions.car_id 
	LEFT JOIN employees ON cars.driver_id = employees.id 
	LEFT JOIN car_models ON cars.model_id = car_models.id
	LEFT JOIN car_versions ON cars.id = car_versions.car_id
        AND car_versions.start_date &lt;= fuel_transactions.transaction_date
        AND (car_versions.end_date IS NULL OR car_versions.end_date &gt;= fuel_transactions.transaction_date)
    LEFT JOIN gazprom_fuel_products gfp ON product_id = gfp.gazprom_product_id
WHERE 
	fuel_transactions.transaction_date &gt;= @start_date
    AND fuel_transactions.transaction_date &lt;= @end_date
    AND ((@driver_id &lt;&gt; -1 AND cars.driver_id  = @driver_id) 
            OR (@car_id &lt;&gt; -1 AND cars.id  = @car_id))
ORDER BY fuel_transactions.transaction_date
),

#Нумеруем транзакции
transactions_data AS
(
SELECT at.*,
##Устанавливаем порядковый номер транзакции за день для сопоставления с МЛ
(SELECT COUNT(*)+1 FROM all_transactions WHERE at.transaction_date &gt; transaction_date AND transaction_date &gt;= DATE(at.transaction_date)) as daily_transaction_number
FROM all_transactions at
),

#Совмещаем данные по МЛ и данные по транзакциям. Выполняем LEFT JOIN, поэтому часть транзакций в выборку не попадет
route_lists_part_of_transactions AS 
(
SELECT
	fod.date,
	fod.route_list_id,
	fod.route_list_date,
	fod.driver,
	fod.distance,
	fod.liters_outlayed,
	fod.liters_gived,
	fod.payed_for_fuel,
	fod.cashless_payed_for_fuel,
	fod.liters_limits,
	fod.reg_number,
	fod.car_id,
	fod.is_fine,
	fod.is_fuel_calibration,
    fod.calibration_actual_fuel_balance,
	td.fuel_transactions_quantity,
	td.fuel_transactions_total_sum,
	td.transaction_id,
	td.gazprom_product_name
FROM  
	fuel_operations_data fod
LEFT JOIN transactions_data td
	ON fod.date = DATE(td.transaction_date) AND fod.route_list_date = DATE(td.transaction_date) AND fod.daily_rl_number = td.daily_transaction_number
),

#Оставшиеся не добавленными после LEFT JOIN транзакции добавляем в выборку
#Транзакция по возможности прикрепляется к последнему МЛ, по которому была выдача топлива за день,
#но при этом значения выданного и истраченного топлива устанавливаем 0, чтобы повторно не отражались на балансе
route_lists_transactions AS 
(
(SELECT
	rlt.date,
	rlt.route_list_id,
	rlt.route_list_date,
	rlt.driver,
	rlt.distance,
	rlt.liters_outlayed,
	rlt.liters_gived,
	rlt.payed_for_fuel,
	rlt.cashless_payed_for_fuel,
	rlt.liters_limits,
	rlt.reg_number,
	rlt.car_id,
	rlt.is_fine,
	rlt.is_fuel_calibration,
    rlt.calibration_actual_fuel_balance,
	IFNULL(rlt.fuel_transactions_quantity,0) as fuel_transactions_quantity,
	IFNULL(rlt.fuel_transactions_total_sum,0) as fuel_transactions_total_sum,
	rlt.gazprom_product_name
FROM route_lists_part_of_transactions rlt)
UNION ALL
(SELECT
	td.transaction_date as date,
	IFNULL((SELECT route_list_id FROM route_lists_part_of_transactions WHERE route_list_date = DATE(td.transaction_date) ORDER BY route_list_id DESC LIMIT 1), 'Топлива в день заправки не выдавалось') as route_list_id,
	IFNULL((SELECT route_list_date FROM route_lists_part_of_transactions WHERE route_list_date = DATE(td.transaction_date) ORDER BY route_list_id DESC LIMIT 1), '') as route_list_date,
	IFNULL((SELECT driver FROM route_lists_part_of_transactions WHERE route_list_date = DATE(td.transaction_date) ORDER BY route_list_id DESC LIMIT 1), td.driver) as driver,
	0 as distance,
	0 as liters_outlayed,
	0 as liters_gived,
	0 as payed_for_fuel,
	0 as cashless_payed_for_fuel,
	0 as liters_limits,
	IFNULL((SELECT reg_number FROM route_lists_part_of_transactions WHERE route_list_date = td.transaction_date ORDER BY route_list_id DESC LIMIT 1), td.reg_number) as reg_number,
	IFNULL((SELECT car_id FROM route_lists_part_of_transactions WHERE route_list_date =  DATE(td.transaction_date) ORDER BY route_list_id DESC LIMIT 1), td.car_id) as car_id,
	0 as is_fine,
	0 as is_fuel_calibration,
    0 as calibration_actual_fuel_balance,
	td.fuel_transactions_quantity,
	td.fuel_transactions_total_sum,
	td.gazprom_product_name
FROM transactions_data td
WHERE td.transaction_id NOT IN (SELECT transaction_id FROM route_lists_part_of_transactions WHERE NOT transaction_id IS NULL))
ORDER BY date, is_fuel_calibration DESC, route_list_id
),

#Добавляем расчет баланса в строках
#Списание топлива в штрафах не учитываем
route_lists_transactions_balance as
(
SELECT
	DATE_FORMAT(route_lists_transactions.date, '%d.%m.%Y') as date,
	route_lists_transactions.route_list_id,
	DATE_FORMAT(route_lists_transactions.route_list_date, '%d.%m.%Y') as route_list_date,
	route_lists_transactions.driver,
	route_lists_transactions.distance,
	route_lists_transactions.liters_outlayed,
	route_lists_transactions.liters_gived,
	ROUND(
        route_lists_transactions.fuel_transactions_quantity + 
        IFNULL(
            route_lists_transactions.payed_for_fuel / (
                SELECT IF(f.quantity &gt; 0, f.total_sum / f.quantity, NULL)
                FROM fuel_transactions f
                WHERE f.card_id = (
                    SELECT fc.card_id
                    FROM fuel_cards fc
                    LEFT JOIN fuel_card_versions fcv ON fc.id = fcv.fuel_card_id
                    WHERE fcv.car_id = route_lists_transactions.car_id
                      AND NOT(fcv.start_date &gt; route_lists_transactions.date)
                      AND (fcv.end_date IS NULL OR fcv.end_date &gt;= route_lists_transactions.date)
                    LIMIT 1
                )
                  AND NOT(f.transaction_date &gt; route_lists_transactions.date)
                ORDER BY f.transaction_date DESC
                LIMIT 1
            ),
            0
        ),
        2
    ) AS fuel_transactions_quantity,
	route_lists_transactions.payed_for_fuel,
	route_lists_transactions.cashless_payed_for_fuel,
	route_lists_transactions.liters_limits,
	route_lists_transactions.reg_number,
	route_lists_transactions.is_fine,
	route_lists_transactions.is_fuel_calibration,
    route_lists_transactions.calibration_actual_fuel_balance,
	route_lists_transactions.fuel_transactions_total_sum,
	route_lists_transactions.gazprom_product_name,
	@start_balance as start_balance,
    ROUND(@fuel_balance := 
			IF(
				route_lists_transactions.is_fuel_calibration=1,
				route_lists_transactions.calibration_actual_fuel_balance,
				@fuel_balance
	    			- IF(route_lists_transactions.is_fine=0,route_lists_transactions.liters_outlayed,0)
	    			+ IF(route_lists_transactions.is_fuel_calibration=0,0,route_lists_transactions.liters_gived)
	    			+ route_lists_transactions.fuel_transactions_quantity)
		, 2) AS balance
FROM 
	route_lists_transactions,
    #Выбирается баланс на начало выбранного периода
	(SELECT @start_balance := IFNULL((SELECT balance FROM start_balance LIMIT 1), 0)) as StartBalance,
    (SELECT @fuel_balance:= @start_balance ) as TableBalance
    #Конец выбора баланса
)
SELECT * FROM route_lists_transactions_balance
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="car_id">
            <Value>=Parameters!car_id</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="route_list_id">
          <DataField>route_list_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="route_list_date">
          <DataField>route_list_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="driver">
          <DataField>driver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="distance">
          <DataField>distance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="liters_outlayed">
          <DataField>liters_outlayed</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="liters_gived">
          <DataField>liters_gived</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="payed_for_fuel">
          <DataField>payed_for_fuel</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="reg_number">
          <DataField>reg_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="balance">
          <DataField>balance</DataField>
          <TypeName>System.Double</TypeName>
        </Field>
        <Field Name="cashless_payed_for_fuel">
          <DataField>cashless_payed_for_fuel</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="fuel_transactions_quantity">
          <DataField>fuel_transactions_quantity</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="fuel_transactions_total_sum">
          <DataField>fuel_transactions_total_sum</DataField>
