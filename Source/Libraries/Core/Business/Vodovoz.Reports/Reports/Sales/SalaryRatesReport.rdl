    <DataSet Name="GetSalaryRates">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT * FROM
(SELECT
        wdlrs.name AS name,
        CASE wdlr.car_type_of_use
            WHEN 'Largus' THEN 'Легковая (Ларгус)'
            WHEN 'Minivan' THEN 'Фургон (Transit Mini)'
            WHEN 'GAZelle' THEN 'Грузовой (Газель)'
            ELSE wdlr.car_type_of_use
        END as car_type_of_use,
       'С экспедитором' as with_out_forwarder,
       SUM(IF(wr.wage_rate_type = 'PhoneCompensation' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as PhoneCompensationCity,
       SUM(IF(wr.wage_rate_type = 'Bottle19L' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as Bottle19LCity,
       SUM(IF(wr.wage_rate_type = 'EmptyBottle19L' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as EmptyBottle19LCity,
       SUM(IF(wr.wage_rate_type = 'Bottle19LInBigOrder' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as Bottle19LInBigOrderCity,
       SUM(IF(wr.wage_rate_type = 'EmptyBottle19LInBigOrder' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as EmptyBottle19LInBigOrderCity,
       SUM(IF(wr.wage_rate_type = 'MinBottlesQtyInBigOrder' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as MinBottlesQtyInBigOrderCity,
       SUM(IF(wr.wage_rate_type = 'Bottle6L' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as Bottle6LCity,
       SUM(IF(wr.wage_rate_type = 'PackOfBottles600ml' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as PackOfBottles600mlCity,
       SUM(IF(wr.wage_rate_type = 'Bottle1500ml' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as Bottle1500mlCity,
       SUM(IF(wr.wage_rate_type = 'Bottle500ml' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as Bottle500mlCity,
       SUM(IF(wr.wage_rate_type = 'Equipment' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as EquipmentCity,
       SUM(IF(wr.wage_rate_type = 'Address' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as AddressCity,
       SUM(IF(wr.wage_rate_type = 'ForeignAddress' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as ForeignAddressCity,
       SUM(IF(wr.wage_rate_type = 'ContractCancelation' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as ContractCancelationCity,
       SUM(IF(wr.wage_rate_type = 'FastDelivery' and wd.id = @cityId, wr.for_driver_with_forwarder, 0)) as FastDeliveryCity,
       SUM(IF(wr.wage_rate_type = 'PhoneCompensation' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as PhoneCompensationSuburb,
       SUM(IF(wr.wage_rate_type = 'Bottle19L' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as Bottle19LSuburb,
       SUM(IF(wr.wage_rate_type = 'EmptyBottle19L' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as EmptyBottle19LSuburb,
       SUM(IF(wr.wage_rate_type = 'Bottle19LInBigOrder' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as Bottle19LInBigOrderSuburb,
