       SUM(IF(wr.wage_rate_type = 'FastDeliveryWithLate' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as FastDeliveryWithLateSuburb
from wage_rates wr
left join wage_district_level_rate wdlr on wr.wage_district_level_rate_id = wdlr.id
