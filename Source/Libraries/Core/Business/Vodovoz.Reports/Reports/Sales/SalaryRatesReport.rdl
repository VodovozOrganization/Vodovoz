       SUM(IF(wr.wage_rate_type = 'MinBottlesQtyInBigOrder' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as MinBottlesQtyInBigOrderSuburb,
       SUM(IF(wr.wage_rate_type = 'Bottle6L' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as Bottle6LSuburb,
       SUM(IF(wr.wage_rate_type = 'PackOfBottles600ml' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as PackOfBottles600mlSuburb,
       SUM(IF(wr.wage_rate_type = 'Bottle1500ml' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as Bottle1500mlSuburb,
       SUM(IF(wr.wage_rate_type = 'Bottle500ml' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as Bottle500mlSuburb,
       SUM(IF(wr.wage_rate_type = 'Equipment' and wd.id = @suburbId, wr.for_driver_with_forwarder, 0)) as EquipmentSuburb,
