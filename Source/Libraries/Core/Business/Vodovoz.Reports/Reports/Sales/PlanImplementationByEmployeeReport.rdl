  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>120</Timeout>
        <CommandText>SELECT
    o.id AS order_id,
    c.name AS client,
    IFNULL(GROUP_CONCAT(DISTINCT items.nomenclature SEPARATOR ', '), 'Нет заказа воды 19 л.') AS nomenclature,
    CAST(IFNULL(SUM(items.count), 0) AS SIGNED) AS count,
    CAST(IFNULL(SUM(items.total_sum), 0) AS SIGNED) AS total_sum,
    IFNULL(o.bottles_return, 0) AS bottles_to_return,
    IFNULL(IF(o.self_delivery, o.returned_tare, a.bottles_returned),0) + IFNULL(o.bottles_by_stock_actual_count, 0) AS bottles_returned,
    CAST(ROUND(100*IFNULL(SUM(items.count), 0)/w.full_quantity, 2) AS SIGNED) AS full_sold_pct,
    CAST(ROUND(
                    100
                    *
                    (
                            IFNULL(
                                    IF(
                                            o.self_delivery,
                                            o.returned_tare,
                                            a.bottles_returned
                                        ),
                                    0
                                )
                            +
                            IFNULL(
                                    o.bottles_by_stock_actual_count,
                                    0
                                )
                        )
                /
                    w.empty_quantity,
                    2
        ) AS SIGNED) AS btls_returned_pct,
    o.order_status != 'Closed' AND a.route_list_id IS NOT NULL AS unclosed_rl,
    o.order_status != 'Closed' AND a.route_list_id IS NULL AS unclosed_not_in_rl,
    CONCAT('МЛ№', rl.id, ', водитель - ',GET_PERSON_NAME_WITH_INITIALS(e.last_name, e.name, e.patronymic)) AS rl_details,
    o.comment_manager,
    IFNULL(additions.adds_sum, 0) as adds_sum,
    auth.id as auth_id,
    GET_PERSON_NAME_WITH_INITIALS(auth.last_name, auth.name, auth.patronymic) AS auth_name,
    w.full_quantity AS full_plan,
    w.empty_quantity AS empty_plan
FROM orders o
         LEFT JOIN (
    SELECT
        o.id AS order_id,
        GET_SHORT_OR_REGULAR_NAME_FOR_NOMENCLATURE(n.id) AS nomenclature,
        IFNULL(i.actual_count, i.count) AS count,
        GET_TOTAL_SUM_FOR_ORDER_ITEM(i.id) AS total_sum,
        n.id AS nom_id
    FROM orders o
             LEFT JOIN order_items i ON o.id = i.order_id
             LEFT JOIN nomenclature n ON n.id = i.nomenclature_id
    WHERE (IF(@is_creation_date,
              o.create_date BETWEEN @start_date AND @end_date,
              o.delivery_date BETWEEN @start_date AND @end_date))
      AND (IF(@is_creation_date,
              o.order_status NOT IN('Canceled', 'DeliveryCanceled', 'NotDelivered'),
              o.order_status NOT IN('NewOrder','Canceled', 'DeliveryCanceled', 'NotDelivered', 'WaitForPayment')
                  OR (o.order_status = 'WaitForPayment' AND o.self_delivery AND o.pay_after_shipment)))
      AND (
            ((IF(o.author_employee_id IS NULL, 0, o.author_employee_id) IN (@order_author_include) AND 0 IN (@order_author_exclude) )
                OR
             (IF(o.author_employee_id IS NULL, 0, o.author_employee_id) NOT IN (@order_author_exclude) AND 0 IN (@order_author_include)))
            OR (0 IN (@order_author_include) AND 0 IN (@order_author_exclude))
        )
      AND !o.is_contract_closer
      AND n.category = 'water' AND n.tare_volume = 'Vol19L'
) AS items ON o.id = items.order_id
         LEFT JOIN ( -- расчет дополнительных продаж
    SELECT
        SUM(GET_TOTAL_SUM_FOR_ORDER_ITEM(oi.id)) as adds_sum,
        o.id as order_id
    FROM orders o
             LEFT JOIN order_items oi ON o.id = oi.order_id
             LEFT JOIN nomenclature adds ON adds.id = oi.nomenclature_id
    WHERE (IF(@is_creation_date,
              o.create_date BETWEEN @start_date AND @end_date,
              o.delivery_date BETWEEN @start_date AND @end_date))
      AND (IF(@is_creation_date,
              o.order_status NOT IN('Canceled', 'DeliveryCanceled', 'NotDelivered'),
              o.order_status NOT IN('NewOrder','Canceled', 'DeliveryCanceled', 'NotDelivered', 'WaitForPayment')
                  OR (o.order_status = 'WaitForPayment' AND o.self_delivery AND o.pay_after_shipment)))
      AND (
            ((IF(o.author_employee_id IS NULL, 0, o.author_employee_id) IN (@order_author_include) AND 0 IN (@order_author_exclude) )
                OR
             (IF(o.author_employee_id IS NULL, 0, o.author_employee_id) NOT IN (@order_author_exclude) AND 0 IN (@order_author_include)))
            OR (0 IN (@order_author_include) AND 0 IN (@order_author_exclude))
        )
      AND !o.is_contract_closer
      AND (adds.category != 'water' OR adds.tare_volume != 'Vol19L')
    GROUP BY o.id
) AS additions on additions.order_id = o.id
         LEFT JOIN employees auth ON auth.id = o.author_employee_id
         INNER JOIN (
    SELECT
        sales_plans.empty_bottles_to_take_salesplan_wage AS empty_quantity,
        sales_plans.full_bottles_to_sell_salesplan_wage AS full_quantity,
        w.employee_id
    FROM wage_parameters w
             LEFT JOIN wage_parameter_items wpi ON wpi.id = w.wage_parameter_item_id
             LEFT JOIN sales_plans ON sales_plans.id = wpi.sales_plan_id
    WHERE wpi.wage_parameter_item_type = 'SalesPlan'
      AND (w.end_date IS NULL OR w.end_date &gt;= CURDATE())
) w ON w.employee_id = auth.id
         LEFT JOIN route_list_addresses a ON a.order_id = o.id
         INNER JOIN counterparty c ON c.id = o.client_id
         LEFT JOIN route_lists rl ON a.route_list_id = rl.id
         LEFT JOIN employees e ON e.id = rl.driver_id
WHERE (IF(@is_creation_date,
          o.create_date BETWEEN @start_date AND @end_date,
          o.delivery_date BETWEEN @start_date AND @end_date))
  AND (IF(@is_creation_date,
          o.order_status NOT IN('Canceled', 'DeliveryCanceled', 'NotDelivered'),
          o.order_status NOT IN('NewOrder','Canceled', 'DeliveryCanceled', 'NotDelivered', 'WaitForPayment')
              OR (o.order_status = 'WaitForPayment' AND o.self_delivery AND o.pay_after_shipment)))
  AND (
        ((IF(o.author_employee_id IS NULL, 0, o.author_employee_id) IN (@order_author_include) AND 0 IN (@order_author_exclude) )
            OR
         (IF(o.author_employee_id IS NULL, 0, o.author_employee_id) NOT IN (@order_author_exclude) AND 0 IN (@order_author_include)))
        OR (0 IN (@order_author_include) AND 0 IN (@order_author_exclude))
    )
  AND !o.is_contract_closer
  AND (items.nomenclature IS NOT NULL OR a.bottles_returned &gt; 0 OR o.bottles_return &gt; 0 OR additions.adds_sum &gt; 0)
  AND !(a.status &lt;=&gt; 'Transfered')
GROUP BY o.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="is_creation_date">
            <Value>=Parameters!is_creation_date</Value>
          </QueryParameter>
          <QueryParameter Name="order_author_include">
            <Value>={?order_author_include}</Value>
          </QueryParameter>
          <QueryParameter Name="order_author_exclude">
            <Value>={?order_author_exclude}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
