  <DataSets>
    <DataSet Name="Sales">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    orders.create_date,
    orders.delivery_date,
    counterparty.name,
    ROUND(SUM(order_items.price * IFNULL(order_items.actual_count, order_items.count)), 2) AS SumWithoutDiscount,
    order_items.discount,
    order_items.discount_money,
    IFNULL(discount_reasons.name, ps.discount_reason_info) AS discount_name,
    nomenclature.name AS nomenclature_name,
    mu.digits AS digits,
    IFNULL(order_items.actual_count, order_items.count) AS count,
    orders.comment,
    GET_PERSON_NAME_WITH_INITIALS(author_employee.last_name, author_employee.name, author_employee.patronymic) AS author,
    GET_PERSON_NAME_WITH_INITIALS(accepted_employee.last_name, accepted_employee.name, accepted_employee.patronymic) AS accepted
FROM
    order_items
        JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
        LEFT JOIN discount_reasons ON discount_reasons.id = order_items.discount_reason_id
        LEFT JOIN promotional_sets ps ON order_items.promotional_set_id = ps.id
        JOIN orders ON orders.id = order_items.order_id
        JOIN counterparty ON counterparty.id = orders.client_id
        LEFT JOIN employees AS author_employee ON author_employee.id = orders.author_employee_id
        LEFT JOIN employees AS accepted_employee ON accepted_employee.id = orders.accepted_order_employee
        LEFT JOIN measurement_units mu ON nomenclature.unit_id = mu.id
WHERE
    (@StartDate = '' OR
     (
             (@DateType = 'CreationDate' AND orders.create_date &gt;= @StartDate)
             OR
             (@DateType = 'DeliveryDate' AND orders.delivery_date &gt;= @StartDate)
         )
        )
  AND
    (@EndDate = '' OR
     (
             (@DateType = 'CreationDate' AND orders.create_date &lt; DATE_ADD(@EndDate, INTERVAL 1 DAY))
             OR
             (@DateType = 'DeliveryDate' AND orders.delivery_date &lt; DATE_ADD(@EndDate, INTERVAL 1 DAY))
         )
        )
  AND (
        ((IFNULL(order_items.discount_reason_id, 0) IN (@discount_reason_include) AND 0 IN (@discount_reason_exclude))
            OR
         (IFNULL(order_items.discount_reason_id, 0) NOT IN (@discount_reason_exclude) AND 0 IN (@discount_reason_include)))
        OR (0 IN (@discount_reason_include) AND 0 IN (@discount_reason_exclude))
    )
  AND (
        ((IFNULL(order_items.promotional_set_id, 0) IN (@promotional_set_include) AND 0 IN (@promotional_set_exclude))
            OR
         (IFNULL(order_items.promotional_set_id, 0) NOT IN (@promotional_set_exclude) AND 0 IN (@promotional_set_include)))
        OR (0 IN (@promotional_set_include) AND 0 IN (@promotional_set_exclude))
    )
  AND order_status not in ('Canceled', 'DeliveryCanceled', 'NotDelivered')
  AND (discount_reasons.id is not null OR ps.id is not null )
  AND order_items.discount &gt; 0
GROUP BY
    order_items.id;</CommandText>
        <QueryParameters>
