  <DataSets>
    <DataSet Name="DriverInfo">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	route_lists.date AS date,
	route_lists.id AS route_list_number,
	IFNULL(SUM(bottles_movement_operations.returned),
			0) AS empty_bottles,
	IFNULL(SUM(bottles_movement_operations.delivered), 0) AS full_bottles,
	IFNULL(wages_movement_operations.money,
			0) AS wage,
	IFNULL(COUNT(IF(Adresses.status = 'Overdue'
					OR Adresses.status = 'Canceled' OR Adresses.transfered_to_id IS NOT NULL,
				NULL,
				Adresses.id)),
			0) AS addresses_count
FROM
	route_lists
		LEFT JOIN
	#Для сравнения водителя из МЛ из которого был перенесен адрес
	(SELECT 
		internal_adresses.*,
			trans_internal_adresses.id AS trans_address_id,
			trans_internal_adresses.route_list_id AS trans_address_rl_id,
			trans_internal_adresses.status AS trans_status,
			internal_trans_route_lists.id AS trans_rl_id,
			internal_trans_route_lists.driver_id AS trans_rl_driver
	FROM
		route_list_addresses AS internal_adresses
	LEFT JOIN route_list_addresses AS trans_internal_adresses ON internal_adresses.transfered_to_id = trans_internal_adresses.id
	LEFT JOIN route_lists AS internal_trans_route_lists ON trans_internal_adresses.route_list_id = internal_trans_route_lists.id) AS Adresses ON Adresses.route_list_id = route_lists.id
		AND (Adresses.trans_rl_driver = route_lists.driver_id
		OR Adresses.trans_rl_driver IS NULL)
		LEFT JOIN
	bottles_movement_operations ON bottles_movement_operations.order_id = Adresses.order_id AND Adresses.transfered_to_id IS NULL
        LEFT JOIN 
        wages_movement_operations ON route_lists.driver_wages_movement_operations_id = wages_movement_operations.id
WHERE
	route_lists.driver_id = @driver_id
		AND date &gt;= @start_date
		AND date &lt;= @end_date
		AND (route_lists.status = 'NotDelivered'
		OR route_lists.status = 'MileageCheck'
		OR route_lists.status = 'Closed')
GROUP BY route_lists.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="route_list_number">
          <DataField>route_list_number</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="addresses_count">
          <DataField>addresses_count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="full_bottles">
          <DataField>full_bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="empty_bottles">
          <DataField>empty_bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="wage">
          <DataField>wage</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
