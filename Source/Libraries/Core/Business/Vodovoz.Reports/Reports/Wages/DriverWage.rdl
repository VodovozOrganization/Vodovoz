    <DataSet Name="Advances">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    wages_movement_operations.operation_time AS date,
    wages_movement_operations.money * (-1) AS advance,
    cash_expense.description AS description
FROM
    wages_movement_operations
INNER JOIN cash_expense ON cash_expense.wages_movement_operations_id = wages_movement_operations.id
WHERE
    wages_movement_operations.operation_type = 'GivedAdvance'
        AND wages_movement_operations.employee_id = @driver_id
        AND wages_movement_operations.operation_time &gt;= DATE(@start_date)
        AND wages_movement_operations.operation_time &lt;= DATE_ADD(DATE(@end_date), INTERVAL 1 DAY);</CommandText>
        <QueryParameters>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="advance">
          <DataField>advance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
