  <DataSets>
    <DataSet Name="DataAdvance">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	cash_expense.id as id,
	cash_expense.financial_expense_category_id as CategoryId,
	IF(cat1.parent_id IS NULL,
		cat1.title,
		CONCAT((SELECT 
						IF(cat2.parent_id IS NULL,
								cat2.title,
								CONCAT((SELECT 
												CONCAT(IF(parent_id IS NULL, '', '... '), title)
											FROM
												financial_categories_groups AS cat3
											WHERE
												id = cat2.parent_id),
										' - ',
										cat2.title))
					FROM
						financial_categories_groups AS cat2
					WHERE
						id = cat1.parent_id),
				' - ',
				cat1.title)) AS Category,

	cash_expense.money as Sum,
	cash_expense.description,
	cash_expense.date as date,
	CONCAT_WS('',
			CONCAT(employees.last_name, ' '),
			if(employees.name = '',
				'',
				CONCAT(LEFT(employees.name, 1), '.')),
			if(employees.patronymic = '',
				'',
				CONCAT(LEFT(employees.patronymic, 1), '.'))) as employee
FROM
	cash_expense
		LEFT JOIN
	financial_categories_groups AS cat1 ON cat1.id = cash_expense.financial_expense_category_id
		LEFT JOIN
	employees ON employees.id = cash_expense.employee_id
WHERE cash_expense.type_operation = 'Advance' AND
	cash_expense.date &gt;= @startDate AND cash_expense.date &lt; @endDate
AND IF(@expenseCategoryUsed, cash_expense.financial_expense_category_id IN (@expenseCategory), 1)
AND (@Casher = -1 OR cash_expense.casher_employee_id = @Casher)
AND (@Employee = -1 OR cash_expense.employee_id = @Employee)
AND cash_expense.related_to_subdivision_id in (@cash_subdivisions)
ORDER BY Category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="expenseCategory">
            <Value>=Parameters!ExpenseCategory</Value>
          </QueryParameter>
          <QueryParameter Name="expenseCategoryUsed">
            <Value>=Parameters!ExpenseCategoryUsed</Value>
          </QueryParameter>
          <QueryParameter Name="Casher">
            <Value>={?Casher}</Value>
          </QueryParameter>
          <QueryParameter Name="Employee">
            <Value>={?Employee}</Value>
          </QueryParameter>
          <QueryParameter Name="cash_subdivisions">
            <Value>=Parameters!cash_subdivisions</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
