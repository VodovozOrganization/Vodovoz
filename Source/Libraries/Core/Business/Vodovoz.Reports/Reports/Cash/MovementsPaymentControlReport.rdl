      <ConnectionProperties>
        <DataProvider>MySqlConnector</DataProvider>
        <ConnectString>database=Vodovoz_honeybee;user=;password=;port=3306;server=vod-srv.qsolution.ru;Allow User Variables=True</ConnectString>
