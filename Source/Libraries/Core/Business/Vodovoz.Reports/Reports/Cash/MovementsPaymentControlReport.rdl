  <DataSets >
    <DataSet Name="movements">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>60</Timeout>
        <CommandText>SELECT
	smd.id as id,
	smd.send_time as send_date,
	CASE
		WHEN smd.storage_from = 'Warehouse' THEN w_from.name
		WHEN smd.storage_from = 'Car' THEN
		CONCAT_WS(
            ' ',
            c_from.reg_number,
            c_from.model) 
		WHEN smd.storage_from = 'Employee' THEN 
		CONCAT_WS(
            ' ',
            e_from.last_name,
            IF(e_from.name = '',
               NULL,
               e_from.name),
            IF(e_from.patronymic = '',
               NULL,
               e_from.patronymic))               
		ELSE 'Нет данных'
	END sender,
	CASE
		WHEN smd.document_type_by_storage = 'ToWarehouse' THEN w_to.name
		WHEN smd.document_type_by_storage = 'ToCar' THEN
		CONCAT_WS(
            ' ',
            c_to.reg_number,
            c_to.model) 
		WHEN smd.document_type_by_storage = 'ToEmployee' THEN
		CONCAT_WS(
            ' ',
            e_to.last_name,
            IF(e_to.name = '',
               NULL,
               e_to.name),
            IF(e_to.patronymic = '',
               NULL,
               e_to.patronymic)) 
		ELSE 'Нет данных'
	END receiver,
	c_wagon.name as wagon,
	smd.transporter_bill as bill,
	FORMAT(smd.tranporter_sum, 2, 'ru_RU') as 'sum',
	c_transporter.full_name as transporter_name
FROM
	store_movement_document smd
LEFT JOIN warehouses w_from ON
	w_from.id = smd.warehouse_from_id
LEFT JOIN warehouses w_to ON
	w_to.id = smd.warehouse_to_id
LEFT JOIN cars c_from ON
	c_from.id = smd.car_from_id
LEFT JOIN cars c_to ON
	c_to.id = smd.car_to_id
LEFt JOIN employees e_from ON
	e_from.id = smd.employee_from_id
LEFT JOIN employees e_to ON
	e_to.id = smd.employee_to_id
LEFT JOIN store_movement_wagon c_wagon ON
	c_wagon.id = smd.transportation_wagon_id
LEFT JOIN counterparty c_transporter ON
	c_transporter.id = smd.transporter_id
WHERE
	NOT ISNULL(smd.transportation_wagon_id)
	AND smd.send_time &gt;= @start_date
	AND smd.send_time &lt; DATE_ADD(@end_date, INTERVAL 1 DAY)
ORDER BY smd.id </CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="send_date">
          <DataField>send_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="sender">
          <DataField>sender</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="receiver">
          <DataField>receiver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="wagon">
          <DataField>wagon</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="bill">
          <DataField>bill</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="sum">
          <DataField>sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="transporter_name">
          <DataField>transporter_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
