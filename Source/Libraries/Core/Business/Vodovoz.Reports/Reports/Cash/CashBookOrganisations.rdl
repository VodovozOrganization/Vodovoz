  <DataSets>
    <DataSet Name="Income">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>75</Timeout>
        <CommandText>##INCOME
SELECT ocmo.operation_time as doc_date,
       IFNULL(ci.id, octd.id) as doc_id,
       CASE WHEN codd.id IS NOT NULL THEN
           IF(employeeal1_.name is null,
              CONCAT_WS
                  (
                      '.',
                      NULLIF(LEFT(author.name, 1), ''),
                      NULLIF(LEFT(author.patronymic, 1), ''),
                      author.last_name
                  ),
              CONCAT_WS
                  (
                      '.',
                      NULLIF(LEFT(employeeal1_.name, 1), ''),
                      NULLIF(LEFT(employeeal1_.patronymic, 1), ''),
                      employeeal1_.last_name
                  ))
       ELSE
          organizations.name
       END as name,
       ocmo.amount as income,
       0 as expense,
       IFNULL(codd.organisation_id, octd.organization_to_id) AS organisation_id,
       IF(octd.id IS NOT NULL, 'v', '') is_jur

FROM organisation_cash_movement_operations ocmo
    LEFT JOIN (cash_organisation_distribution_documents codd 
    inner join cash_income ci on codd.cash_income_id = ci.id) ON codd.organisation_cash_movement_operation_id = ocmo.id
    left join employees employeeal1_ on codd.employee_id = employeeal1_.id
    left join employees author on codd.author_id = author.id
    LEFT JOIN organization_cash_transfer_documents octd ON octd.organisation_cash_movement_operation_to_id = ocmo.id
    LEFT JOIN organizations ON organizations.id = octd.organization_to_id

WHERE IF(@organisation != -1,
        (IFNULL(codd.organisation_id, octd.organization_to_id) = @organisation AND ocmo.operation_time BETWEEN @StartDate and @EndDate),
        (ocmo.operation_time BETWEEN @StartDate and @EndDate) AND (IFNULL(codd.organisation_id, octd.organization_to_id) IS NOT NULL))

UNION ALL

##EXPENCE
SELECT ocmo.operation_time as doc_date,
       IFNULL(ce.id, octd.id) as doc_id,
       CASE WHEN codd.id IS NOT NULL THEN
           IF(employeeal1_.name is null,
              CONCAT_WS
                  (
                      '.',
                      NULLIF(LEFT(author.name, 1), ''),
                      NULLIF(LEFT(author.patronymic, 1), ''),
                      author.last_name
                  ),
              CONCAT_WS
                  (
                      '.',
                      NULLIF(LEFT(employeeal1_.name, 1), ''),
                      NULLIF(LEFT(employeeal1_.patronymic, 1), ''),
                      employeeal1_.last_name
                  ))
       ELSE
          organizations.name
       END as name,
       0 as income,
       -ocmo.amount as expance,
       IFNULL(codd.organisation_id, octd.organization_from_id) AS organisation_id,
       IF(octd.id IS NOT NULL, 'v', '') is_jur
       
FROM organisation_cash_movement_operations ocmo
    LEFT JOIN (cash_organisation_distribution_documents codd 
    inner join cash_expense ce on codd.cash_expense_id = ce.id) ON codd.organisation_cash_movement_operation_id = ocmo.id
    left join employees employeeal1_ on codd.employee_id = employeeal1_.id
    left join employees author on codd.author_id = author.id
    LEFT JOIN organization_cash_transfer_documents octd ON octd.organisation_cash_movement_operation_from_id = ocmo.id
    LEFT JOIN organizations ON organizations.id = octd.organization_from_id      
	 
WHERE IF(@organisation != -1,
        (IFNULL(codd.organisation_id, octd.organization_from_id) = @organisation AND ocmo.operation_time BETWEEN @StartDate and @EndDate),
        (ocmo.operation_time BETWEEN @StartDate and @EndDate) AND (IFNULL(codd.organisation_id, octd.organization_to_id) IS NOT NULL));</CommandText>
        <QueryParameters>
          <QueryParameter Name="StartDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="EndDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="organisation">
            <Value>=Parameters!organisation</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="doc_date">
          <DataField>doc_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="doc_id">
          <DataField>doc_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="income">
          <DataField>income</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="expense">
          <DataField>expense</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="is_jur">
          <DataField>is_jur</DataField>
          <TypeName>System.Boolean</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="StartBalance">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>Select
    SUM(ocmo.amount) as balance
FROM organisation_cash_movement_operations ocmo
WHERE ocmo.operation_time &lt; @StartDate
    AND IF(@organisation != -1, ocmo.organisation_id = @organisation, 1=1);</CommandText>
        <QueryParameters>
          <QueryParameter Name="StartDate">
            <Value>=Parameters!StartDate</Value>
