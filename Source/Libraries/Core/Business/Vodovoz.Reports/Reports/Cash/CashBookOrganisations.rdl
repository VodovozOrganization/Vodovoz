    <DataSet Name="CountIncome">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    count(*) as income_count
FROM cash_organisation_distribution_documents codd
WHERE codd.amount &gt; 0
    AND IF(@organisation != -1,
         (codd.organisation_id = @organisation and codd.last_edited_time BETWEEN @StartDate and @EndDate),
         (codd.last_edited_time BETWEEN @StartDate AND @EndDate));</CommandText>
        <QueryParameters>
          <QueryParameter Name="StartDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="EndDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="organisation">
            <Value>=Parameters!organisation</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="income_count">
          <DataField>income_count</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CountExpense">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    count(*) as expense_count
FROM cash_organisation_distribution_documents codd
WHERE codd.amount &lt; 0
    AND IF(@organisation != -1,
         (codd.organisation_id = @organisation and codd.last_edited_time BETWEEN @StartDate and @EndDate),
         (codd.last_edited_time BETWEEN @StartDate AND @EndDate));</CommandText>
        <QueryParameters>
