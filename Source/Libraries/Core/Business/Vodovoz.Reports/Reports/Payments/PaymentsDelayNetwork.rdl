  <DataSets>
    <DataSet Name="orders">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_num,
    orders.create_date AS create_date, # для сортировки
    orders.delivery_date AS delivery_date,
    counterparty.name AS counterparty_name,
    counterparty.inn AS INN,
    dp.compiled_address_short AS delivery_points_compiled_address_short,

    IF(orders.author_employee_id IS NOT NULL,
       CONCAT(employees.last_name,
              ' ',
              LEFT(employees.name, 1),
              '.',
              LEFT(employees.patronymic, 1),
              '.'),
       '') AS author,
    CAST(
            IF(orders.sum_to_receive &lt;&gt; 0,
               orders.sum_to_receive,
               IFNULL(
                       (SELECT
                            SUM(IFNULL(order_items.actual_count, order_items.count) * order_items.price - order_items.discount_money)
                        FROM
                            order_items
                        WHERE
                                orders.id = order_items.order_id
                        GROUP BY order_items.order_id),
                       0)
                )
        AS DECIMAL (12 , 2 )) AS order_sum,
    DATE_ADD(orders.delivery_date, INTERVAL IFNULL(counterparty.delay_days_for_buyers, 0) DAY) AS estimated_payment_date,
    TO_DAYS(CURDATE()) - TO_DAYS(DATE_ADD(orders.delivery_date, INTERVAL IFNULL(counterparty.delay_days_for_buyers, 0) DAY)) AS days_overdue,
    CAST(
                IF(orders.sum_to_receive &lt;&gt; 0,
                   orders.sum_to_receive,
                   IFNULL(
                           (SELECT
                                SUM(IFNULL(order_items.actual_count, order_items.count) * order_items.price - order_items.discount_money)
                            FROM
                                order_items
                            WHERE
                                    orders.id = order_items.order_id
                            GROUP BY order_items.order_id),
                           0)
                    )
                - IF(orders.order_payment_status = 'PartiallyPaid',
                     (SELECT SUM(cmo.expense)
                      FROM orders o
                               LEFT JOIN payment_items pi ON o.id = pi.order_id
                               LEFT JOIN cashless_movement_operations cmo ON pi.cashless_movement_operation_id = cmo.id
                      WHERE o.id = orders.id and cmo.cashless_movement_operation_status != 'Cancelled'
                      GROUP BY o.id),
                     0)
        AS DECIMAL (12 , 2 )) AS debt,
    orders.op_comment AS OP_manager_comment,
    orders.odz_comment AS ODZ_logist_comment
FROM
    orders
        LEFT JOIN counterparty ON counterparty.id = orders.client_id
        LEFT JOIN employees ON employees.id = orders.author_employee_id
        LEFT JOIN delivery_points dp on dp.id = orders.delivery_point_id
WHERE
        DATE_ADD(orders.delivery_date, INTERVAL IFNULL(counterparty.delay_days_for_buyers, 0) DAY) &lt; DATE(@date)
  AND orders.order_payment_status IN ('UnPaid', 'PartiallyPaid')
  AND (
    (@mode = "Networks" AND counterparty.is_chain_store = 1)
     OR
    (@mode = "Tenders" AND counterparty.counterparty_camefrom_id = 46)
  )
  AND orders.payment_type = 'Cashless'
  AND orders.order_status NOT IN ('Canceled', 'NotDelivered', 'DeliveryCanceled')
  AND (orders.order_status &lt;&gt; 'WaitForPayment' OR (orders.pay_after_shipment = 1 AND orders.self_delivery = 1))
  AND (@counterparty_id = -1 OR counterparty.id = @counterparty_id )
  AND (@sell_manager_id = -1 OR counterparty.sales_manager_id = @sell_manager_id)
  AND (@order_author_id = -1 OR orders.author_employee_id = @order_author_id)
GROUP BY orders.id
HAVING order_sum &gt; 0
ORDER BY estimated_payment_date;</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
          <QueryParameter Name="counterparty_id">
            <Value>=Parameters!counterparty_id</Value>
          </QueryParameter>
          <QueryParameter Name="sell_manager_id">
            <Value>=Parameters!sell_manager_id</Value>
          </QueryParameter>
          <QueryParameter Name="order_author_id">
            <Value>=Parameters!order_author_id</Value>
          </QueryParameter>
          <QueryParameter Name="mode">
            <Value>=Parameters!mode</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="order_num">
          <DataField>order_num</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="counterparty_name">
          <DataField>counterparty_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="INN">
          <DataField>INN</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="create_date">
          <DataField>create_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="author">
          <DataField>author</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="order_sum">
