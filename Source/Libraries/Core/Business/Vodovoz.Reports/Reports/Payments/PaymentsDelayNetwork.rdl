        </Field>
        <Field Name="estimated_payment_date">
          <DataField>estimated_payment_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="days_overdue">
          <DataField>days_overdue</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="OP_manager_comment">
          <DataField>OP_manager_comment</DataField>
