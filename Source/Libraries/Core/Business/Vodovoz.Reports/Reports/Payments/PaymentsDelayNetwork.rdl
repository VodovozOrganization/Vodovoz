        </Field>
        <Field Name="ODZ_logist_comment">
          <DataField>ODZ_logist_comment</DataField>
          <TypeName>System.Int16</TypeName>
        </Field>
        <Field Name="debt">
          <DataField>debt</DataField>
        </Field>
        <Field Name="delivery_date">
          <DataField>delivery_date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="delivery_points_compiled_address_short">
