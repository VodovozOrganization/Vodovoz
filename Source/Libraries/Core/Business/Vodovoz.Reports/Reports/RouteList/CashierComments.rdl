  <DataSets>
    <DataSet Name="CashierComments">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    counterparty.full_name AS client,
    delivery_points.compiled_address AS address,
    route_list_addresses.route_list_id AS route_list,
    route_lists.date AS route_list_date,
    CONCAT_WS(' ', driver_q.last_name, driver_q.name) AS driver_name,
    route_list_addresses.cashier_comment AS cashier_comment,
    CONCAT_WS(' ', comment_author_q.last_name, comment_author_q.name) AS author_name
FROM
    route_list_addresses
    LEFT JOIN
    orders ON orders.id = route_list_addresses.order_id
    LEFT JOIN
    route_lists ON route_lists.id = route_list_addresses.route_list_id
    LEFT JOIN
    employees AS driver_q ON driver_q.id = route_lists.driver_id
    LEFT JOIN
    employees AS comment_author_q ON comment_author_q.id = route_list_addresses.cashier_comment_author
    LEFT JOIN
    counterparty ON counterparty.id = orders.client_id
    LEFT JOIN
    delivery_points ON delivery_points.id = orders.delivery_point_id
WHERE
    route_list_addresses.cashier_comment IS NOT NULL
        AND route_list_addresses.cashier_comment &lt;&gt; ''
        AND ((route_list_addresses.cashier_comment_create_date &gt;= @start_date
        AND route_list_addresses.cashier_comment_create_date &lt;= @end_date)
        OR (route_list_addresses.cashier_comment_last_update &gt;= @start_date
        AND route_list_addresses.cashier_comment_last_update &lt;= @end_date));</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="client">
          <DataField>client</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="address">
          <DataField>address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="route_list">
          <DataField>route_list</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="cashier_comment">
          <DataField>cashier_comment</DataField>
