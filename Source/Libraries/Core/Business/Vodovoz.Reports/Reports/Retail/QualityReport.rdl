  <DataSets>
    <DataSet Name="quality">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    date(orders.create_date) AS orderdate,
	delivery_point_categories.name AS entrance,
    orders.id AS order_id,
	districts.district_name AS district,
	counterparty.name AS client,
	delivery_points.compiled_address AS address,
	sales_channels.name AS channel_name,
	CASE
		WHEN orders.payment_type = 'Cash' THEN 'Наличная'
		WHEN orders.payment_type = 'Terminal' THEN 'Терминал (QR-код терминал)'
		WHEN orders.payment_type = 'DriverApplicationQR' THEN 'МП водителя (QR-код)'
		WHEN orders.payment_type = 'SmsQR' THEN 'SMS (QR-код)'
		WHEN orders.payment_type = 'PaidOnline' THEN 'Оплачено онлайн'
		WHEN orders.payment_type = 'Barter' THEN 'Бартер'	
		WHEN orders.payment_type = 'ContractDocumentation' THEN 'Контрактная документация'
		WHEN orders.payment_type = 'Cashless' THEN 'Безналичная'

		ELSE 'Неизвестная форма оплаты'
	END AS payment_type,
    if(orders.order_status in ('Shipped','Closed'), date(orders.delivery_date), '') AS delivery_date,
	CASE
		WHEN orders.order_payment_status = 'Paid' THEN 'Оплачен'
		WHEN orders.order_payment_status = 'PartiallyPaid' THEN 'Частично оплачен'
		WHEN orders.order_payment_status = 'UnPaid' THEN 'Неоплачен'
	ELSE 'Неизвестная форма оплаты'
	END AS order_payment_status,
    ROUND(o_items.price * IFNULL(o_items.actual_count, o_items.count) - o_items.discount_money, 2) AS ordersum,
	GET_PERSON_NAME_WITH_INITIALS(employees.last_name, employees.name, employees.patronymic) AS author
FROM orders

LEFT JOIN counterparty ON counterparty.id = orders.client_id
LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
LEFT JOIN districts ON districts.id = delivery_points.district_id
LEFT JOIN sales_channel_to_counterparty ON  sales_channel_to_counterparty.counterparty_id = orders.client_id
LEFT JOIN sales_channels ON sales_channels.id = sales_channel_to_counterparty.sales_channel_id
LEFT JOIN delivery_points_responsible_persons points ON points.delivery_point_id = delivery_points.id
LEFT JOIN employees ON employees.id = points.employee_id
LEFT JOIN order_items o_items ON o_items.order_id = orders.id
LEFT JOIN delivery_point_categories ON delivery_point_categories.id = delivery_points.delivery_point_category_id
WHERE
	orders.is_for_retail
    AND if(@shipping_start_date != '', orders.order_status in ('Shipped', 'Closed'), orders.order_status not in ('Canceled', 'NewOrder', 'NotDelivered','DeliveryCanceled'))
    AND (@create_date = '' OR orders.create_date &gt;= @create_date)
    AND (@end_date = '' OR orders.create_date &lt;= @end_date)
    AND (@shipping_start_date = '' OR orders.delivery_date &gt;= @shipping_start_date)
    AND (@shipping_end_date = '' OR orders.delivery_date &lt;= @shipping_end_date)
	AND (@sales_channel_id = 0 OR sales_channels.id = @sales_channel_id)
	AND (@counterparty_id = 0 OR orders.client_id = @counterparty_id)
	AND (@main_contact_id = 0 OR employees.id = @main_contact_id)
GROUP BY orders.id
ORDER BY orderdate DESC, orders.id DESC;</CommandText>
