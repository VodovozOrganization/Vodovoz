  <DataSets>
    <DataSet Name="counterparty">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
counterparty.id AS counter_id,
counterparty.name AS name,
counterparty.full_name AS full_name,
delivery_points.compiled_address AS address,
districts.district_name AS district,
delivery_points.id as points,
date(orders.create_date) AS first_order,
GET_PERSON_NAME_WITH_INITIALS(employees.last_name, employees.name, employees.patronymic) AS employee_full_name,
sales_channels.name AS channel_name,
CASE
     WHEN counterparty.payment_method = 'Cash' THEN 'Наличная'
     WHEN counterparty.payment_method = 'Terminal' THEN 'Терминал (QR-код терминал)'
     WHEN counterparty.payment_method = 'DriverApplicationQR' THEN 'SMS (QR-код)'
     WHEN counterparty.payment_method = 'SmsQR' THEN 'МП водителя (QR-код)'
     WHEN counterparty.payment_method = 'PaidOnline' THEN 'Оплачено онлайн'
     WHEN counterparty.payment_method = 'Barter' THEN 'Бартер'
     WHEN counterparty.payment_method = 'ContractDocumentation' THEN 'Контрактная документация'
     WHEN counterparty.payment_method = 'Cashless' THEN 'Безналичная'

     ELSE 'Неизвестная форма оплаты'
END AS payment_type
FROM counterparty
LEFT JOIN delivery_points ON delivery_points.counterparty_id = counterparty.id
LEFT JOIN districts ON districts.id = delivery_points.district_id
LEFT JOIN sales_channel_to_counterparty ON sales_channel_to_counterparty.counterparty_id = counterparty.id
LEFT JOIN sales_channels ON sales_channels.id = sales_channel_to_counterparty.sales_channel_id
LEFT JOIN delivery_points_responsible_persons points ON points.delivery_point_id = delivery_points.id
LEFT JOIN employees ON employees.id = points.employee_id
LEFT JOIN orders ON orders.client_id = counterparty.id
    WHERE
    counterparty.is_for_retail
    AND (@create_date = '' OR orders.create_date &gt;= @create_date)
    AND (@end_date = '' OR orders.create_date &lt;= @end_date)
    AND (orders.order_status = 'Closed')
    AND (@sales_channel_id = 0 OR sales_channels.id = @sales_channel_id)
    AND (@district = 0 OR delivery_points.district_id = @district)
    AND (IF(@all_types, true, counterparty.payment_method = @payment_type))
GROUP BY delivery_points.compiled_address, counterparty.full_name
ORDER BY counter_id, first_order DESC, orders.create_date DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="create_date">
            <Value>=Parameters!create_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="sales_channel_id">
            <Value>=Parameters!sales_channel_id</Value>
          </QueryParameter>
          <QueryParameter Name="district">
