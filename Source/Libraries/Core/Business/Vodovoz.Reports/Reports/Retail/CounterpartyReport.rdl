          <QueryParameter Name="payment_type">
            <Value>=Parameters!payment_type</Value>
          </QueryParameter>
          <QueryParameter Name="all_types">
            <Value>=Parameters!all_types</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
