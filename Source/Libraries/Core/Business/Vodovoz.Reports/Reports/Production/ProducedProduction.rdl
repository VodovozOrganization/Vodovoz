  <DataSets>
    <DataSet Name="MainData">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT nomenclature.id as nomenclature_id,
       nomenclature.name as nomenclature_name,
       IFNULL(subquery1.name_for_group, 'Неизвестно') as warehouse_for_grouping,
       IFNULL(subquery1.sum, -1) as current_month_sum,
       IFNULL(subquery2.sum, 0) as month_minus_1_sum,
       IFNULL(subquery3.sum, 0) as month_minus_2_sum,
       IFNULL(subquery1.sum - subquery2.sum, 0) as diff_curr_minus_1_month,
       IFNULL(subquery2.sum - subquery3.sum, 0) as diff_minus_1_minus_2_month,
       IFNULL((subquery1.sum - subquery2.sum)*100/subquery1.sum, 0) as diff_curr_minus_1_month_percent,
       IFNULL((subquery2.sum - subquery3.sum)*100/subquery2.sum, 0) as diff_minus_1_minus_2_month_percent

FROM nomenclature
         LEFT JOIN warehouses w on nomenclature.warehouse_id = w.id
         LEFT JOIN
     (SELECT
          goods_accounting_operations.nomenclature_id as id,
          SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
             AND goods_accounting_operations.amount &gt; 0
             AND store_incoming_water.id IS NOT NULL, cast(goods_accounting_operations.amount as signed), 0)) as sum,
          (SELECT warehouses.name FROM warehouses WHERE id = @warehouse_id) as name_for_group

      FROM
          goods_accounting_operations
              LEFT JOIN store_incoming_water ON store_incoming_water.produce_operation_id = goods_accounting_operations.id

      WHERE
              store_incoming_water.time_stamp &gt;= @month_start
        AND store_incoming_water.time_stamp &lt;= DATE_ADD(@month_end, INTERVAL 1 DAY)
      GROUP BY goods_accounting_operations.nomenclature_id) as subquery1 on subquery1.id  = nomenclature.id


         LEFT JOIN
     (SELECT
          goods_accounting_operations.nomenclature_id as id,
          SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
             AND goods_accounting_operations.amount &gt; 0
             AND store_incoming_water.id IS NOT NULL, cast(goods_accounting_operations.amount as signed), 0)) as sum,
          (SELECT warehouses.name FROM warehouses WHERE id = @warehouse_id) as name_for_group

      FROM
          goods_accounting_operations
              LEFT JOIN store_incoming_water ON store_incoming_water.produce_operation_id = goods_accounting_operations.id

      WHERE
              store_incoming_water.time_stamp &gt;= @month_minus_1_start
        AND store_incoming_water.time_stamp &lt;= DATE_ADD(@month_minus_1_end, INTERVAL 1 DAY)
      GROUP BY goods_accounting_operations.nomenclature_id) as subquery2 on subquery2.id  = nomenclature.id


         LEFT JOIN
     (SELECT
          goods_accounting_operations.nomenclature_id as id,
          SUM(IF(goods_accounting_operations.warehouse_id = @warehouse_id
             AND goods_accounting_operations.amount &gt; 0
             AND store_incoming_water.id IS NOT NULL, cast(goods_accounting_operations.amount as signed), 0)) as sum,
          (SELECT warehouses.name FROM warehouses WHERE id = @warehouse_id) as name_for_group

      FROM
          goods_accounting_operations
              LEFT JOIN store_incoming_water ON store_incoming_water.produce_operation_id = goods_accounting_operations.id

      WHERE
              store_incoming_water.time_stamp &gt;= @month_minus_2_start
        AND store_incoming_water.time_stamp &lt;= DATE_ADD(@month_minus_2_end, INTERVAL 1 DAY)
      GROUP BY goods_accounting_operations.nomenclature_id) as subquery3 on subquery3.id  = nomenclature.id

WHERE nomenclature.category = 'water' AND
    (nomenclature.id = @nomenclature_id OR @nomenclature_id = -1)

GROUP BY nomenclature.id;
     </CommandText>
        <QueryParameters>
          <QueryParameter Name="month_start">
            <Value>={?month_start}</Value>
          </QueryParameter>
          <QueryParameter Name="month_end">
            <Value>={?month_end}</Value>
