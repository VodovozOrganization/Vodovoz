  <DataSets>
    <DataSet Name="MainData">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>
SELECT
t.warehouse_for_grouping,
t.nomenclature_name,
SUM(IF(t.month = MONTH(@year_minus_1_start) AND t.year = YEAR(@year_minus_1_start), t.oper_sum, 0)) AS year_minus_1_sum,
SUM(IF(t.month = MONTH(@month_minus_1_start) AND t.year = YEAR(@month_minus_1_start), t.oper_sum, 0)) AS month_minus_1_sum,
SUM(IF(t.month = MONTH(@month_start) AND t.year = YEAR(@month_start), t.oper_sum, 0)) current_month_sum

FROM (

SELECT      
       nomenclature.name  as  nomenclature_name,
       SUM(
		  IF(@measurement_unit = 'Liters',
		    goods_accounting_operations.amount *
		      CASE nomenclature.tare_volume
		        WHEN 'Vol19L' THEN 19
		        WHEN 'Vol6L' THEN 6
		        WHEN 'Vol1500ml' THEN 1.5
		        WHEN 'Vol600ml' THEN 0.6
		        WHEN 'Vol500ml' THEN 0.5
		        ELSE 0
		      END,
		    goods_accounting_operations.amount
		  )
		) as oper_sum,
       YEAR(store_incoming_water.time_stamp) year,
       MONTH(store_incoming_water.time_stamp) month,
       (SELECT name FROM warehouses w WHERE w.id = goods_accounting_operations.warehouse_id) AS warehouse_for_grouping

FROM nomenclature
    LEFT JOIN goods_accounting_operations on goods_accounting_operations.nomenclature_id = nomenclature.id
    LEFT JOIN store_incoming_water ON store_incoming_water.produce_operation_id = goods_accounting_operations.id
    WHERE
         nomenclature.category = 'water'
         AND 
         (
             (store_incoming_water.time_stamp &gt;= @month_minus_1_start
             AND store_incoming_water.time_stamp &lt;= DATE_ADD( @month_end, INTERVAL 1 DAY))
             OR
             (store_incoming_water.time_stamp &gt;= @year_minus_1_start
             AND store_incoming_water.time_stamp &lt;= DATE_ADD( @year_minus_1_end, INTERVAL 1 DAY))
          )
             
         AND goods_accounting_operations.amount &gt; 0
         AND store_incoming_water.id IS NOT NULL
         AND
         (0 IN (@Nomenclature_include) OR (goods_accounting_operations.nomenclature_id IN (@Nomenclature_include)))
				AND NOT ( goods_accounting_operations.nomenclature_id IN (@Nomenclature_exclude))
         AND
         (0 IN (@Warehouse_include) OR (goods_accounting_operations.warehouse_id  IN (@Warehouse_include)))
				AND NOT ( goods_accounting_operations.warehouse_id  IN (@Warehouse_exclude))				                 
         
         GROUP BY year, month, nomenclature_name, warehouse_for_grouping
) t
         
GROUP BY t.nomenclature_name, t.warehouse_for_grouping</CommandText>
        <QueryParameters>
          <QueryParameter Name="month_start">
            <Value>={?month_start}</Value>
          </QueryParameter>
          <QueryParameter Name="month_end">
            <Value>={?month_end}</Value>
          </QueryParameter>
          <QueryParameter Name="month_minus_1_start">
            <Value>={?month_minus_1_start}</Value>
          </QueryParameter>
          <QueryParameter Name="month_minus_1_end">
            <Value>={?month_minus_1_end}</Value>
          </QueryParameter>
          <QueryParameter Name="year_minus_1_start">
            <Value>={?year_minus_1_start}</Value>
          </QueryParameter>
          <QueryParameter Name="year_minus_1_end">
            <Value>={?year_minus_1_end}</Value>
          </QueryParameter>
          <QueryParameter Name="Nomenclature_include">
            <Value>={?Nomenclature_include}</Value>
          </QueryParameter>
          <QueryParameter Name="Nomenclature_exclude">
            <Value>={?Nomenclature_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="Warehouse_include">
            <Value>={?Warehouse_include}</Value>
          </QueryParameter>
          <QueryParameter Name="Warehouse_exclude">
            <Value>={?Warehouse_exclude}</Value>
          </QueryParameter>
          <QueryParameter Name="measurement_unit">
            <Value>={?measurement_unit}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
