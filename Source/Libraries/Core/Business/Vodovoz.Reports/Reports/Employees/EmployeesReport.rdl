  <DataSets>
    <DataSet Name="drivers">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select
    concat_ws('', concat(empl.last_name, ' '), if(empl.name = '', '', concat(left(empl.name, 1),'.')), if(empl.patronymic = '', '', concat(left(empl.patronymic, 1),'.')))
        as empl_name,
    s.short_name as subdiv,
    CONCAT(
        CASE empl.driver_of_car_type_of_use
            WHEN 'Largus' THEN 'Л'
            WHEN 'Truck' THEN 'Ф'
            WHEN 'GAZelle' THEN 'Г'
            WHEN NULL THEN '-'
            ELSE 'Неизв.'
        END,
        CASE empl.driver_of_car_own_type
            WHEN 'Company' THEN 'К'
            WHEN 'Raskat' THEN 'Р'
            WHEN 'Driver' THEN 'В'
            WHEN NULL THEN '-'
            ELSE 'Неизв.'
        END
    ) as driver_of,
    cm.name as model,
    c.reg_number as reg_number,
    cm.max_weight as max_weight,
    frl.date as first_rl,
    lrl.date as last_rl,
    concat_ws('', concat(works_with.last_name, ' '), if(works_with.name = '', '', concat(left(works_with.name, 1),'.')), if(works_with.patronymic = '', '', concat(left(works_with.patronymic, 1),'.')))
        as works_with_name,
    group_concat(dicts.district_name SEPARATOR ', ') as districts,
    empl.min_route_addresses as min_addresses,
    empl.max_route_addresses as max_addresses
from employees empl
left join subdivisions s on s.id = empl.subdivision_id
left join employees works_with on works_with.id = empl.default_forwarder_id
left join cars c on c.driver_id = empl.id
left join car_models cm ON c.model_id = cm.id
left join driver_district_priority_sets ddps   on (ddps.driver_id = empl.id and ddps.is_active=1)
left join driver_district_priorities ddp 	   on ddp.driver_district_priority_set_id = ddps.id
left join districts dicts 					   on dicts.id = ddp.district_id
left join route_lists frl on frl.id = (select sub_rl.id from route_lists as sub_rl where empl.id = sub_rl.driver_id order by sub_rl.date limit 1)
left join route_lists lrl on lrl.id = (select sub_rl.id from route_lists as sub_rl where empl.id = sub_rl.driver_id order by sub_rl.date desc limit 1)
where (@empl_category = 'driver'
    and empl.category=@empl_category
    and instr(@empl_status, empl.status) &gt; 0
    and (empl.visiting_master=0 or empl.visiting_master is null)
    and (empl.is_driver_for_one_day=0 or empl.is_driver_for_one_day is null)
    and (empl.driver_of_car_type_of_use is null or !(empl.driver_of_car_type_of_use = 'Truck' and empl.driver_of_car_own_type = 'Company'))
    and ((empl.status='IsFired' and (empl.date_fired &gt;= @date_fired and empl.date_fired &lt;= @date_fired_end) or empl.date_fired is null) or @date_fired = '')
    and (((empl.status='OnCalculation' or empl.status='IsFired') and (empl.date_calculated &gt;= @date_calculation and empl.date_calculated &lt;= @date_calculation_end) or empl.date_calculated is null) or @date_calculation = '')
    and (empl.creation_date &gt;= @creation_date and empl.creation_date &lt;= @creation_date_end or @creation_date = '')
    and (empl.first_work_day &gt;= @first_work_day and empl.first_work_day  &lt;= @first_work_day_end or @first_work_day = '')
    and (empl.date_hired &gt;= @date_hired and empl.date_hired &lt;= @date_hired_end or @date_hired = '')
    and (frl.date &gt;= @first_rl and frl.date &lt;= @first_rl_end or @first_rl ='')
    and (lrl.date &gt;= @last_rl and lrl.date &lt;= @last_rl_end or @last_rl ='')
          ) group by empl.id order by empl.last_name
</CommandText>
        <QueryParameters>
          <QueryParameter Name="empl_category">
            <Value>={?empl_category}</Value>
          </QueryParameter>
          <QueryParameter Name="empl_status">
            <Value>={?empl_status}</Value>
          </QueryParameter>
          <QueryParameter Name="creation_date">
            <Value>={?creation_date}</Value>
          </QueryParameter>
