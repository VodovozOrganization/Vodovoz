          </QueryParameter>
          <QueryParameter Name="date_hired">
            <Value>={?date_hired}</Value>
          </QueryParameter>
          <QueryParameter Name="date_hired_end">
            <Value>={?date_hired_end}</Value>
          </QueryParameter>
          <QueryParameter Name="date_fired">
            <Value>={?date_fired}</Value>
          </QueryParameter>
          <QueryParameter Name="date_fired_end">
            <Value>={?date_fired_end}</Value>
          </QueryParameter>
          <QueryParameter Name="date_calculation">
