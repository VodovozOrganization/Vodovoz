    <DataSet Name="PaymentsWithoutOrders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>120</Timeout>
        <CommandText>SELECT
    pfa.*,
    (
        Select
            CASE fp.pay_type
                WHEN 'Unknown' THEN 'Неизвестно'
                WHEN 'ByCard' THEN 'По карте'
                WHEN 'ByQrCode' THEN 'По Qr коду'
                END
        From fast_payments fp
        Where fp.external_id = pfa.order_num
          and date(fp.paid_date) = date(pfa.paid_date)
    ) AS pay_type
FROM payments_from_avangard pfa
LEFT JOIN orders o ON o.online_order = pfa.order_num
    AND payment_from_id IN (10, 11, 12)
    AND delivery_date &gt;= @startDate
WHERE
    o.id is null
    AND pfa.paid_date &gt;= @startDate
    AND pfa.paid_date &lt;= @endDate
ORDER BY pfa.paid_date DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!startDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!endDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
