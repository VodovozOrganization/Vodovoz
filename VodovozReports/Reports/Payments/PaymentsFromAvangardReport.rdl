  <DataSets>
    <DataSet Name="Orders">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>120</Timeout>
        <CommandText>SELECT
    o.delivery_date AS order_date,
    o.id AS order_id,
    c.full_name AS client,
    IF(o.self_delivery, 'Самовывоз', dp.compiled_address_short) AS address,
    o.online_order AS payment_nr_from_order,
    p.total_sum AS sum_from_bank,
    o_sum.total AS sum_from_order,
    (
        CASE o.order_status
            WHEN 'NewOrder' THEN 'Новый'
            WHEN 'Canceled' THEN 'Отменён'
            WHEN 'WaitForPayment' THEN 'Ожидание оплаты'
            WHEN 'Accepted' THEN 'Принят'
            WHEN 'InTravelList' THEN 'В маршрутном листе'
            WHEN 'OnLoading' THEN 'На погрузке'
            WHEN 'OnTheWay' THEN 'В пути'
            WHEN 'DeliveryCanceled' THEN 'Доставка отменена'
            WHEN 'Shipped' THEN 'Доставлен'
            WHEN 'UnloadingOnStock' THEN 'Выгрузка на складе'
            WHEN 'NotDelivered' THEN 'Недовоз'
            WHEN 'Closed' THEN 'Закрыт'
            ELSE o.order_status
            END
        ) AS order_satus,
    TRIM(BOTH '.' FROM CONCAT_WS('.', LEFT(author.name, 1), LEFT(author.patronymic, 1), author.last_name)) AS author,
    IFNULL(
            DATE_FORMAT(
                    p.paid_date,
                    '%d.%m.%Y %H:%i:%s'
                ),
            'оплата не найдена'
        ) AS payment_date_time,
    o.payment_type AS payment_type,
    #номер группы для порядка в группировке в RDL
    (
        CASE o.order_status
            WHEN 'Canceled' THEN 0
            WHEN 'NewOrder' THEN 1
            WHEN 'WaitForPayment' THEN 2
            WHEN 'Accepted' THEN 3
            WHEN 'InTravelList' THEN 4
            WHEN 'OnLoading' THEN 5
            WHEN 'OnTheWay' THEN 6
            WHEN 'DeliveryCanceled' THEN 7
            WHEN 'Shipped' THEN 8
            WHEN 'UnloadingOnStock' THEN 9
            WHEN 'NotDelivered' THEN 10
            WHEN 'Closed' THEN 11
            ELSE 999
            END
        ) AS order_satus_numbered,
    #имя группы для группировки в RDL
    (
        CASE
            WHEN p.total_sum = o_sum.total THEN 'Оплачено'
            WHEN p.total_sum IS NULL OR p.total_sum = 0 AND p.total_sum &lt; o_sum.total THEN 'Отсутствует оплата'
            WHEN p.total_sum &gt; 0 AND p.total_sum &gt; o_sum.total THEN 'Переплата'
            WHEN p.total_sum &gt; 0 AND p.total_sum &lt; o_sum.total THEN 'Недоплата'
            ELSE 'Оплачено'
            END
        ) AS underpayment,
    #номер группы для порядка в группировке в RDL
    (
        CASE
            WHEN p.total_sum = o_sum.total THEN 0
            WHEN p.total_sum IS NULL OR p.total_sum = 0 AND p.total_sum &lt; o_sum.total THEN 1
            WHEN p.total_sum &gt; 0 AND p.total_sum &gt; o_sum.total THEN 2
            WHEN p.total_sum &gt; 0 AND p.total_sum &lt; o_sum.total THEN 3
            ELSE 99
            END
        ) AS underpayment_number,
    o.delivery_date &gt; @endDate AS is_future_order,
    p.order_num AS number,
    (
        CASE p.pay_type
            WHEN 'Unknown' THEN 'Неизвестно'
            WHEN 'ByCard' THEN 'По карте'
            WHEN 'ByQrCode' THEN 'По Qr коду'
            END
    ) AS pay_type
    #для отладки на тестовых БД. Для того, чтоб увидеть заказы на будущие даты
    #o.delivery_date &gt; '2018-10-03' AS is_future_order
FROM
    orders o
        LEFT JOIN
    (
        Select
            o_online.id as order_id,
            p_avangard.*,
            fp.pay_type as pay_type
        From orders o_online
                 JOIN payments_from_avangard p_avangard on p_avangard.order_num = o_online.online_order
                 JOIN fast_payments fp on fp.external_id = p_avangard.order_num  and fp.payment_status = 'Performed'
            AND p_avangard.paid_date &gt; DATE_ADD(o_online.create_date, INTERVAL -10 DAY)
            AND p_avangard.paid_date &lt; DATE_ADD(o_online.create_date, INTERVAL 10 DAY)
    ) as p on p.order_id = o.id
        LEFT JOIN
    counterparty c ON c.id = o.client_id
        LEFT JOIN
    delivery_points dp ON dp.id = o.delivery_point_id
        LEFT JOIN
    employees author ON author.id = o.author_employee_id
        LEFT JOIN
    (
        SELECT
            o_sub.id AS order_id,
            SUM(ROUND(price * IFNULL(actual_count, count) - discount_money, 2)) AS total
        FROM
            orders o_sub
        JOIN
            order_items i ON o_sub.id = i.order_id
        WHERE
            o_sub.payment_type = 'ByCard'
          AND o_sub.online_order IS NOT NULL
          AND o_sub.payment_from_id IN (10, 11, 12)
          AND o_sub.delivery_date &gt;= @startDate
          AND o_sub.delivery_date &lt;= @endDate
        GROUP BY
            i.order_id
    ) o_sum ON o.id = o_sum.order_id
WHERE
    o.payment_type = 'ByCard'
  AND o.online_order IS NOT NULL
  AND o.payment_from_id IN (10, 11, 12)
  AND o.delivery_date &gt;= @startDate
  AND o.delivery_date &lt;= @endDate
ORDER BY order_satus_numbered DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!startDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!endDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
