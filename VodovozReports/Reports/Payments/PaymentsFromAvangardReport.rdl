        </Field>
        <Field Name="payment_type">
          <DataField>payment_type</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="order_satus_numbered">
          <DataField>order_satus_numbered</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="underpayment">
          <DataField>underpayment</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="underpayment_number">
          <DataField>underpayment_number</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="is_future_order">
          <DataField>is_future_order</DataField>
          <TypeName>System.Boolean</TypeName>
        </Field>
        <Field Name="number">
          <DataField>number</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="pay_type">
          <DataField>pay_type</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
